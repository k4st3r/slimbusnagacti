# ============= usr/lib/nagios/plugins/jquery-1.7.2.js ==============
if test -n "${keep_file}" && test -f 'usr/lib/nagios/plugins/jquery-1.7.2.js'
then
${echo} "x - SKIPPING usr/lib/nagios/plugins/jquery-1.7.2.js (file already exists)"
else
${echo} "x - extracting usr/lib/nagios/plugins/jquery-1.7.2.js (text)"
  sed 's/^X//' << 'SHAR_EOF' > 'usr/lib/nagios/plugins/jquery-1.7.2.js' &&
/*!
X * jQuery JavaScript Library v1.7.2
X * http://jquery.com/
X *
X * Copyright 2011, John Resig
X * Dual licensed under the MIT or GPL Version 2 licenses.
X * http://jquery.org/license
X *
X * Includes Sizzle.js
X * http://sizzlejs.com/
X * Copyright 2011, The Dojo Foundation
X * Released under the MIT, BSD, and GPL Licenses.
X *
X * Date: Wed Mar 21 12:46:34 2012 -0700
X */
(function( window, undefined ) {
X
// Use the correct document accordingly with window argument (sandbox)
var document = window.document,
X	navigator = window.navigator,
X	location = window.location;
var jQuery = (function() {
X
// Define a local copy of jQuery
var jQuery = function( selector, context ) {
X		// The jQuery object is actually just the init constructor 'enhanced'
X		return new jQuery.fn.init( selector, context, rootjQuery );
X	},
X
X	// Map over jQuery in case of overwrite
X	_jQuery = window.jQuery,
X
X	// Map over the $ in case of overwrite
X	_$ = window.$,
X
X	// A central reference to the root jQuery(document)
X	rootjQuery,
X
X	// A simple way to check for HTML strings or ID strings
X	// Prioritize #id over <tag> to avoid XSS via location.hash (#9521)
X	quickExpr = /^(?:[^#<]*(<[\w\W]+>)[^>]*$|#([\w\-]*)$)/,
X
X	// Check if a string has a non-whitespace character in it
X	rnotwhite = /\S/,
X
X	// Used for trimming whitespace
X	trimLeft = /^\s+/,
X	trimRight = /\s+$/,
X
X	// Match a standalone tag
X	rsingleTag = /^<(\w+)\s*\/?>(?:<\/\1>)?$/,
X
X	// JSON RegExp
X	rvalidchars = /^[\],:{}\s]*$/,
X	rvalidescape = /\\(?:["\\\/bfnrt]|u[0-9a-fA-F]{4})/g,
X	rvalidtokens = /"[^"\\\n\r]*"|true|false|null|-?\d+(?:\.\d*)?(?:[eE][+\-]?\d+)?/g,
X	rvalidbraces = /(?:^|:|,)(?:\s*\[)+/g,
X
X	// Useragent RegExp
X	rwebkit = /(webkit)[ \/]([\w.]+)/,
X	ropera = /(opera)(?:.*version)?[ \/]([\w.]+)/,
X	rmsie = /(msie) ([\w.]+)/,
X	rmozilla = /(mozilla)(?:.*? rv:([\w.]+))?/,
X
X	// Matches dashed string for camelizing
X	rdashAlpha = /-([a-z]|[0-9])/ig,
X	rmsPrefix = /^-ms-/,
X
X	// Used by jQuery.camelCase as callback to replace()
X	fcamelCase = function( all, letter ) {
X		return ( letter + "" ).toUpperCase();
X	},
X
X	// Keep a UserAgent string for use with jQuery.browser
X	userAgent = navigator.userAgent,
X
X	// For matching the engine and version of the browser
X	browserMatch,
X
X	// The deferred used on DOM ready
X	readyList,
X
X	// The ready event handler
X	DOMContentLoaded,
X
X	// Save a reference to some core methods
X	toString = Object.prototype.toString,
X	hasOwn = Object.prototype.hasOwnProperty,
X	push = Array.prototype.push,
X	slice = Array.prototype.slice,
X	trim = String.prototype.trim,
X	indexOf = Array.prototype.indexOf,
X
X	// [[Class]] -> type pairs
X	class2type = {};
X
jQuery.fn = jQuery.prototype = {
X	constructor: jQuery,
X	init: function( selector, context, rootjQuery ) {
X		var match, elem, ret, doc;
X
X		// Handle $(""), $(null), or $(undefined)
X		if ( !selector ) {
X			return this;
X		}
X
X		// Handle $(DOMElement)
X		if ( selector.nodeType ) {
X			this.context = this[0] = selector;
X			this.length = 1;
X			return this;
X		}
X
X		// The body element only exists once, optimize finding it
X		if ( selector === "body" && !context && document.body ) {
X			this.context = document;
X			this[0] = document.body;
X			this.selector = selector;
X			this.length = 1;
X			return this;
X		}
X
X		// Handle HTML strings
X		if ( typeof selector === "string" ) {
X			// Are we dealing with HTML string or an ID?
X			if ( selector.charAt(0) === "<" && selector.charAt( selector.length - 1 ) === ">" && selector.length >= 3 ) {
X				// Assume that strings that start and end with <> are HTML and skip the regex check
X				match = [ null, selector, null ];
X
X			} else {
X				match = quickExpr.exec( selector );
X			}
X
X			// Verify a match, and that no context was specified for #id
X			if ( match && (match[1] || !context) ) {
X
X				// HANDLE: $(html) -> $(array)
X				if ( match[1] ) {
X					context = context instanceof jQuery ? context[0] : context;
X					doc = ( context ? context.ownerDocument || context : document );
X
X					// If a single string is passed in and it's a single tag
X					// just do a createElement and skip the rest
X					ret = rsingleTag.exec( selector );
X
X					if ( ret ) {
X						if ( jQuery.isPlainObject( context ) ) {
X							selector = [ document.createElement( ret[1] ) ];
X							jQuery.fn.attr.call( selector, context, true );
X
X						} else {
X							selector = [ doc.createElement( ret[1] ) ];
X						}
X
X					} else {
X						ret = jQuery.buildFragment( [ match[1] ], [ doc ] );
X						selector = ( ret.cacheable ? jQuery.clone(ret.fragment) : ret.fragment ).childNodes;
X					}
X
X					return jQuery.merge( this, selector );
X
X				// HANDLE: $("#id")
X				} else {
X					elem = document.getElementById( match[2] );
X
X					// Check parentNode to catch when Blackberry 4.6 returns
X					// nodes that are no longer in the document #6963
X					if ( elem && elem.parentNode ) {
X						// Handle the case where IE and Opera return items
X						// by name instead of ID
X						if ( elem.id !== match[2] ) {
X							return rootjQuery.find( selector );
X						}
X
X						// Otherwise, we inject the element directly into the jQuery object
X						this.length = 1;
X						this[0] = elem;
X					}
X
X					this.context = document;
X					this.selector = selector;
X					return this;
X				}
X
X			// HANDLE: $(expr, $(...))
X			} else if ( !context || context.jquery ) {
X				return ( context || rootjQuery ).find( selector );
X
X			// HANDLE: $(expr, context)
X			// (which is just equivalent to: $(context).find(expr)
X			} else {
X				return this.constructor( context ).find( selector );
X			}
X
X		// HANDLE: $(function)
X		// Shortcut for document ready
X		} else if ( jQuery.isFunction( selector ) ) {
X			return rootjQuery.ready( selector );
X		}
X
X		if ( selector.selector !== undefined ) {
X			this.selector = selector.selector;
X			this.context = selector.context;
X		}
X
X		return jQuery.makeArray( selector, this );
X	},
X
X	// Start with an empty selector
X	selector: "",
X
X	// The current version of jQuery being used
X	jquery: "1.7.2",
X
X	// The default length of a jQuery object is 0
X	length: 0,
X
X	// The number of elements contained in the matched element set
X	size: function() {
X		return this.length;
X	},
X
X	toArray: function() {
X		return slice.call( this, 0 );
X	},
X
X	// Get the Nth element in the matched element set OR
X	// Get the whole matched element set as a clean array
X	get: function( num ) {
X		return num == null ?
X
X			// Return a 'clean' array
X			this.toArray() :
X
X			// Return just the object
X			( num < 0 ? this[ this.length + num ] : this[ num ] );
X	},
X
X	// Take an array of elements and push it onto the stack
X	// (returning the new matched element set)
X	pushStack: function( elems, name, selector ) {
X		// Build a new jQuery matched element set
X		var ret = this.constructor();
X
X		if ( jQuery.isArray( elems ) ) {
X			push.apply( ret, elems );
X
X		} else {
X			jQuery.merge( ret, elems );
X		}
X
X		// Add the old object onto the stack (as a reference)
X		ret.prevObject = this;
X
X		ret.context = this.context;
X
X		if ( name === "find" ) {
X			ret.selector = this.selector + ( this.selector ? " " : "" ) + selector;
X		} else if ( name ) {
X			ret.selector = this.selector + "." + name + "(" + selector + ")";
X		}
X
X		// Return the newly-formed element set
X		return ret;
X	},
X
X	// Execute a callback for every element in the matched set.
X	// (You can seed the arguments with an array of args, but this is
X	// only used internally.)
X	each: function( callback, args ) {
X		return jQuery.each( this, callback, args );
X	},
X
X	ready: function( fn ) {
X		// Attach the listeners
X		jQuery.bindReady();
X
X		// Add the callback
X		readyList.add( fn );
X
X		return this;
X	},
X
X	eq: function( i ) {
X		i = +i;
X		return i === -1 ?
X			this.slice( i ) :
X			this.slice( i, i + 1 );
X	},
X
X	first: function() {
X		return this.eq( 0 );
X	},
X
X	last: function() {
X		return this.eq( -1 );
X	},
X
X	slice: function() {
X		return this.pushStack( slice.apply( this, arguments ),
X			"slice", slice.call(arguments).join(",") );
X	},
X
X	map: function( callback ) {
X		return this.pushStack( jQuery.map(this, function( elem, i ) {
X			return callback.call( elem, i, elem );
X		}));
X	},
X
X	end: function() {
X		return this.prevObject || this.constructor(null);
X	},
X
X	// For internal use only.
X	// Behaves like an Array's method, not like a jQuery method.
X	push: push,
X	sort: [].sort,
X	splice: [].splice
};
X
// Give the init function the jQuery prototype for later instantiation
jQuery.fn.init.prototype = jQuery.fn;
X
jQuery.extend = jQuery.fn.extend = function() {
X	var options, name, src, copy, copyIsArray, clone,
X		target = arguments[0] || {},
X		i = 1,
X		length = arguments.length,
X		deep = false;
X
X	// Handle a deep copy situation
X	if ( typeof target === "boolean" ) {
X		deep = target;
X		target = arguments[1] || {};
X		// skip the boolean and the target
X		i = 2;
X	}
X
X	// Handle case when target is a string or something (possible in deep copy)
X	if ( typeof target !== "object" && !jQuery.isFunction(target) ) {
X		target = {};
X	}
X
X	// extend jQuery itself if only one argument is passed
X	if ( length === i ) {
X		target = this;
X		--i;
X	}
X
X	for ( ; i < length; i++ ) {
X		// Only deal with non-null/undefined values
X		if ( (options = arguments[ i ]) != null ) {
X			// Extend the base object
X			for ( name in options ) {
X				src = target[ name ];
X				copy = options[ name ];
X
X				// Prevent never-ending loop
X				if ( target === copy ) {
X					continue;
X				}
X
X				// Recurse if we're merging plain objects or arrays
X				if ( deep && copy && ( jQuery.isPlainObject(copy) || (copyIsArray = jQuery.isArray(copy)) ) ) {
X					if ( copyIsArray ) {
X						copyIsArray = false;
X						clone = src && jQuery.isArray(src) ? src : [];
X
X					} else {
X						clone = src && jQuery.isPlainObject(src) ? src : {};
X					}
X
X					// Never move original objects, clone them
X					target[ name ] = jQuery.extend( deep, clone, copy );
X
X				// Don't bring in undefined values
X				} else if ( copy !== undefined ) {
X					target[ name ] = copy;
X				}
X			}
X		}
X	}
X
X	// Return the modified object
X	return target;
};
X
jQuery.extend({
X	noConflict: function( deep ) {
X		if ( window.$ === jQuery ) {
X			window.$ = _$;
X		}
X
X		if ( deep && window.jQuery === jQuery ) {
X			window.jQuery = _jQuery;
X		}
X
X		return jQuery;
X	},
X
X	// Is the DOM ready to be used? Set to true once it occurs.
X	isReady: false,
X
X	// A counter to track how many items to wait for before
X	// the ready event fires. See #6781
X	readyWait: 1,
X
X	// Hold (or release) the ready event
X	holdReady: function( hold ) {
X		if ( hold ) {
X			jQuery.readyWait++;
X		} else {
X			jQuery.ready( true );
X		}
X	},
X
X	// Handle when the DOM is ready
X	ready: function( wait ) {
X		// Either a released hold or an DOMready/load event and not yet ready
X		if ( (wait === true && !--jQuery.readyWait) || (wait !== true && !jQuery.isReady) ) {
X			// Make sure body exists, at least, in case IE gets a little overzealous (ticket #5443).
X			if ( !document.body ) {
X				return setTimeout( jQuery.ready, 1 );
X			}
X
X			// Remember that the DOM is ready
X			jQuery.isReady = true;
X
X			// If a normal DOM Ready event fired, decrement, and wait if need be
X			if ( wait !== true && --jQuery.readyWait > 0 ) {
X				return;
X			}
X
X			// If there are functions bound, to execute
X			readyList.fireWith( document, [ jQuery ] );
X
X			// Trigger any bound ready events
X			if ( jQuery.fn.trigger ) {
X				jQuery( document ).trigger( "ready" ).off( "ready" );
X			}
X		}
X	},
X
X	bindReady: function() {
X		if ( readyList ) {
X			return;
X		}
X
X		readyList = jQuery.Callbacks( "once memory" );
X
X		// Catch cases where $(document).ready() is called after the
X		// browser event has already occurred.
X		if ( document.readyState === "complete" ) {
X			// Handle it asynchronously to allow scripts the opportunity to delay ready
X			return setTimeout( jQuery.ready, 1 );
X		}
X
X		// Mozilla, Opera and webkit nightlies currently support this event
X		if ( document.addEventListener ) {
X			// Use the handy event callback
X			document.addEventListener( "DOMContentLoaded", DOMContentLoaded, false );
X
X			// A fallback to window.onload, that will always work
X			window.addEventListener( "load", jQuery.ready, false );
X
X		// If IE event model is used
X		} else if ( document.attachEvent ) {
X			// ensure firing before onload,
X			// maybe late but safe also for iframes
X			document.attachEvent( "onreadystatechange", DOMContentLoaded );
X
X			// A fallback to window.onload, that will always work
X			window.attachEvent( "onload", jQuery.ready );
X
X			// If IE and not a frame
X			// continually check to see if the document is ready
X			var toplevel = false;
X
X			try {
X				toplevel = window.frameElement == null;
X			} catch(e) {}
X
X			if ( document.documentElement.doScroll && toplevel ) {
X				doScrollCheck();
X			}
X		}
X	},
X
X	// See test/unit/core.js for details concerning isFunction.
X	// Since version 1.3, DOM methods and functions like alert
X	// aren't supported. They return false on IE (#2968).
X	isFunction: function( obj ) {
X		return jQuery.type(obj) === "function";
X	},
X
X	isArray: Array.isArray || function( obj ) {
X		return jQuery.type(obj) === "array";
X	},
X
X	isWindow: function( obj ) {
X		return obj != null && obj == obj.window;
X	},
X
X	isNumeric: function( obj ) {
X		return !isNaN( parseFloat(obj) ) && isFinite( obj );
X	},
X
X	type: function( obj ) {
X		return obj == null ?
X			String( obj ) :
X			class2type[ toString.call(obj) ] || "object";
X	},
X
X	isPlainObject: function( obj ) {
X		// Must be an Object.
X		// Because of IE, we also have to check the presence of the constructor property.
X		// Make sure that DOM nodes and window objects don't pass through, as well
X		if ( !obj || jQuery.type(obj) !== "object" || obj.nodeType || jQuery.isWindow( obj ) ) {
X			return false;
X		}
X
X		try {
X			// Not own constructor property must be Object
X			if ( obj.constructor &&
X				!hasOwn.call(obj, "constructor") &&
X				!hasOwn.call(obj.constructor.prototype, "isPrototypeOf") ) {
X				return false;
X			}
X		} catch ( e ) {
X			// IE8,9 Will throw exceptions on certain host objects #9897
X			return false;
X		}
X
X		// Own properties are enumerated firstly, so to speed up,
X		// if last one is own, then all properties are own.
X
X		var key;
X		for ( key in obj ) {}
X
X		return key === undefined || hasOwn.call( obj, key );
X	},
X
X	isEmptyObject: function( obj ) {
X		for ( var name in obj ) {
X			return false;
X		}
X		return true;
X	},
X
X	error: function( msg ) {
X		throw new Error( msg );
X	},
X
X	parseJSON: function( data ) {
X		if ( typeof data !== "string" || !data ) {
X			return null;
X		}
X
X		// Make sure leading/trailing whitespace is removed (IE can't handle it)
X		data = jQuery.trim( data );
X
X		// Attempt to parse using the native JSON parser first
X		if ( window.JSON && window.JSON.parse ) {
X			return window.JSON.parse( data );
X		}
X
X		// Make sure the incoming data is actual JSON
X		// Logic borrowed from http://json.org/json2.js
X		if ( rvalidchars.test( data.replace( rvalidescape, "@" )
X			.replace( rvalidtokens, "]" )
X			.replace( rvalidbraces, "")) ) {
X
X			return ( new Function( "return " + data ) )();
X
X		}
X		jQuery.error( "Invalid JSON: " + data );
X	},
X
X	// Cross-browser xml parsing
X	parseXML: function( data ) {
X		if ( typeof data !== "string" || !data ) {
X			return null;
X		}
X		var xml, tmp;
X		try {
X			if ( window.DOMParser ) { // Standard
X				tmp = new DOMParser();
X				xml = tmp.parseFromString( data , "text/xml" );
X			} else { // IE
X				xml = new ActiveXObject( "Microsoft.XMLDOM" );
X				xml.async = "false";
X				xml.loadXML( data );
X			}
X		} catch( e ) {
X			xml = undefined;
X		}
X		if ( !xml || !xml.documentElement || xml.getElementsByTagName( "parsererror" ).length ) {
X			jQuery.error( "Invalid XML: " + data );
X		}
X		return xml;
X	},
X
X	noop: function() {},
X
X	// Evaluates a script in a global context
X	// Workarounds based on findings by Jim Driscoll
X	// http://weblogs.java.net/blog/driscoll/archive/2009/09/08/eval-javascript-global-context
X	globalEval: function( data ) {
X		if ( data && rnotwhite.test( data ) ) {
X			// We use execScript on Internet Explorer
X			// We use an anonymous function so that context is window
X			// rather than jQuery in Firefox
X			( window.execScript || function( data ) {
X				window[ "eval" ].call( window, data );
X			} )( data );
X		}
X	},
X
X	// Convert dashed to camelCase; used by the css and data modules
X	// Microsoft forgot to hump their vendor prefix (#9572)
X	camelCase: function( string ) {
X		return string.replace( rmsPrefix, "ms-" ).replace( rdashAlpha, fcamelCase );
X	},
X
X	nodeName: function( elem, name ) {
X		return elem.nodeName && elem.nodeName.toUpperCase() === name.toUpperCase();
X	},
X
X	// args is for internal usage only
X	each: function( object, callback, args ) {
X		var name, i = 0,
X			length = object.length,
X			isObj = length === undefined || jQuery.isFunction( object );
X
X		if ( args ) {
X			if ( isObj ) {
X				for ( name in object ) {
X					if ( callback.apply( object[ name ], args ) === false ) {
X						break;
X					}
X				}
X			} else {
X				for ( ; i < length; ) {
X					if ( callback.apply( object[ i++ ], args ) === false ) {
X						break;
X					}
X				}
X			}
X
X		// A special, fast, case for the most common use of each
X		} else {
X			if ( isObj ) {
X				for ( name in object ) {
X					if ( callback.call( object[ name ], name, object[ name ] ) === false ) {
X						break;
X					}
X				}
X			} else {
X				for ( ; i < length; ) {
X					if ( callback.call( object[ i ], i, object[ i++ ] ) === false ) {
X						break;
X					}
X				}
X			}
X		}
X
X		return object;
X	},
X
X	// Use native String.trim function wherever possible
X	trim: trim ?
X		function( text ) {
X			return text == null ?
X				"" :
X				trim.call( text );
X		} :
X
X		// Otherwise use our own trimming functionality
X		function( text ) {
X			return text == null ?
X				"" :
X				text.toString().replace( trimLeft, "" ).replace( trimRight, "" );
X		},
X
X	// results is for internal usage only
X	makeArray: function( array, results ) {
X		var ret = results || [];
X
X		if ( array != null ) {
X			// The window, strings (and functions) also have 'length'
X			// Tweaked logic slightly to handle Blackberry 4.7 RegExp issues #6930
X			var type = jQuery.type( array );
X
X			if ( array.length == null || type === "string" || type === "function" || type === "regexp" || jQuery.isWindow( array ) ) {
X				push.call( ret, array );
X			} else {
X				jQuery.merge( ret, array );
X			}
X		}
X
X		return ret;
X	},
X
X	inArray: function( elem, array, i ) {
X		var len;
X
X		if ( array ) {
X			if ( indexOf ) {
X				return indexOf.call( array, elem, i );
X			}
X
X			len = array.length;
X			i = i ? i < 0 ? Math.max( 0, len + i ) : i : 0;
X
X			for ( ; i < len; i++ ) {
X				// Skip accessing in sparse arrays
X				if ( i in array && array[ i ] === elem ) {
X					return i;
X				}
X			}
X		}
X
X		return -1;
X	},
X
X	merge: function( first, second ) {
X		var i = first.length,
X			j = 0;
X
X		if ( typeof second.length === "number" ) {
X			for ( var l = second.length; j < l; j++ ) {
X				first[ i++ ] = second[ j ];
X			}
X
X		} else {
X			while ( second[j] !== undefined ) {
X				first[ i++ ] = second[ j++ ];
X			}
X		}
X
X		first.length = i;
X
X		return first;
X	},
X
X	grep: function( elems, callback, inv ) {
X		var ret = [], retVal;
X		inv = !!inv;
X
X		// Go through the array, only saving the items
X		// that pass the validator function
X		for ( var i = 0, length = elems.length; i < length; i++ ) {
X			retVal = !!callback( elems[ i ], i );
X			if ( inv !== retVal ) {
X				ret.push( elems[ i ] );
X			}
X		}
X
X		return ret;
X	},
X
X	// arg is for internal usage only
X	map: function( elems, callback, arg ) {
X		var value, key, ret = [],
X			i = 0,
X			length = elems.length,
X			// jquery objects are treated as arrays
X			isArray = elems instanceof jQuery || length !== undefined && typeof length === "number" && ( ( length > 0 && elems[ 0 ] && elems[ length -1 ] ) || length === 0 || jQuery.isArray( elems ) ) ;
X
X		// Go through the array, translating each of the items to their
X		if ( isArray ) {
X			for ( ; i < length; i++ ) {
X				value = callback( elems[ i ], i, arg );
X
X				if ( value != null ) {
X					ret[ ret.length ] = value;
X				}
X			}
X
X		// Go through every key on the object,
X		} else {
X			for ( key in elems ) {
X				value = callback( elems[ key ], key, arg );
X
X				if ( value != null ) {
X					ret[ ret.length ] = value;
X				}
X			}
X		}
X
X		// Flatten any nested arrays
X		return ret.concat.apply( [], ret );
X	},
X
X	// A global GUID counter for objects
X	guid: 1,
X
X	// Bind a function to a context, optionally partially applying any
X	// arguments.
X	proxy: function( fn, context ) {
X		if ( typeof context === "string" ) {
X			var tmp = fn[ context ];
X			context = fn;
X			fn = tmp;
X		}
X
X		// Quick check to determine if target is callable, in the spec
X		// this throws a TypeError, but we will just return undefined.
X		if ( !jQuery.isFunction( fn ) ) {
X			return undefined;
X		}
X
X		// Simulated bind
X		var args = slice.call( arguments, 2 ),
X			proxy = function() {
X				return fn.apply( context, args.concat( slice.call( arguments ) ) );
X			};
X
X		// Set the guid of unique handler to the same of original handler, so it can be removed
X		proxy.guid = fn.guid = fn.guid || proxy.guid || jQuery.guid++;
X
X		return proxy;
X	},
X
X	// Mutifunctional method to get and set values to a collection
X	// The value/s can optionally be executed if it's a function
X	access: function( elems, fn, key, value, chainable, emptyGet, pass ) {
X		var exec,
X			bulk = key == null,
X			i = 0,
X			length = elems.length;
X
X		// Sets many values
X		if ( key && typeof key === "object" ) {
X			for ( i in key ) {
X				jQuery.access( elems, fn, i, key[i], 1, emptyGet, value );
X			}
X			chainable = 1;
X
X		// Sets one value
X		} else if ( value !== undefined ) {
X			// Optionally, function values get executed if exec is true
X			exec = pass === undefined && jQuery.isFunction( value );
X
X			if ( bulk ) {
X				// Bulk operations only iterate when executing function values
X				if ( exec ) {
X					exec = fn;
X					fn = function( elem, key, value ) {
X						return exec.call( jQuery( elem ), value );
X					};
X
X				// Otherwise they run against the entire set
X				} else {
X					fn.call( elems, value );
X					fn = null;
X				}
X			}
X
X			if ( fn ) {
X				for (; i < length; i++ ) {
X					fn( elems[i], key, exec ? value.call( elems[i], i, fn( elems[i], key ) ) : value, pass );
X				}
X			}
X
X			chainable = 1;
X		}
X
X		return chainable ?
X			elems :
X
X			// Gets
X			bulk ?
X				fn.call( elems ) :
X				length ? fn( elems[0], key ) : emptyGet;
X	},
X
X	now: function() {
X		return ( new Date() ).getTime();
X	},
X
X	// Use of jQuery.browser is frowned upon.
X	// More details: http://docs.jquery.com/Utilities/jQuery.browser
X	uaMatch: function( ua ) {
X		ua = ua.toLowerCase();
X
X		var match = rwebkit.exec( ua ) ||
X			ropera.exec( ua ) ||
X			rmsie.exec( ua ) ||
X			ua.indexOf("compatible") < 0 && rmozilla.exec( ua ) ||
X			[];
X
X		return { browser: match[1] || "", version: match[2] || "0" };
X	},
X
X	sub: function() {
X		function jQuerySub( selector, context ) {
X			return new jQuerySub.fn.init( selector, context );
X		}
X		jQuery.extend( true, jQuerySub, this );
X		jQuerySub.superclass = this;
X		jQuerySub.fn = jQuerySub.prototype = this();
X		jQuerySub.fn.constructor = jQuerySub;
X		jQuerySub.sub = this.sub;
X		jQuerySub.fn.init = function init( selector, context ) {
X			if ( context && context instanceof jQuery && !(context instanceof jQuerySub) ) {
X				context = jQuerySub( context );
X			}
X
X			return jQuery.fn.init.call( this, selector, context, rootjQuerySub );
X		};
X		jQuerySub.fn.init.prototype = jQuerySub.fn;
X		var rootjQuerySub = jQuerySub(document);
X		return jQuerySub;
X	},
X
X	browser: {}
});
X
// Populate the class2type map
jQuery.each("Boolean Number String Function Array Date RegExp Object".split(" "), function(i, name) {
X	class2type[ "[object " + name + "]" ] = name.toLowerCase();
});
X
browserMatch = jQuery.uaMatch( userAgent );
if ( browserMatch.browser ) {
X	jQuery.browser[ browserMatch.browser ] = true;
X	jQuery.browser.version = browserMatch.version;
}
X
// Deprecated, use jQuery.browser.webkit instead
if ( jQuery.browser.webkit ) {
X	jQuery.browser.safari = true;
}
X
// IE doesn't match non-breaking spaces with \s
if ( rnotwhite.test( "\xA0" ) ) {
X	trimLeft = /^[\s\xA0]+/;
X	trimRight = /[\s\xA0]+$/;
}
X
// All jQuery objects should point back to these
rootjQuery = jQuery(document);
X
// Cleanup functions for the document ready method
if ( document.addEventListener ) {
X	DOMContentLoaded = function() {
X		document.removeEventListener( "DOMContentLoaded", DOMContentLoaded, false );
X		jQuery.ready();
X	};
X
} else if ( document.attachEvent ) {
X	DOMContentLoaded = function() {
X		// Make sure body exists, at least, in case IE gets a little overzealous (ticket #5443).
X		if ( document.readyState === "complete" ) {
X			document.detachEvent( "onreadystatechange", DOMContentLoaded );
X			jQuery.ready();
X		}
X	};
}
X
// The DOM ready check for Internet Explorer
function doScrollCheck() {
X	if ( jQuery.isReady ) {
X		return;
X	}
X
X	try {
X		// If IE is used, use the trick by Diego Perini
X		// http://javascript.nwbox.com/IEContentLoaded/
X		document.documentElement.doScroll("left");
X	} catch(e) {
X		setTimeout( doScrollCheck, 1 );
X		return;
X	}
X
X	// and execute any waiting functions
X	jQuery.ready();
}
X
return jQuery;
X
})();
