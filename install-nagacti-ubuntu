# ============= usr/lib/nagios/plugins/jmx4node_routestats.pl ==============
if test -n "${keep_file}" && test -f 'usr/lib/nagios/plugins/jmx4node_routestats.pl'
then
${echo} "x - SKIPPING usr/lib/nagios/plugins/jmx4node_routestats.pl (file already exists)"
else
${echo} "x - extracting usr/lib/nagios/plugins/jmx4node_routestats.pl (text)"
  sed 's/^X//' << 'SHAR_EOF' > 'usr/lib/nagios/plugins/jmx4node_routestats.pl' &&
#!/usr/bin/perl 
X
use Cwd 'abs_path';
use Getopt::Long;
X
$_ = abs_path($0);
s/\/[^\/]+$//;
X
$THISDIR=$_;
X
sub getHostnameFromIp {
X    my $hostname = "UNKNOWN_HOSTNAME";
X    #print "starting with $hostname \n";
X    open(DNSRESP, "nslookup $_[0] | grep in-addr.arpa | awk '{print \$4;}' |");
X    chomp($hostname=<DNSRESP>);
X    close(DNSRESP);
X    #print "$hostname\n";
X    return $hostname;
}
X
my $HOSTIP = "";
my $HOSTPORT = 8080;
my $MBEAN = 0;
my $CONTEXT = '';
my $ROUTENAME = '';
my $VERBOSE = '';
my @PATHITEMS = ();
my @ATTRIBUTES = ();
X
`echo jmx4node_routestats.pl called >> /tmp/cacti_jmx4node_routestats.log`;
#`called with args: "@ARGV" `
$result = GetOptions (
X	"hostip=s" 	=> \$HOSTIP,
X	"hostport=i" 	=> \$HOSTPORT,
X	"mbean=s"	=> \$MBEAN,
X	"context=s"	=> \$CONTEXT,
X	"routename=s"	=> \$ROUTENAME,
X	"attributes=s"	=> \$ATTRIBSTR,
X	"path=s"	=> \$PATHSTR
X	);
X
X
if($ATTRIBSTR ) { @ATTRIBUTES = split(/,/, "$ATTRIBSTR"); } else { @ATTRIBUTES=(".*"); }
if($PATHSTR)   { @PATHITEMS = split(/,/, "$PATHSTR"); 	}
X
if( ! "$HOSTIP" ) { die "must supply host ip or host name!"; }
if( ! "$HOSTPORT" ) { $HOSTPORT=8080; }
X
if ( ! "$MBEAN" ) { 
X    if( ! "$ROUTENAME" ) { die "must supply mbean name or camel route name!"; }
X    if( ! "$CONTEXT" ) {
X	#print "context was false, hostip was $HOSTIP \n";
X	# if the host ip was supplied as an ip address atempt to get hostname
X	my $FQDN;
X	if( "$HOSTIP" =~ /[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}/ ) {
X	    #print "looking up host:";
X	    $FQDN = getHostnameFromIp("$HOSTIP");
X	} 
X	$_ = $FQDN;
X	s/\..*//g;
X	my $HOSTNAME = $_;
X	#print $HOSTNAME . "\n";
X	$MBEAN = "org.apache.camel:'context=".$HOSTNAME."/camel-1,name=\"".$ROUTENAME."\",type=routes'";
X	}
} 
else {
X    $MBEAN = "org.apache.camel:'context=".$CONTEXT.",name=\\".$ROUTENAME."\",type=routes'";
}
X		
X
X 	
X
#$HOSTIP = "10.0.44.40";
#$HOSTPORT = "8080";
#$ROUTENAME= "org.apache.camel:'context=server-3604/camel-1,name=\"encore.aab.exceptionHandler.v1_3\",type=routes'";
#@ATTRIBUTES=( "MeanProcessingTime", "Load05", "ExchangesFailed", "FailuresHandled", 
#	"TotalProcessingTime", "Redeliveries", "Load01", "MaxProcessingTime", 
#	"LastProcessingTime", "ExchangesTotal", "ExternalRediliveries", "ExhangesCompleted",
#	"InflightExchanges", "Load15" );
X
$CMDLINE="$THISDIR/jmx4node http://$HOSTIP:$HOSTPORT/jolokia read $MBEAN";
X
`echo "executing -> ". $CMDLINE ."\n" >> /tmp/cacti_jmx4node_routestats.log`; 
open (MBEANPIPE, "$CMDLINE|") or die "couldnt spawn jolokia!";
@MBEANRAWINFO = <MBEANPIPE>;
close(MBEANPIPE);
X
$outstring = "";
foreach(@MBEANRAWINFO) {
X
X      $line = $_;
X      #print $line;
X      foreach(@ATTRIBUTES) {
X	 if ($line =~ /$_/) {
X	    $_ = $line;
X	    s/\s{1,}//g;    # get rid of multiple consecutive whitespaces
X	    s/[^A-Za-z0-9\.\: ]//g; #get rid of all other non important characters
X	    s/: /:/g;
X	    $outstring = $outstring . " ". $_;
X	 }	
X      }
}
X
$outstring =~ s/ $//g; #get rid of trailing space
$outstring =~ s/^ {1,}//g; #get rid of leading space
`echo outstring: $outstring >> /tmp/cacti_jmx4node_routestats.log`;
X
X
print $outstring;
X
X
SHAR_EOF
  (set 20 12 08 27 16 27 11 'usr/lib/nagios/plugins/jmx4node_routestats.pl'
   eval "${shar_touch}") && \
  chmod 0755 'usr/lib/nagios/plugins/jmx4node_routestats.pl'
if test $? -ne 0
then ${echo} "restore of usr/lib/nagios/plugins/jmx4node_routestats.pl failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'usr/lib/nagios/plugins/jmx4node_routestats.pl': 'MD5 check failed'
       ) << \SHAR_EOF
bbb3e14c54a48e933f5b97927a456616  usr/lib/nagios/plugins/jmx4node_routestats.pl
SHAR_EOF
  else
test `LC_ALL=C wc -c < 'usr/lib/nagios/plugins/jmx4node_routestats.pl'` -ne 3084 && \
