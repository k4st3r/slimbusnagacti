  sed 's/^X//' << 'SHAR_EOF' > 'usr/lib/nagios/plugins/jolokia.js' &&
/*
X * Copyright 2009-2012 Roland Huss
X *
X * Licensed under the Apache License, Version 2.0 (the "License");
X * you may not use this file except in compliance with the License.
X j * You may obtain a copy of the License at
X *
X * http://www.apache.org/licenses/LICENSE-2.0
X *
X * Unless required by applicable law or agreed to in writing, software
X * distributed under the License is distributed on an "AS IS" BASIS,
X * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.
X * See the License for the specific language governing permissions and
X * limitations under the License.
X */
X
/* =================================
X * Jolokia Javascript Client Library
X * =================================
X *
X * Requires jquery.js and json2.js
X * (if no native JSON.stringify() support is available)
X */
X
(function() {
X
X    var _jolokiaConstructorFunc = function ($) {
X
X        // Default paramerters for GET and POST requests
X        var DEFAULT_CLIENT_PARAMS = {
X            type:"POST",
X            jsonp:false
X        };
X
X        var GET_AJAX_PARAMS = {
X            type:"GET"
X        };
X
X        var POST_AJAX_PARAMS = {
X            type:"POST",
X            processData:false,
X            dataType:"json",
X            contentType:"text/json"
X        };
X
X        // Processing parameters which are added to the
X        // URL as query parameters if given as options
X        var PROCESSING_PARAMS = ["maxDepth", "maxCollectionSize", "maxObjects", "ignoreErrors"];
X
X        /**
X         * Constructor for creating a client to the Jolokia agent.
X         *
X         * An object containing the default parameters can be provided as argument. For the possible parameters
X         * see {@link #request()}.
X         *
X         * @param param either a string in which case it is used as the URL to the agent or
X         *              an object with the default parameters as key-value pairs
X         */
X        function Jolokia(param) {
X            // If called without 'new', we are constructing an object
X            // nevertheless
X            if (!(this instanceof arguments.callee)) {
X                return new Jolokia(param);
X            }
X
X            // Jolokia Javascript Client version
X            this.CLIENT_VERSION = "1.0.5";
X
X            // Registered requests for fetching periodically
X            var jobs = [];
X
X            // State of the scheduler
X            var pollerIsRunning = false;
X
X            // Allow a single URL parameter as well
X            if (typeof param === "string") {
X                param = {url:param};
X            }
X            $.extend(this, DEFAULT_CLIENT_PARAMS, param);
X
X            // ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
X            // Public methods
X
X            /**
X             * The request method using one or more JSON requests and sending it to the agent. Beside the
X             * request a bunch of options can be given, which are merged with the options provided
X             * at the constructor (where the options given here take precedence).
X             *
X             * Known options are:
X             *
X             * <dl>
X             *   <dt>url</dt>
X             *   <dd>Agent URL, which is mandatory</dd>
X             *   <dt>method</dt>
X             *   <dd>
X             *     Either "post" or "get" depending on the desired HTTP method (case does not matter).
X             *     Please note, that bulk requests are not possible with "get". On the other
X             *     hand, JSONP requests are not possible with "post" (which obviously implies
X             *     that bulk request cannot be used with JSONP requests). Also, when using a
X             *     <code>read</code> type request for multiple attributes, this also can
X             *     only be sent as "post" requests. If not given, a HTTP method is determined
X             *     dyamically. If a method is selected which doesn't fit to the request, an error
X             *     is raised.
X             *   </dd>
X             *   <dt>jsonp</dt>
X             *   <dd>
X             *     Whether the request should be sent via JSONP (a technique for allowing cross
X             *     domain request circumventing the infamous "same-origin-policy"). This can be
X             *     used only with HTTP "get" requests.
X             *    </dd>
X             *   <dt>success</dt>
X             *   <dd>
X             *     Callback function which is called for a successful request. The callback receives
X             *     the response as single argument. If no <code>success</code> callback is given, then
X             *     the request is performed synchronously and gives back the response as return
X             *     value.
X             *   </dd>
X             *   <dt>error</dt>
X             *   <dd>
X             *     Callback in case a Jolokia error occurs. A Jolokia error is one, in which the HTTP request
X             *     suceeded with a status code of 200, but the response object contains a status other
X             *     than OK (200) which happens if the request JMX operation fails. This callback receives
X             *     the full Jolokia response object (with a key <code>error</code> set). If no error callback
X             *     is given, but an asynchronous operation is performed, the error response is printed
X             *     to the Javascript console by default.
X             *   </dd>
X             *   <dt>ajaxError</dt>
X             *   <dd>
X             *     Global error callback called when the Ajax request itself failed. It obtains the same arguments
X             *     as the error callback given for <code>jQuery.ajax()</code>, i.e. the <code>XmlHttpResonse</code>,
X             *     a text status and an error thrown. Refer to the jQuery documentation for more information about
X             *     this error handler.
X             *   </dd>
X             *   <dt>username</dt>
X             *   <dd>A username used for HTTP authentication</dd>
X             *   <dt>password</dt>
X             *   <dd>A password used for HTTP authentication</dd>
X             *   <dt>timeout</dt>
X             *   <dd>Timeout for the HTTP request</dd>
X             *   <dt>maxDepth</dt>
X             *   <dd>Maximum traversal depth for serialization of complex return values</dd>
X             *   <dt>maxCollectionSize</dt>
X             *   <dd>
X             *      Maximum size of collections returned during serialization.
X             *      If larger, the collection is returned truncated.
X             *   </dd>
X             *   <dt>maxObjects</dt>
X             *   <dd>
X             *      Maximum number of objects contained in the response.
X             *   </dd>
X             *   <dt>ignoreErrors</dt>
X             *   <dd>
X             *     If set to true, errors during JMX operations and JSON serialization
X             *     are ignored. Otherwise if a single deserialization fails, the whole request
X             *     returns with an error. This works only for certain operations like pattern reads..
X             *   </dd>
X             * </dl>
X             *
X             * @param request the request to send
X             * @param params parameters used for sending the request
X             * @return the response object if called synchronously or nothing if called for asynchronous operation.
X             */
X            this.request = function (request, params) {
X                var opts = $.extend({}, this, params);
X                assertNotNull(opts.url, "No URL given");
X
X                var ajaxParams = {};
X
X                // Copy over direct params for the jQuery ajax call
X                $.each(["username", "password", "timeout"], function (i, key) {
X                    if (opts[key]) {
X                        ajaxParams[key] = opts[key];
X                    }
X                });
X
X                if (extractMethod(request, opts) === "post") {
X                    $.extend(ajaxParams, POST_AJAX_PARAMS);
X                    ajaxParams.data = JSON.stringify(request);
X                    ajaxParams.url = ensureTrailingSlash(opts.url);
X                } else {
X                    $.extend(ajaxParams, GET_AJAX_PARAMS);
X                    ajaxParams.dataType = opts.jsonp ? "jsonp" : "json";
X                    ajaxParams.url = opts.url + "/" + constructGetUrlPath(request);
X                }
X
X                // Add processing parameters as query parameters
X                ajaxParams.url = addProcessingParameters(ajaxParams.url, opts);
X
X                // Global error handler
X                if (opts.ajaxError) {
X                    ajaxParams.error = opts.ajaxError;
X                }
X
X                // Dispatch Callbacks to error and success handlers
X                if (opts.success) {
X                    var success_callback = constructCallbackDispatcher(opts.success);
X                    var error_callback = constructCallbackDispatcher(opts.error);
X                    ajaxParams.success = function (data) {
X                        var responses = $.isArray(data) ? data : [ data ];
X                        for (var idx = 0; idx < responses.length; idx++) {
X                            var resp = responses[idx];
X                            if (Jolokia.isError(resp)) {
X                                error_callback(resp, idx);
X                            } else {
X                                success_callback(resp, idx);
X                            }
X                        }
X                    };
X
X                    // Perform the request
X                    $.ajax(ajaxParams);
X                    return null;
X                } else {
X                    // Synchronous operation requested (i.e. no callbacks provided)
X                    if (opts.jsonp) {
X                        throw Error("JSONP is not supported for synchronous requests");
X                    }
X                    ajaxParams.async = false;
X                    var xhr = $.ajax(ajaxParams);
X                    if (httpSuccess(xhr)) {
X                        return $.parseJSON(xhr.responseText);
X                    } else {
X                        return null;
X                    }
X                }
X            };
X
X            // ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
X            // Schedule related methods
X
X            /**
X             * Register one or more requests for periodically polling the agent along with a callback to call on receipt
X             * of the response.
X             *
X             * @param callback either a function which will be called on a successful performed request or an object
X             *        with the two attributes <code>success</code> and <code>error</code> for two callbacks, one for a
X             *        successful call, one in case on an error. If given only a single callback function, then this
X             *        function is called with all responses received as argument, regardless whether the response indicates
X             *        a success or error stat. For the second case, when an object with an success and error function is
X             *        given, these callback are called with with a single response object as argument. If multiple requests
X             *        have been registered along with this callback object, the callback is called multiple times, one for
X             *        each request in the same order as the request are given.
X             *        As second argument, the handle which is returned by this method is given and as third argument the index
X             *        within the list of requests
X             * @param request, request, .... One or more requests to be registered for this single callback
X             * @return handle which can be used for unregistering the request again or for correlation purposes in the callbacks
X             */
X            this.register = function() {
X                if (arguments.length < 2) {
X                    throw "At a least one request must be provided";
X                }
X                var callback = arguments[0],
X                    requests = Array.prototype.slice.call(arguments,1),
X                    job;
X                if (typeof callback === 'object') {
X                    job = {
X                        success: callback.success,
X                        error: callback.error,
X                        callback: null
X                    };
X                } else if (typeof callback === 'function') {
X                    job = {
X                        success: null,
X                        error: null,
X                        callback: callback
X                    };
X                } else {
X                    throw "First argument must be either a callback func " + "or an object with 'success' and 'error' attrs";
X                }
X                job.requests = requests;
X                jobs.push(job);
X                return jobs.length - 1;
X            };
X
X            /**
X             * Unregister a one or more request which has been registered with {@link #registerRequest}. As parameter
X             * the handle returned during the registration process must be given
X             * @param handle
X             */
X            this.unregister = function(handle) {
X                if (handle < jobs.length) {
X                    jobs.splice(handle,1);
X                }
X            };
X
X            /**
X             * Start the poller. The interval between two polling attempts can be optionally given or are taken from
X             * the parameter <code>fetchInterval</code> given at construction time. If no interval is given at all,
X             * 30 seconds is the default.
X             *
X             * If the poller is already running (i.e. {@link #isRunning()} is <code>true</code> then the scheduler
X             * is restarted, but only if the new interval differs from the currently active one.
X             *
X             * @param interval interval in milliseconds between two polling attempts
X             */
X            this.start = function(interval) {
X                interval = interval || this.fetchInterval || 30000;
X                if (pollerIsRunning) {
X                    if (interval === this.fetchInterval) {
X                        // Nothing to do
X                        return;
X                    }
X                    // Re-start with new interval
X                    this.stop();
X                }
X                this.fetchInterval = interval;
X                this.timerId = setInterval(callJolokia(this,jobs), interval);
X
X                pollerIsRunning = true;
X            };
X
X            /**
X             * Stop the poller. If the poller is not running, no operation is performed.
X             */
X            this.stop = function() {
X                if (!pollerIsRunning && this.timerId != undefined) {
X                    return;
X                }
X                clearInterval(this.timerId);
X                this.timerId = null;
X
X                pollerIsRunning = false;
X            };
X
X            /**
X             * Check whether the poller is running.
X             * @return true if the poller is running, false otherwise.
X             */
X            this.isRunning = function() {
X                return pollerIsRunning;
X            };
X
X            // ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
X        }
X
X        // Private Methods:
X
X        // ========================================================================
X
X
X        // Create a function called by a timer, which requests the registered requests
X        // calling the stored callback on receipt. jolokia and jobs are put into the closure
X        function callJolokia(jolokia,jobs) {
X            return function() {
X                var errorCbs = [],
X                    successCbs = [],
X                    reqs,
X                    i, j,
X                    len = jobs.length;
X                var requests = [];
X                var opts;
X                for (i = 0; i < len; i++) {
X                    var job = jobs[i];
X                    reqs = job != null ? job.requests : void 0;
X                    var reqsLen = reqs.length;
X                    if (job.success) {
X                        // Success/error pair of callbacks
X                        var successCb = cbSuccessErrorClosure("success",job,i);
X                        var errorCb = cbSuccessErrorClosure("error",job,i);
X                        for (j = 0; j < reqsLen; j++) {
X                            requests.push(reqs[j]);
X                            successCbs.push(successCb);
X                            errorCbs.push(errorCb);
X                        }
X                    } else {
X                        // Pure callback getting all requests at once
X                        var dCb = cbCallbackClosure(job,jolokia);
X                        // Add callbacks which collect the responses
X                        for (j = 0; j < reqsLen - 1; j++) {
X                            requests.push(reqs[j]);
X                            successCbs.push(dCb.cb);
X                            errorCbs.push(dCb.cb);
X                        }
X                        // Add final callback which calls the last method
X                        requests.push(reqs[reqsLen-1]);
X                        successCbs.push(dCb.lcb);
X                        errorCbs.push(dCb.lcb);
X                    }
X                }
X                opts = {
X                    success: function(resp, j) {
X                        return successCbs[j].apply(jolokia, [resp, j]);
X                    },
X                    error: function(resp, j) {
X                        return errorCbs[j].apply(jolokia, [resp, j]);
X                    }
X                };
X                return jolokia.request(requests, opts);
X            };
X        }
X
X        // Closure for a full callback which stores the responses in an (closed) array
X        function cbCallbackClosure(job,jolokia) {
X            var responses = [],
X                callback = job.callback;
X
X            return {
X                cb : function(resp,j) {
X                    responses.push(resp);
X                },
X                lcb : function(resp,j) {
X                    responses.push(resp);
X                    callback.apply(jolokia,responses);
X                }
X            };
X        }
X
X        // Own function for creating a closure to avoid reference to mutable state in the loop
X        function cbSuccessErrorClosure(type, job, i) {
X            return function(resp,j) {
X                if (job[type]) {
X                    job[type](resp,i,j)
X                }
X            }
X        }
X
X        // Construct a callback dispatcher for appropriately dispatching
X        // to a single callback or within an array of callbacks
X        function constructCallbackDispatcher(callback) {
X            if (callback == null) {
X                return function (response) {
X                    console.log("Ignoring response " + JSON.stringify(response));
X                };
X            } else if (callback === "ignore") {
X                // Ignore the return value
X                return function () {
X                };
X            }
X            var callbackArray = $.isArray(callback) ? callback : [ callback ];
X            return function (response, idx) {
X                callbackArray[idx % callbackArray.length](response, idx);
X            }
X        }
X
X        // Extract the HTTP-Method to use and make some sanity checks if
X        // the method was provided as part of the options, but dont fit
X        // to the request given
X        function extractMethod(request, opts) {
X            var methodGiven = opts && opts.method ? opts.method.toLowerCase() : null,
X                    method;
X            if (methodGiven) {
X                if (methodGiven === "get") {
X                    if ($.isArray(request)) {
X                        throw new Error("Cannot use GET with bulk requests");
X                    }
X                    if (request.type.toLowerCase() === "read" && $.isArray(request.attribute)) {
X                        throw new Error("Cannot use GET for read with multiple attributes");
X                    }
X                    if (request.target) {
X                        throw new Error("Cannot use GET request with proxy mode");
X                    }
X                }
X                method = methodGiven;
X            } else {
X                // Determine method dynamically
X                method = $.isArray(request) ||
X                         (request.type.toLowerCase() === "read" && $.isArray(request.attribute)) ||
X                         request.target ?
X                        "post" : "get";
X            }
X            if (opts.jsonp && method === "post") {
X                throw new Error("Can not use JSONP with POST requests");
X            }
X            return method;
X        }
X
X        // Add processing parameters given as request options
X        // to an URL as GET query parameters
X        function addProcessingParameters(url, opts) {
X            var sep = url.indexOf("?") > 0 ? "&" : "?";
X            $.each(PROCESSING_PARAMS, function (i, key) {
X                if (opts[key] != null) {
X                    url += sep + key + "=" + opts[key];
X                    sep = "&";
X                }
X            });
X            return url;
X        }
X
X        // ========================================================================
X        // GET-Request handling
X
X        // Create the URL used for a GET request
X        function constructGetUrlPath(request) {
X            var type = request.type;
X            assertNotNull(type, "No request type given for building a GET request");
X            type = type.toLowerCase();
X            var extractor = GET_URL_EXTRACTORS[type];
X            assertNotNull(extractor, "Unknown request type " + type);
X            var result = extractor(request);
X            var parts = result.parts || {};
X            var url = type;
X            $.each(parts, function (i, v) {
X                url += "/" + Jolokia.escape(v)
X            });
X            if (result.path) {
X                url += (result.path[0] == '/' ? "" : "/") + result.path;
X            }
X            return url;
X        }
X
X        // For POST requests it is recommended to have a trailing slash at the URL
X        // in order to avoid a redirect which then results in a GET request.
X        // See also https://bugs.eclipse.org/bugs/show_bug.cgi?id=331194#c1
X        // for an explanation
X        function ensureTrailingSlash(url) {
X            // Squeeze any URL to a single one, optionally adding one
X            return url.replace(/\/*$/, "/");
X        }
X
X        // Extractors used for preparing a GET request, i.e. for creating a stack
X        // of arguments which gets appended to create the proper access URL
X        // key: lowercase request type.
X        // The return value is an object with two properties: The 'parts' to glue together, where
X        // each part gets escaped and a 'path' which is appended literally
X        var GET_URL_EXTRACTORS = {
X            "read":function (request) {
X                if (request.attribute == null) {
X                    // Path gets ignored for multiple attribute fetch
X                    return { parts:[ request.mbean ] };
X                } else {
X                    return { parts:[ request.mbean, request.attribute ], path:request.path };
X                }
X            },
X            "write":function (request) {
X                return { parts:[request.mbean, request.attribute, valueToString(request.value)], path:request.path};
X            },
X            "exec":function (request) {
X                var ret = [ request.mbean, request.operation ];
X                if (request.arguments && request.arguments.length > 0) {
X                    $.each(request.arguments, function (index, value) {
X                        ret.push(valueToString(value));
X                    });
X                }
X                return {parts:ret};
X            },
X            "version":function () {
X                return {};
X            },
X            "search":function (request) {
X                return { parts:[request.mbean]};
X            },
X            "list":function (request) {
X                return { path:request.path};
X            }
X        };
X
X        // Convert a value to a string for passing it to the Jolokia agent via
X        // a get request (write, exec). Value can be either a single object or an array
X        function valueToString(value) {
X            if (value == null) {
X                return "[null]";
X            }
X            if ($.isArray(value)) {
X                var ret = "";
X                for (var i = 0; i < value.length; i++) {
X                    ret += value == null ? "[null]" : singleValueToString(value[i]);
X                    if (i < value.length - 1) {
X                        ret += ",";
X                    }
X                }
X                return ret;
X            } else {
X                return singleValueToString(value);
X            }
X        }
X
X        // Single value conversion for write/exec GET requests
X        function singleValueToString(value) {
X            if (typeof value === "string" && value.length == 0) {
X                return "\"\"";
X            } else {
X                return value.toString();
X            }
X        }
X
X        // Check whether a synchronous request was a success or not
X        // Taken from jQuery 1.4
X        function httpSuccess(xhr) {
X            try {
X                return !xhr.status && location.protocol === "file:" ||
X                       xhr.status >= 200 && xhr.status < 300 ||
X                       xhr.status === 304 || xhr.status === 1223;
X            } catch (e) {
X            }
X            return false;
X        }
X
X        // ===============================================================================================
X        // Utility methods:
X
X        function assertNotNull(object, message) {
X            if (object == null) {
X                throw new Error(message);
X            }
X        }
X
X        // ================================================================================================
X
X        // Escape a path part, can be used as a static method outside this function too
X        Jolokia.prototype.escape = Jolokia.escape = function (part) {
X            return part.replace(/!/g, "!!").replace(/\//g, "!/");
X        };
X
X        /**
X         * Utility method which checks whether a response is an error or a success
X         * @param resp response to check
X         * @return true if response is an error, false otherwise
X         */
X        Jolokia.prototype.isError = Jolokia.isError = function(resp) {
X            return resp.status == null || resp.status != 200;
X        };
X
X        // Return back exported function/constructor
X        return Jolokia;
X    };
X
X    // =====================================================================================================
X    // Register either as global or as AMD module
X
X    (function (root, factory) {
X        if (typeof define === 'function' && define.amd) {
X            // AMD. Register as a named module
X            define(["jquery"], factory);
X        } else {
X            // Browser globals
X            root.Jolokia = factory(root.jQuery);
X        }
X    }(this, function (jQuery) {
X        return _jolokiaConstructorFunc(jQuery);
X    }));
}());
SHAR_EOF
  (set 20 12 08 27 16 27 11 'usr/lib/nagios/plugins/jolokia.js'
   eval "${shar_touch}") && \
  chmod 0755 'usr/lib/nagios/plugins/jolokia.js'
if test $? -ne 0
then ${echo} "restore of usr/lib/nagios/plugins/jolokia.js failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'usr/lib/nagios/plugins/jolokia.js': 'MD5 check failed'
       ) << \SHAR_EOF
3fedf4121141d231354f144866ed90fa  usr/lib/nagios/plugins/jolokia.js
SHAR_EOF
  else
test `LC_ALL=C wc -c < 'usr/lib/nagios/plugins/jolokia.js'` -ne 27286 && \
  ${echo} "restoration warning:  size of 'usr/lib/nagios/plugins/jolokia.js' is not 27286"
