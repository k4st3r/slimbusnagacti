SHAR_EOF
  (set 20 12 07 23 22 56 59 'usr/lib/nagios/plugins/jmxquery.jar'
   eval "${shar_touch}") && \
  chmod 0755 'usr/lib/nagios/plugins/jmxquery.jar'
if test $? -ne 0
then ${echo} "restore of usr/lib/nagios/plugins/jmxquery.jar failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'usr/lib/nagios/plugins/jmxquery.jar': 'MD5 check failed'
       ) << \SHAR_EOF
6d9939b414291565aa902a806e4cc7cf  usr/lib/nagios/plugins/jmxquery.jar
SHAR_EOF
  else
test `LC_ALL=C wc -c < 'usr/lib/nagios/plugins/jmxquery.jar'` -ne 6383 && \
  ${echo} "restoration warning:  size of 'usr/lib/nagios/plugins/jmxquery.jar' is not 6383"
  fi
fi
# ============= usr/lib/nagios/plugins/check_jmx ==============
if test ! -d 'usr/lib/nagios/plugins'; then
  mkdir 'usr/lib/nagios/plugins'
if test $? -eq 0
then ${echo} "x - created directory usr/lib/nagios/plugins."
else ${echo} "x - failed to create directory usr/lib/nagios/plugins."
     exit 1
fi
fi
if test -n "${keep_file}" && test -f 'usr/lib/nagios/plugins/check_jmx'
then
${echo} "x - SKIPPING usr/lib/nagios/plugins/check_jmx (file already exists)"
else
${echo} "x - extracting usr/lib/nagios/plugins/check_jmx (text)"
  sed 's/^X//' << 'SHAR_EOF' > 'usr/lib/nagios/plugins/check_jmx' &&
#!/bin/sh
#
# Nagios plugin to monitor Java JMX (http://java.sun.com/jmx)attributes.
#
RDIR=`dirname $0`
java -cp $RDIR/jmxquery.jar org.nagios.JMXQuery $@
SHAR_EOF
  (set 20 12 07 23 22 56 59 'usr/lib/nagios/plugins/check_jmx'
   eval "${shar_touch}") && \
  chmod 0755 'usr/lib/nagios/plugins/check_jmx'
if test $? -ne 0
then ${echo} "restore of usr/lib/nagios/plugins/check_jmx failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'usr/lib/nagios/plugins/check_jmx': 'MD5 check failed'
       ) << \SHAR_EOF
aac647a2763b203dc6c848c9088c89a0  usr/lib/nagios/plugins/check_jmx
SHAR_EOF
  else
test `LC_ALL=C wc -c < 'usr/lib/nagios/plugins/check_jmx'` -ne 156 && \
  ${echo} "restoration warning:  size of 'usr/lib/nagios/plugins/check_jmx' is not 156"
  fi
fi
# ============= usr/share/nagios/disco-scripts/nmapDiscover.sh ==============
if test ! -d 'usr/share'; then
  mkdir 'usr/share'
if test $? -eq 0
then ${echo} "x - created directory usr/share."
else ${echo} "x - failed to create directory usr/share."
     exit 1
fi
fi
if test ! -d 'usr/share/nagios'; then
  mkdir 'usr/share/nagios'
if test $? -eq 0
then ${echo} "x - created directory usr/share/nagios."
else ${echo} "x - failed to create directory usr/share/nagios."
     exit 1
fi
fi
if test ! -d 'usr/share/nagios/disco-scripts'; then
  mkdir 'usr/share/nagios/disco-scripts'
if test $? -eq 0
then ${echo} "x - created directory usr/share/nagios/disco-scripts."
else ${echo} "x - failed to create directory usr/share/nagios/disco-scripts."
     exit 1
fi
fi
if test -n "${keep_file}" && test -f 'usr/share/nagios/disco-scripts/nmapDiscover.sh'
then
${echo} "x - SKIPPING usr/share/nagios/disco-scripts/nmapDiscover.sh (file already exists)"
else
${echo} "x - extracting usr/share/nagios/disco-scripts/nmapDiscover.sh (text)"
  sed 's/^X//' << 'SHAR_EOF' > 'usr/share/nagios/disco-scripts/nmapDiscover.sh' &&
#!/bin/bash
sudo nmap -A -O --osscan-guess -sV -v --servicedb=./nimbmap-services -oG=discovered_hosts --system-dns `./getMyVlanSubnet.sh`
SHAR_EOF
  (set 20 12 07 25 19 03 13 'usr/share/nagios/disco-scripts/nmapDiscover.sh'
   eval "${shar_touch}") && \
  chmod 0775 'usr/share/nagios/disco-scripts/nmapDiscover.sh'
if test $? -ne 0
then ${echo} "restore of usr/share/nagios/disco-scripts/nmapDiscover.sh failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'usr/share/nagios/disco-scripts/nmapDiscover.sh': 'MD5 check failed'
       ) << \SHAR_EOF
32a855f95cf7080390e7343c2f0d7ca3  usr/share/nagios/disco-scripts/nmapDiscover.sh
SHAR_EOF
  else
test `LC_ALL=C wc -c < 'usr/share/nagios/disco-scripts/nmapDiscover.sh'` -ne 138 && \
  ${echo} "restoration warning:  size of 'usr/share/nagios/disco-scripts/nmapDiscover.sh' is not 138"
  fi
fi
# ============= usr/share/nagios/disco-scripts/nimbmap-services ==============
if test ! -d 'usr/share/nagios/disco-scripts'; then
  mkdir 'usr/share/nagios/disco-scripts'
if test $? -eq 0
then ${echo} "x - created directory usr/share/nagios/disco-scripts."
else ${echo} "x - failed to create directory usr/share/nagios/disco-scripts."
     exit 1
fi
fi
if test -n "${keep_file}" && test -f 'usr/share/nagios/disco-scripts/nimbmap-services'
then
${echo} "x - SKIPPING usr/share/nagios/disco-scripts/nimbmap-services (file already exists)"
else
${echo} "x - extracting usr/share/nagios/disco-scripts/nimbmap-services (text)"
