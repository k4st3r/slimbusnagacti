#!/bin/bash

RUNSTARTTIME=`date +%Y%m%d%H%M%S`
SCRIPTLOG=/tmp/naginstall_$RUNSTARTTIME
MYPATH=$0
EXTRACT_SHARFILE=/tmp/nagcac_config_shar_script_$RUNSTARTTIME
#EXTRACT_STAGE_DIR=/tmp/stage_extract_$RUNSTARTTIME
EXTRACT_STAGE_DIR=/
echo For this run:
echo -- Your script results log = $SCRIPTLOG
echo -- Your extract sharfile = $EXTRACT_SHARFILE
echo -- Your extract staging directory = $EXTRACT_STAGE_DIR
echo Once you have verified your install is successful you can delete the above files and folders...

function error_exit
{
	echo "$@" | tee -a $SCRIPTLOG
	exit -1
}

function install_nagios_cacti_and_deps
{
(
 echo && echo && echo "$FUNCNAME now entering"
 #echo -n Go/? \(CTRL-C to exit now\)... 
 #read discard
 sudo apt-get -y update 2>&1

 sudo DEBIAN_FRONTEND=noninteractive aptitude install -q -y \
	nagios3	nagios-nrpe-plugin nagios-nrpe-doc \
	nagios-nrpe-server nagios-snmp-plugins nsca \
	nagios-plugin-check-multi nagios-plugins-extra \
	libnagios-plugin-perl libnagios-object-perl \
	gawk php5 subversion procmail php5-curl sharutils \
	openjdk-7-jdk unzip erlang make libwww-perl \
	libjson-any-perl libxml-simple-perl nmap ipcalc 2>&1 \
	|| error_exit "failed to get dependency packages from repo"

 sudo DEBIAN_FRONTEND=noninteractive aptitude install -q -y \
	ndoutils-nagios3-mysql ipcalc nodejs 2>&1 || error_exit "failed to get ndoutils from repo"

 sudo DEBIAN_FRONTEND=noninteractive aptitude install -q -y npm cacti 2>&1 \
	|| error_exit "failed to get cacti from repo"

 sudo npm install underscore 2>&1
 sudo npm install optimist 2>&1
 # mysql for ndoutils with nagios for provisioning
 #sudo mysqladmin -u root password cspdrocks! 2>&1 | tee -a $SCRIPTLOG
 echo && echo "$FUNCNAME now exiting" 
 ) | tee -a $SCRIPTLOG
}

function extract_sharutils_script_attachment
{
  (
   echo && echo && echo "$FUNCNAME now executing"
   ARCHIVE=`awk '/^__ARCHIVE_BELOW__/ { print NR + 1; exit 0; }' $MYPATH`
   tail -n+$ARCHIVE $0 > "$EXTRACT_SHARFILE" 2>&1
   chmod +x "$EXTRACT_SHARFILE" 2>&1 
   [ -e "$EXTRACT_STAGE_DIR" ] || mkdir "$EXTRACT_STAGE_DIR"  2>&1 
   cd "$EXTRACT_STAGE_DIR"  2>&1 && sudo "$EXTRACT_SHARFILE" -c 2>&1
   echo && echo "$FUNCNAME now exiting"
  ) | tee -a $SCRIPTLOG
}

function post_process_extracted_contents
{ 
  (
    echo && echo && echo "$FUNCNAME now executing"
    sudo htpasswd -cb /etc/nagios3/htpasswd.users nagiosadmin changeme
    usermod -G nagios www-data
    sudo chmod -R g+rwx /var/lib/nagios3
    sudo /etc/init.d/nagios3 restart
    sudo /etc/init.d/apache2 restart
    sudo sed -i 's/debug=0/debug=1/g' /etc/nsca.cfg
    sudo sed -i 's/#password=/password=testpass/g' /etc/nsca.cfg
    sudo /etc/init.d/nsca start
    echo && echo "$FUNCNAME now exiting" 
  ) | tee -a $SCRIPTLOG
}

function discover_and_configure_nagios_targets
{
  (
    echo && echo && echo "$FUNCNAME now executing"
    SAVEDIR=`pwd`
    cd /usr/share/nagios/disco-scripts && ./nmapDiscover.sh 2>&1 || error_exit "failed to run nmap discovery"
    grep -iv "status: down" ./discovered_hosts > /tmp/discovered_hosts_up_$RUNSTARTTIME 2>&1 || error_exit "failed to trim nmap discovery results"
    sudo ./nbi2.pl /tmp/discovered_hosts_up_$RUNSTARTTIME 2>&1 
    [ -e hosts.cfg ] || error_exit "failed to derive nagios host config from nmap trimmed nmap discovery results"
    # verify config will parse
    rm /tmp/discovered_hosts_up_$RUNSTARTTIME 2>&1
    CFGTARGET=/etc/nagios3/conf.d/discovered_hosts_nag 2>&1
    sudo mv hosts.cfg $CFGTARGET-$RUNSTARTTIME.cfg 2>&1
    CFGTESTRESULTS=$(sudo /usr/sbin/nagios3 -v /etc/nagios3/nagios.cfg)
    echo Config test results = 
    echo "$CFGTESTRESULTS"
    TRES=`echo "$CFGTESTRESULTS"|grep Errors|awk '{print $3;}'|grep -v [1-9]`
    if [ "$TRES" != "0" ]; then
      sudo mv $CFGTARGET-$RUNSTARTTIME.cfg \
	/tmp/discovered_hosts_nag-$RUNSTARTTIME.cfg.fail 2&>1
      echo "nagios discovered hosts cfg file failed validation! aborting! " 2>&1
      error_exit "check /tmp/discovered_hosts_nag-$RUNSTARTTIME.cfg.fail " 2>&1
    else
      sudo /etc/init.d/nagios3 reload 2>&1
    fi
    echo && echo "$FUNCNAME now exiting" && echo
  ) | tee -a $SCRIPTLOG
}



### !!!! THIS IS "MAIN" !!!! ####
install_nagios_cacti_and_deps
extract_sharutils_script_attachment
post_process_extracted_contents
discover_and_configure_nagios_targets

exit 0

# End of first part Bash part of script
# ==============================================================================
