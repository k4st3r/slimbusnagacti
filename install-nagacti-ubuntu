SHAR_EOF
  (set 20 12 08 15 18 07 44 'usr/share/nagios/disco-scripts/nimbmap-service-probes'
   eval "${shar_touch}") && \
  chmod 0644 'usr/share/nagios/disco-scripts/nimbmap-service-probes'
if test $? -ne 0
then ${echo} "restore of usr/share/nagios/disco-scripts/nimbmap-service-probes failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'usr/share/nagios/disco-scripts/nimbmap-service-probes': 'MD5 check failed'
       ) << \SHAR_EOF
903dd321a7fed9f03cfc5faef54f3c52  usr/share/nagios/disco-scripts/nimbmap-service-probes
SHAR_EOF
  else
test `LC_ALL=C wc -c < 'usr/share/nagios/disco-scripts/nimbmap-service-probes'` -ne 1050395 && \
  ${echo} "restoration warning:  size of 'usr/share/nagios/disco-scripts/nimbmap-service-probes' is not 1050395"
  fi
fi
if rm -fr ${lock_dir}
then ${echo} "x - removed lock directory ${lock_dir}."
else ${echo} "x - failed to remove lock directory ${lock_dir}."
     exit 1
fi
exit 0
