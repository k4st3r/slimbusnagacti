#!/usr/bin/env node
X
var _usage = "Usage: $0 <url> <command> [params]\n" +
X    "  valid <command>s are\n" +
X    "    list [<mbean>]\n" +
X    "    read <mbean> [<attribute>]" +
X    "    dump [<mbean>]";
X
var util = require('util');
var jolokia = require("./index");
var _ = require("underscore");
var argv = require('optimist').usage(_usage).argv;
X
process.on('uncaughtException', function(err) {
X  console.log("exception while running: " + err.message + "\n" + err.stack + "\n");
X  process.exit(1);
});
X
function usage() {
X  argv.showHelp();
X  process.exit(1);
}
X
if (process.argv.length < 4) {
X  usage();
}
X
var url = process.argv[2];
var client = new jolokia(url);
var command = process.argv[3];
var args = _.rest(process.argv, 4);
X
if (command === 'list') {
X  client.list(args, function(response){
X    console.log(util.inspect(response, true, 10));
X  });
} else if (command === 'read') {
X  client.read(args[0], args[1], function(response){
X    console.log(util.inspect(response, true, 10));
X  });
} else if (command === 'dump') {
X  client.dump(args[0], function(response){
X    console.log(util.inspect(response, true, 10));
X  });
} else {
X  usage();
}
SHAR_EOF
  (set 20 12 08 27 16 27 11 'usr/lib/nagios/plugins/jmx4node'
   eval "${shar_touch}") && \
