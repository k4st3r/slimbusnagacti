  sed 's/^X//' << 'SHAR_EOF' > 'usr/lib/nagios/plugins/jolokia-simple.js' &&
/*
X * Copyright 2009-2010 Roland Huss
X *
X * Licensed under the Apache License, Version 2.0 (the "License");
X * you may not use this file except in compliance with the License.
X * You may obtain a copy of the License at
X *
X * http://www.apache.org/licenses/LICENSE-2.0
X *
X * Unless required by applicable law or agreed to in writing, software
X * distributed under the License is distributed on an "AS IS" BASIS,
X * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.
X * See the License for the specific language governing permissions and
X * limitations under the License.
X */
X
X
/**
X * Simplified access to the Jolokia agent.
X *
X * This script will add convenience methods to <code>Jolokia</code> for
X * simplified access to JMX information. Before including this script, "jolokia.js"
X * must be included.
X *
X * It is recommended to compress this script before using it in production.
X *
X * @author roland
X */
X
(function() {
X    var builder = function($,Jolokia) {
X        /**
X         * Get one or more attributes
X         *
X         * @param mbean objectname of MBean to query. Can be a pattern.
X         * @param attribute attribute name. If an array, multiple attributes are fetched.
X         *                  If <code>null</code>, all attributes are fetched.
X         * @param path optional path within the return value. For multi-attribute fetch, the path
X         *             is ignored.
X         * @param opts options passed to Jolokia.request()
X         * @return the value of the attribute, possibly a complex object
X         */
X        function getAttribute(mbean,attribute,path,opts) {
X            if (arguments.length === 3 && $.isPlainObject(path)) {
X                opts = path;
X                path = null;
X            } else if (arguments.length == 2 && $.isPlainObject(attribute)) {
X                opts = attribute;
X                attribute = null;
X                path = null;
X            }
X            var req = { type: "read", mbean: mbean, attribute: attribute };
X            addPath(req,path);
X            return extractValue(this.request(req,prepareSucessCallback(opts)),opts);
X        }
X
X        /**
X         * Set an attribute on a MBean.
X         *
X         * @param mbean objectname of MBean to set
X         * @param attribute the attribute to set
X         * @param value the value to set
X         * @param path an optional <em>inner path</em> which, when given, is used to determine
X         *        an inner object to set the value on
X         * @param opts additional options passed to Jolokia.request()
X         * @return the previous value
X         */
X        function setAttribute(mbean,attribute,value,path,opts) {
X            if (arguments.length === 4 && $.isPlainObject(path)) {
X                opts = path;
X                path = null;
X            }
X            var req = { type: "write", mbean: mbean, attribute: attribute, value: value };
X            addPath(req,path);
X            return extractValue(this.request(req,prepareSucessCallback(opts)),opts);
X        }
X
X        /**
X         * Execute a JMX operation and return the result value
X         *
X         * @param mbean objectname of the MBean to operate on
X         * @param operation name of operation to execute. Can contain a signature in case overloaded
X         *                  operations are to be called (comma separated fully qualified argument types
X         *                  append to the operation name within parentheses)
X         * @param arg1, arg2, ..... one or more argument required for executing the operation.
X         * @param opts optional options for Jolokia.request() (must be an object)
X         * @return the return value of the JMX operation.
X         */
X        function execute(mbean,operation) {
X            var req = { type: "exec", mbean: mbean, operation: operation };
X            var opts, end = arguments.length;
X            if (arguments.length > 2 && $.isPlainObject(arguments[arguments.length-1])) {
X                opts = arguments[arguments.length-1];
X                end = arguments.length-1;
X            }
X            if (end > 2) {
X                var args = [];
X                for (var i = 2; i < end; i++) {
X                    args[i-2] = arguments[i];
X                }
X                req.arguments = args;
X            }
X            return extractValue(this.request(req,prepareSucessCallback(opts)),opts);
X        }
X
X        /**
X         * Search for MBean based on a pattern and return a reference to the list of found
X         * MBeans names (as string). If no MBean can be found, <code>null</code> is returned. For
X         * example,
X         *
X         * jolokia.search("*:j2eeType=J2EEServer,*")
X         *
X         * searches all MBeans whose name are matching this pattern, which are according
X         * to JSR77 all application servers in all available domains.
X         *
X         * @param mbeanPattern pattern to search for
X         * @param opts optional options for Jolokia.request()
X         * @return an array with ObjectNames as string
X         */
X        function search(mbeanPattern,opts) {
X            var req = { type: "search", mbean: mbeanPattern};
X            return extractValue(this.request(req,prepareSucessCallback(opts)),opts);
X        }
X
X        /**
X         * This method return the version of the agent and the Jolokia protocol
X         * version as part of an object. If available, server specific information
X         * like the application server's name are returned as wel.
X         * A typical response looks like
X         *
X         * <pre>
X         *  {
X         *    protocol: "4.0",
X         *    agent: "0.82",
X         *    info: {
X         *       product: "glassfish",
X         *       vendor": "Sun",
X         *       extraInfo: {
X         *          amxBooted: false
X         *       }
X         *  }
X         * </pre>
X         *
X         * @param opts optional options for Jolokia.request()
X         * @param version and other meta information as object
X         */
X        function version(opts) {
X            return extractValue(this.request({type: "version"},prepareSucessCallback(opts)),opts);
X        }
X
X
X        /**
X         * Get all MBeans as registered at the specified server. A C<$path> can be
X         * specified in order to fetchy only a subset of the information. When no path is
X         * given, the returned value has the following format
X         *
X         * <pre>
X         * {
X         *     &lt;domain&gt; :
X         *     {
X         *       &lt;canonical property list&gt; :
X         *       {
X         *           "attr" :
X         *           {
X         *              &lt;atrribute name&gt; :
X         *              {
X         *                 desc : &lt;description of attribute&gt;
X         *                 type : &lt;java type&gt;,
X         *                 rw : true/false
X         *              },
X         *              ....
X         *           },
X         *           "op" :
X         *           {
X         *              &lt;operation name&gt; :
X         *              {
X         *                "desc" : &lt;description of operation&gt;
X         *                "ret" : &lt;return java type&gt;
X         *                "args" :
X         *                [
X         *                   {
X         *                     "desc" : &lt;description&gt;,
X         *                     "name" : &lt;name&gt;,
X         *                     "type" : &lt;java type&gt;
X         *                   },
X         *                   ....
X         *                ]
X         *              },
X         *              ....
X         *       },
X         *       ....
X         *     }
X         *     ....
X         *  }
X         * </pre>
X         *
X         * A complete path has the format &lt;domain&gt;/property
X         * list&gt;/("attribute"|"operation")/&lt;index&gt;">
X         * (e.g. <code>java.lang/name=Code Cache,type=MemoryPool/attribute/0</code>). A path can be
X         * provided partially, in which case the remaining map/array is returned. The path given must
X         * be already properly escaped (i.e. slashes must be escaped like <code>!/</code> and exlamation
X         * marks like <code>!!</code>.
X         * See also the Jolokia Reference Manual for a more detailed discussion of inner pathes and escaping.
X         *
X         *
X         * @param path optional path for diving into the list
X         * @param opts optional opts passed to Jolokia.request()
X         */
X        function list(path,opts) {
X            if (arguments.length == 1 && !$.isArray(path) && $.isPlainObject(path)) {
X                opts = path;
X                path = null;
X            }
X            var req = { type: "list" };
X            addPath(req,path);
X            return extractValue(this.request(req,prepareSucessCallback(opts)),opts);
X        }
X
X        // =======================================================================
X        // Private methods:
X
X        // If path is an array, the elements get escaped. If not, it is
X        // taken directly
X        function addPath(req,path) {
X            if (path != null) {
X                if ($.isArray(path)) {
X                    req.path = $.map(path,Jolokia.escape).join("/");
X                } else {
X                    req.path = path;
X                }
X            }
X        }
X
X        function extractValue(response,opts) {
X            if (response == null) {
X                return null;
X            }
X            if (response.status == 200) {
X                return response.value;
X            }
X            if (opts && opts.error) {
X                return opts.error(response);
X            } else {
X                throw new Error("Jolokia-Error: " + JSON.stringify(response));
X            }
X        }
X
X        // Prepare callback to receive directly the value (instead of the full blown response)
X        function prepareSucessCallback(opts) {
X            if (opts && opts.success) {
X                var parm = $.extend({},opts);
X                parm.success = function(resp) {
X                    opts.success(resp.value);
X                };
X                return parm;
X            } else {
X                return opts;
X            }
X        }
X
X        // Extend the Jolokia prototype with new functionality (mixin)
X        $.extend(Jolokia.prototype,
X                {
X                    "getAttribute" : getAttribute,
X                    "setAttribute" : setAttribute,
X                    "execute": execute,
X                    "search": search,
X                    "version": version,
X                    "list": list
X                });
X        return Jolokia;
X    };
X
X    // =====================================================================================================
X    // Register either at the global Jolokia object global or as an AMD module
X    (function (root, factory) {
X        if (typeof define === 'function' && define.amd) {
X            // AMD. Register as a named module
X            define(["jquery","jolokia"], factory);
X        } else {
X            if (root.Jolokia) {
X                builder(jQuery,root.Jolokia);
X            } else {
X                console.error("No Jolokia definition found. Please include jolokia.js before jolokia-simple.js");
X            }
X        }
X    }(this, function (jQuery,Jolokia) {
X        return builder(jQuery,Jolokia);
X    }));
})();
SHAR_EOF
  (set 20 12 08 27 16 27 11 'usr/lib/nagios/plugins/jolokia-simple.js'
   eval "${shar_touch}") && \
  chmod 0755 'usr/lib/nagios/plugins/jolokia-simple.js'
if test $? -ne 0
then ${echo} "restore of usr/lib/nagios/plugins/jolokia-simple.js failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'usr/lib/nagios/plugins/jolokia-simple.js': 'MD5 check failed'
       ) << \SHAR_EOF
f2a54b6c381c64bb10a6416ffe692847  usr/lib/nagios/plugins/jolokia-simple.js
SHAR_EOF
  else
test `LC_ALL=C wc -c < 'usr/lib/nagios/plugins/jolokia-simple.js'` -ne 11330 && \
  ${echo} "restoration warning:  size of 'usr/lib/nagios/plugins/jolokia-simple.js' is not 11330"
  fi
fi
# ============= usr/lib/nagios/plugins/check_wrapper ==============
if test -n "${keep_file}" && test -f 'usr/lib/nagios/plugins/check_wrapper'
then
${echo} "x - SKIPPING usr/lib/nagios/plugins/check_wrapper (file already exists)"
else
${echo} "x - extracting usr/lib/nagios/plugins/check_wrapper (text)"
  sed 's/^X//' << 'SHAR_EOF' > 'usr/lib/nagios/plugins/check_wrapper' &&
#! /bin/bash
X
PROGNAME=`/usr/bin/basename $0`
PROGPATH=`echo $0 | sed -e 's,[\\/][^\\/][^\\/]*$,,'`
REVISION="0.0"
PLUGDIR=`dirname $0`
X
X. $PROGPATH/utils.sh
X
if [ $# -lt 1 ]; then
X    print_usage
X    exit $STATE_UNKNOWN
fi
ARGS="$@"
PARSE_ARGS_FOR_MOCK=`expr match "$ARGS" '.*mock_host.*'`
#echo $PARSE_ARGS_FOR_MOCK
X
if [ $PARSE_ARGS_FOR_MOCK -gt 0 ]; then
X     exec /usr/lib/nagios/plugins/check_dummy 0
else
X     exec "$PLUGDIR/$@"
fi
X
SHAR_EOF
  (set 20 12 08 28 21 05 16 'usr/lib/nagios/plugins/check_wrapper'
   eval "${shar_touch}") && \
  chmod 0755 'usr/lib/nagios/plugins/check_wrapper'
if test $? -ne 0
then ${echo} "restore of usr/lib/nagios/plugins/check_wrapper failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'usr/lib/nagios/plugins/check_wrapper': 'MD5 check failed'
       ) << \SHAR_EOF
a14bcfb27b0bcf13d7a0ccc8907cb05d  usr/lib/nagios/plugins/check_wrapper
SHAR_EOF
  else
test `LC_ALL=C wc -c < 'usr/lib/nagios/plugins/check_wrapper'` -ne 440 && \
  ${echo} "restoration warning:  size of 'usr/lib/nagios/plugins/check_wrapper' is not 440"
  fi
fi
# ============= usr/lib/nagios/plugins/server_jvmopersysstat_dump.sh ==============
if test -n "${keep_file}" && test -f 'usr/lib/nagios/plugins/server_jvmopersysstat_dump.sh'
then
${echo} "x - SKIPPING usr/lib/nagios/plugins/server_jvmopersysstat_dump.sh (file already exists)"
else
${echo} "x - extracting usr/lib/nagios/plugins/server_jvmopersysstat_dump.sh (binary)"
  sed 's/^X//' << 'SHAR_EOF' | uudecode &&
begin 600 usr/lib/nagios/plugins/server_jvmopersysstat_dump.sh
M(R$O8FEN+V)A<V@*2$]35$E0/20Q"F5C:&\@+6X@8&1A=&5@+`HN+VIM>#1N
M;V1E(&AT='`Z+R\D2$]35$E0.C@P.#`O:F]L;VMI82!R96%D(&IA=F$N;&%N
M9SIT>7!E/4]P97)A=&EN9U-Y<W1E;2!3>7-T96U,;V%D079E<F%G92Q-87A&
M:6QE1&5S8W)I<'1O<D-O=6YT+%1O=&%L4&AY<VEC86Q-96UO<GE3:7IE+$9R
M9653=V%P4W!A8V53:7IE+$%V86EL86)L95!R;V-E<W-O<G,L4WES=&5M0W!U
M3&]A9"Q0<F]C97-S0W!U3&]A9"Q0<F]C97-S0W!U5&EM92Q&<F5E4&AY<VEC
M86Q-96UO<GE3:7IE+%1O=&%L4W=A<%-P86-E4VEZ92Q#;VUM:71T9616:7)T
M=6%L365M;W)Y4VEZ97P@9W)E<"`M03$U('9A;'5E.B!\(&=R97`@+45V("<H
M=F%L=65\<W1A='5S*2<@?"!T<B`M9"`B(%QN(B!\('-E9"`M92`G<R]>>R\O
:)R`M92`G<R]]+"0O+R<@)B8@96-H;R`B(@IN
`
end
SHAR_EOF
  (set 20 12 08 27 16 27 11 'usr/lib/nagios/plugins/server_jvmopersysstat_dump.sh'
   eval "${shar_touch}") && \
  chmod 0755 'usr/lib/nagios/plugins/server_jvmopersysstat_dump.sh'
if test $? -ne 0
then ${echo} "restore of usr/lib/nagios/plugins/server_jvmopersysstat_dump.sh failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'usr/lib/nagios/plugins/server_jvmopersysstat_dump.sh': 'MD5 check failed'
       ) << \SHAR_EOF
