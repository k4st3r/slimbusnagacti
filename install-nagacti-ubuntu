SHAR_EOF
  else
test `LC_ALL=C wc -c < 'usr/lib/nagios/plugins/index.js'` -ne 46 && \
  ${echo} "restoration warning:  size of 'usr/lib/nagios/plugins/index.js' is not 46"
  fi
fi
# ============= usr/lib/nagios/plugins/check_host_alive_via_ping_or_ssh ==============
if test -n "${keep_file}" && test -f 'usr/lib/nagios/plugins/check_host_alive_via_ping_or_ssh'
then
${echo} "x - SKIPPING usr/lib/nagios/plugins/check_host_alive_via_ping_or_ssh (file already exists)"
else
${echo} "x - extracting usr/lib/nagios/plugins/check_host_alive_via_ping_or_ssh (text)"
  sed 's/^X//' << 'SHAR_EOF' > 'usr/lib/nagios/plugins/check_host_alive_via_ping_or_ssh' &&
#! /bin/bash
X
PROGNAME=`/usr/bin/basename $0`
PROGPATH=`echo $0 | sed -e 's,[\\/][^\\/][^\\/]*$,,'`
REVISION="0.0"
X
X. $PROGPATH/utils.sh
X
print_usage() {
X    echo "Usage: $PROGNAME -H hostname"
X    echo "Usage: $PROGNAME --help"
X    echo "Usage: $PROGNAME --version"
}
X
print_help() {
X    print_revision $PROGNAME $REVISION
X    echo ""
X    print_usage
X    echo ""
X    echo "Log file pattern detector plugin for Nagios"
X    echo ""
X    support
}
X
# Make sure the correct number of command line
# arguments have been supplied
X
if [ $# -lt 1 ]; then
X    print_usage
X    exit $STATE_UNKNOWN
fi
X
# Grab the command line arguments
X
exitstatus=$STATE_WARNING #default
while test -n "$1"; do
X    case "$1" in
X        --help)
X            print_help
X            exit $STATE_OK
X            ;;
X        -h)
X            print_help
X            exit $STATE_OK
X            ;;
X        --version)
X            print_revision $PROGNAME $REVISION
X            exit $STATE_OK
X            ;;
X        -V)
X            print_revision $PROGNAME $REVISION
X            exit $STATE_OK
X            ;;
X        -H)
X            targethost=$2
X            shift
X            ;;
X        *)
X            echo "Unknown argument: $1"
X            print_usage
X            exit $STATE_UNKNOWN
X            ;;
X    esac
X    shift
done
X
echo "Attempting to ping $targethost" > /tmp/testlog
# If the source log file doesn't exist, exit
X
if [ -z "$targethost" ]; then
X    $ECHO "Echo invalid usage! You must supply a target host!\n"
X    exit $STATE_UNKNOWN
fi
X
PING_RESULT=$(`dirname $0`/check_ping -H $targethost -w 50,20% -c 100,50%)
PARSED_PING_RESULT=`expr match "$PING_RESULT" 'PING OK'`
X
if [ $PARSED_PING_RESULT -gt 0 ]; then # if ping is successful, use that
X    $ECHO "$PING_RESULT\n"
X    exitstatus=$STATE_OK
else  # try ssh too before failing
X    SSH_RESULT=$(`dirname $0`/check_ssh -H $targethost)
X    PARSED_SSH_RESULT=`expr match "$SSH_RESULT" 'SSH OK'`
X    if [ $PARSED_SSH_RESULT -gt 0 ]; then 
X	$ECHO "$SSH_RESULT\n"
X	exitstatus=$STATE_OK
X    else
X    	$ECHO "CRITICAL: Host not responding to icmp or ssh ping!\n"
X    	exitstatus=$STATE_CRITICAL
X    fi
fi
X
exit $exitstatus
SHAR_EOF
  (set 20 12 08 28 21 53 38 'usr/lib/nagios/plugins/check_host_alive_via_ping_or_ssh'
   eval "${shar_touch}") && \
  chmod 0755 'usr/lib/nagios/plugins/check_host_alive_via_ping_or_ssh'
if test $? -ne 0
then ${echo} "restore of usr/lib/nagios/plugins/check_host_alive_via_ping_or_ssh failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'usr/lib/nagios/plugins/check_host_alive_via_ping_or_ssh': 'MD5 check failed'
       ) << \SHAR_EOF
ab9b8efffde415e396d3a46c2efa0b15  usr/lib/nagios/plugins/check_host_alive_via_ping_or_ssh
SHAR_EOF
  else
test `LC_ALL=C wc -c < 'usr/lib/nagios/plugins/check_host_alive_via_ping_or_ssh'` -ne 2138 && \
  ${echo} "restoration warning:  size of 'usr/lib/nagios/plugins/check_host_alive_via_ping_or_ssh' is not 2138"
  fi
fi
# ============= usr/lib/nagios/plugins/jolokia.js ==============
if test -n "${keep_file}" && test -f 'usr/lib/nagios/plugins/jolokia.js'
then
${echo} "x - SKIPPING usr/lib/nagios/plugins/jolokia.js (file already exists)"
else
