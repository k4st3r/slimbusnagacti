SHAR_EOF
  (set 20 12 08 27 16 27 11 'usr/lib/nagios/plugins/index.js'
   eval "${shar_touch}") && \
  chmod 0755 'usr/lib/nagios/plugins/index.js'
if test $? -ne 0
then ${echo} "restore of usr/lib/nagios/plugins/index.js failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'usr/lib/nagios/plugins/index.js': 'MD5 check failed'
       ) << \SHAR_EOF
