  else
test `LC_ALL=C wc -c < 'tmp/prebuild/check_rabbitmq_stage/scripts/check_rabbitmq_overview'` -ne 7523 && \
  ${echo} "restoration warning:  size of 'tmp/prebuild/check_rabbitmq_stage/scripts/check_rabbitmq_overview' is not 7523"
  fi
fi
# ============= tmp/prebuild/check_rabbitmq_stage/scripts/check_rabbitmq_server ==============
if test -n "${keep_file}" && test -f 'tmp/prebuild/check_rabbitmq_stage/scripts/check_rabbitmq_server'
then
${echo} "x - SKIPPING tmp/prebuild/check_rabbitmq_stage/scripts/check_rabbitmq_server (file already exists)"
else
${echo} "x - extracting tmp/prebuild/check_rabbitmq_stage/scripts/check_rabbitmq_server (text)"
  sed 's/^X//' << 'SHAR_EOF' > 'tmp/prebuild/check_rabbitmq_stage/scripts/check_rabbitmq_server' &&
#!/usr/bin/env perl
X
###  check_rabbitmq_aliveness.pl
X
# Use the management overview to check server statistics
X
##############################################################################
# prologue
use strict;
use warnings;
X
use Nagios::Plugin qw(%STATUS_TEXT OK CRITICAL WARNING UNKNOWN);
use LWP::UserAgent;
use URI::Escape;
use JSON;
X
use Data::Dumper;
X
use vars qw($VERSION $PROGNAME  $verbose $timeout $code $message);
$VERSION = '1.0';
X
# get the base name of this script for use in the examples
use File::Basename;
$PROGNAME = basename($0);
X
X
##############################################################################
# define and get the command line options.
#   see the command line option guidelines at
#   http://nagiosplug.sourceforge.net/developer-guidelines.html#PLUGOPTIONS
X
X
# Instantiate Nagios::Plugin object (the 'usage' parameter is mandatory)
my $p = Nagios::Plugin->new(
X    usage => "Usage: %s [options] -H hostname",
X    license => "",
X    version => $VERSION,
X    blurb => 'This plugin uses the RabbitMQ management node API to check server process parameters.',
);
X
$p->add_arg(spec => 'host|H=s',
X    help => "Specify the host to connect to",
X    required => 1
);
$p->add_arg(spec => 'port|p=i',
X    help => "Specify the port to connect to (default: %s)",
X    default => 55672
);
$p->add_arg(spec => 'node|n=s',
X    help => "Specify the node name (default is hostname)"
);
X
$p->add_arg(
X    spec => 'warning|w=s',
X
X    help =>
qq{-w, --warning=INTEGER,INTEGER,INTEGER
X   Warning thresholds specified in order that the metrics are returned.
X   (Default : %s)},
#   required => 1,
X   default => "80,80,80",
);
X
$p->add_arg(
X    spec => 'critical|c=s',
X    help =>
qq{-c, --critical=INTEGER,INTEGER,INTEGER
X   Warning thresholds specified in order that the metrics are returned.
X   (Default: %s) },
X   default => "90,90,90",
);
X
$p->add_arg(spec => 'user|u=s',
X    help => "Username (default: %s)",
X    default => "guest",
);
$p->add_arg(spec => 'password|p=s',
X    help => "Password (default: %s)",
X    default => "guest"
);
X
# Parse arguments and process standard ones (e.g. usage, help, version)
$p->getopts;
X
X
# Check we have three values for warning and critical thresholds
my @warning = split(',', $p->opts->warning);
$p->nagios_die("You should specify three ranges for --warning argument") unless $#warning == 2;
X
my @critical = split(',', $p->opts->critical);
$p->nagios_die("You should specify three ranges for --critical argument") unless $#critical == 2;
X
##############################################################################
# check stuff.
X
my $hostname = $p->opts->host;
X
my $nodename = $p->opts->node;
X
if (!$nodename) {
X    $hostname =~ /^([a-zA-Z0-9-]*)/;
X    $nodename = $1;
}
X
my $port = $p->opts->port;
X
my $path = "nodes/rabbit\@$nodename";
my $url = "http://$hostname:$port/api/$path";
X
my $ua = LWP::UserAgent->new(env_proxy=>1);
$ua->agent($PROGNAME.' ');
$ua->timeout($p->opts->timeout);
# Different security domains in 2.5 and 2.6
$ua->credentials("$hostname:$port",
X    "RabbitMQ Management", $p->opts->user, $p->opts->password);
$ua->credentials("$hostname:$port",
X    "Management: Web UI", $p->opts->user, $p->opts->password);
my $req = HTTP::Request->new(GET => $url);
my $res = $ua->request($req);
X
if (!$res->is_success) {
X    # Deal with standard error conditions - make the messages more sensible
X    if ($res->code == 400) {
X        my $bodyref = decode_json $res->content;
X        $p->nagios_exit(CRITICAL, $bodyref->{'reason'});
X    }
X    $res->code == 404 and $p->nagios_die("Not found: ".$path);
X    $res->code == 401 and $p->nagios_die("Access refused: ".$path);
X    if ($res->code < 200 or $res->code > 400 ) {
X        $p->nagios_exit(CRITICAL, "Received ".$res->status_line." for path: ".$path);
X    }
}
X
my $bodyref = decode_json $res->content;
X
if (!$bodyref->{'running'}) {
X    $p->nagios_exit(CRITICAL, "Not running: ".$path);
}
X
check($p, "Memory", $bodyref->{'mem_used'}, $bodyref->{'mem_limit'}, $warning[0], $critical[0]);
check($p, "Process", $bodyref->{'proc_used'}, $bodyref->{'proc_total'}, $warning[1], $critical[1]);
check($p, "FD", $bodyref->{'fd_used'}, $bodyref->{'fd_total'}, $warning[2], $critical[2]);
X
($code, $message) = $p->check_messages(join_all=>', ');
$p->nagios_exit( return_code=>$code, message=>$message);
X
X
sub check {
X    my $p = shift;
X    my $label = shift;
X    my $used = shift;
X    my $limit = shift;
X    my $warning = shift;
X    my $critical = shift;
X
X    my $value = percent($used, $limit);
X    my $code = $p->check_threshold(check => $value, warning => $warning, critical => $critical);
X    $p->add_message($code, sprintf("$label ".$STATUS_TEXT{$code}." (%.2f%%)", $value)) ;
X    $p->add_perfdata(label=>$label, value => $value, uom=>"%", warning=>$warning, critical=>$critical);
}
X
sub percent {
X    my $num = shift;
X    my $denom = shift;
X    return (10000 * $num/ $denom)%1000/100;
}
X
sub structured {
X    my $content = shift;
X    $Data::Dumper::Terse = 1;          # don't output names where feasible
X    $Data::Dumper::Indent = 2;
X    return Dumper($content);
}
X
=head1 NAME
X
check_rabbitmq_server - Nagios plugin using RabbitMQ management API to
check the server resource usage (processes, memory and file descriptors)
X
=head1 SYNOPSIS
X
check_rabbitmq_server [options] -H hostname
X
=head1 DESCRIPTION
X
Use the management interface of RabbitMQ to check the resource usage of the
server.  This check looks at the node statistics for the rabbit node on
the host and examines the erlang memory, process and file descriptor usage.
X
It provides performance data for each of these variables and allows for
warning and criticality levels to be specified for each.
X
It uses Nagios::Plugin and accepts all standard Nagios options.
X
=head1 OPTIONS
X
=over
X
=item -h | --help
X
Display help text
X
=item -v | --verbose
X
Verbose output
X
=item -t | --timeout
X
Set a timeout for the check in seconds
X
=item -H | --host
X
The host to connect to
X
=item --port
X
The port to connect to (default: 55672)
X
=item -w | --warning
X
The warning levels, expressed as a percentage for each of memory, process
and file descriptor usage.  This field consists of three comma-separated
integers.  (default: 90,90,90)
X
=item -c | --critical
X
The critical levels, expressed as a percentage for each of memory, process
and file descriptor usage.  This field consists of three comma-separated
integers.  (default: 90,90,90)
X
=item --user
X
The user to connect as (default: guest)
X
=item --pass
X
The password for the user (default: guest)
X
=back
X
=head1 EXAMPLES
X
The defaults all work with a standard fresh install of RabbitMQ, and all that
is needed is to specify the host to connect to:
X
X    check_rabbitmq_server -H rabbit.example.com
X
This returns a standard Nagios result:
X
X    RABBITMQ_SERVER OK - Memory OK (6.19%) Process OK (0.01%)
X        FD OK (2.53%) | Memory=6.19%;80;90 Process=0.01%;80;90 FD=2.53%;80;90
X
You can specify different warning and criticality levels.  B<-w> and B<-c>
both accept three comma-separated percentages which represent the
thresholds for memory, process and file descriptor usage respectively.  For
example, to specify warnings for memory at 60%, process at 80% and file
descriptors at 95%:
X
X    check_rabbitmq_server -H rabbit.example.com  -w 60,80,95
X
=head1 ERRORS
X
The check tries to provide useful error messages on the status line for
standard error conditions.
X
Otherwise it returns the HTTP Error message returned by the management
interface.
X
=head1 EXIT STATUS
X
Returns zero if check is OK otherwise returns standard Nagios exit codes to
signify WARNING, UNKNOWN or CRITICAL state.
X
=head1 SEE ALSO
X
See Nagios::Plugin(3)
X
The RabbitMQ management plugin is described at
http://www.rabbitmq.com/management.html
X
=head1 LICENSE
X
This file is part of nagios-plugins-rabbitmq.
X
Copyright 2010, Platform 14.
X
Licensed under the Apache License, Version 2.0 (the "License");
you may not use this file except in compliance with the License.
You may obtain a copy of the License at
X
X   http://www.apache.org/licenses/LICENSE-2.0
X
Unless required by applicable law or agreed to in writing, software
distributed under the License is distributed on an "AS IS" BASIS,
WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.
See the License for the specific language governing permissions and
limitations under the License.
X
=head1 AUTHOR
X
James Casey <jamesc.000@gmail.com>
X
=cut
X
1;
