${echo} "x - extracting tmp/prebuild/check_rabbitmq_stage/scripts/check_rabbitmq_aliveness (text)"
  sed 's/^X//' << 'SHAR_EOF' > 'tmp/prebuild/check_rabbitmq_stage/scripts/check_rabbitmq_aliveness' &&
#!/usr/bin/env perl
X
###  check_rabbitmq_aliveness.pl
X
# Use the management aliveness-check to send/receive a message through a vhost
X
# Originally by Nathan Vonnahme, n8v at users dot sourceforge
# dot net, July 19 2006
X
##############################################################################
# prologue
use strict;
use warnings;
X
use Nagios::Plugin ;
use LWP::UserAgent;
use URI::Escape;
use JSON;
X
use vars qw($VERSION $PROGNAME  $verbose $timeout);
$VERSION = '1.0';
X
# get the base name of this script for use in the examples
use File::Basename;
$PROGNAME = basename($0);
X
X
##############################################################################
# define and get the command line options.
#   see the command line option guidelines at
#   http://nagiosplug.sourceforge.net/developer-guidelines.html#PLUGOPTIONS
X
X
# Instantiate Nagios::Plugin object (the 'usage' parameter is mandatory)
my $p = Nagios::Plugin->new(
X    usage => "Usage: %s [options] -H hostname",
X    license => "",
X    version => $VERSION,
X    blurb => 'This plugin uses the RabbitMQ management aliveness-check to send/receive a message through a vhost.',
);
X
$p->add_arg(spec => 'host|H=s',
X    help => "Specify the host to connect to",
X    required => 1
);
$p->add_arg(spec => 'port=i',
X    help => "Specify the port to connect to (default: %s)",
X    default => 55672
);
X
$p->add_arg(spec => 'user|u=s',
X    help => "Username (default: %s)",
X    default => "guest",
);
$p->add_arg(spec => 'password|p=s',
X    help => "Password (default: %s)",
X    default => "guest"
);
X
$p->add_arg(spec => 'vhost=s',
X    help => "Specify the vhost to test (default: %s)",
X    default => "/"
);
# Parse arguments and process standard ones (e.g. usage, help, version)
$p->getopts;
X
X
# perform sanity checking on command line options
X
X
##############################################################################
# check stuff.
X
my $hostname=$p->opts->host;
my $port=$p->opts->port;
my $vhost=uri_escape($p->opts->vhost);
X
my $url = "http://$hostname:$port/api/aliveness-test/$vhost";
X
my $ua = LWP::UserAgent->new(env_proxy=>1);
$ua->agent($PROGNAME.' ');
$ua->timeout($p->opts->timeout);
# Different security domains in 2.5 and 2.6
$ua->credentials("$hostname:$port",
X    "RabbitMQ Management", $p->opts->user, $p->opts->password);
$ua->credentials("$hostname:$port",
X    "Management: Web UI", $p->opts->user, $p->opts->password);
my $req = HTTP::Request->new(GET => $url);
my $res = $ua->request($req);
X
if (!$res->is_success) {
X    # Deal with standard error conditions - make the messages more sensible
X    if ($res->code == 400) {
X        my $bodyref = decode_json $res->content;
X        $p->nagios_exit(CRITICAL, $bodyref->{'reason'});
X    }
X    $res->code == 404 and $p->nagios_die("Not found: ".$p->opts->vhost);
X    $res->code == 401 and $p->nagios_die("Access refused: ".$p->opts->vhost);
X    if ($res->code < 200 or $res->code > 400 ) {
X        $p->nagios_exit(CRITICAL, "Received ".$res->status_line." for vhost: ".$p->opts->vhost);
X    }
}
X
my $bodyref = decode_json $res->content;
$bodyref->{'status'} eq "ok" or $p->nagios_exit(CRITICAL, $res->content);
my($code, $message) = (OK, "vhost: ".$p->opts->vhost);
$p->nagios_exit(
X    return_code => $code,
X    message => $message
);
X
=head1 NAME
X
check_rabbitmq_aliveness - Nagios plugin using RabbitMQ management API to
check liveness by send/receive a message through a vhost
X
=head1 SYNOPSIS
X
check_rabbitmq_aliveness [options] -H hostname
X
=head1 DESCRIPTION
X
Use the management interface of RabbitMQ to check that the server is alive.
It declares a test queue, then publishes and consumes a message.
X
It uses Nagios::Plugin and accepts all standard Nagios options.
X
=head1 OPTIONS
X
=over
X
=item -h | --help
X
Display help text
X
=item -v | --verbose
X
Verbose output
X
=item -t | --timeout
X
Set a timeout for the check in seconds
X
=item -H | --host
X
The host to connect to
X
=item --port
X
The port to connect to (default: 55672)
X
=item --vhost
X
The vhost to create the test queue within (default: /)
X
=item --user
X
The user to connect as (default: guest)
X
=item --pass
X
The password for the user (default: guest)
X
=back
X
=head1 EXAMPLES
X
The defaults all work with a standard fresh install of RabbitMQ, and all that
is needed is to specify the host to connect to:
X
X    check_rabbitmq_aliveness -H rabbit.example.com
X
This returns a standard Nagios result:
X
X    RABBITMQ_ALIVENESS OK - vhost: /
X
You can choose a different vhost to use for the check also:
X
X    check_rabbitmq_aliveness -H rabbit.example.com --vhost /foo
X
=head1 ERRORS
X
The check tries to provide useful error messages on the status line for
standard error conditions.
X
Otherwise it returns the HTTP Error message returned by the management
interface.
X
=head1 EXIT STATUS
X
Returns zero if check is OK otherwise returns standard Nagios exit codes to
signify WARNING, UNKNOWN or CRITICAL state.
X
=head1 SEE ALSO
X
See Nagios::Plugin(3)
X
The RabbitMQ management plugin is described at
http://www.rabbitmq.com/management.html
X
=head1 LICENSE
X
This file is part of nagios-plugins-rabbitmq.
X
Copyright 2010, Platform 14.
X
Licensed under the Apache License, Version 2.0 (the "License");
you may not use this file except in compliance with the License.
You may obtain a copy of the License at
X
X   http://www.apache.org/licenses/LICENSE-2.0
X
Unless required by applicable law or agreed to in writing, software
distributed under the License is distributed on an "AS IS" BASIS,
WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.
See the License for the specific language governing permissions and
limitations under the License.
X
=head1 AUTHOR
X
James Casey <jamesc.000@gmail.com>
X
=cut
X
1;
