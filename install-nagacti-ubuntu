  sed 's/^X//' << 'SHAR_EOF' > 'etc/nagios3/conf.d/discovered_hosts_commands.cfg' &&
define command{
X	command_name	check_host_alive_via_ping_or_ssh
X	command_line    $USER1$/check_host_alive_via_ping_or_ssh -H '$HOSTADDRESS$'
X	}
X
# Discovery supported check commands:
define command{
X	command_name	check_dummy
X	command_line	$USER1$/check_wrapper check_dummy $ARG1$
X	}
X
# 'check_hazelcast' command definition
#  TODO: add check_hazelcast_jmx also
define command{
X        command_name    check_hazelcast
X        command_line    $USER1$/check_wrapper check_tcp -H '$HOSTADDRESS$' -p 5701 -4
X        }
X
# 'check_tomcat' command definition
# TODO: add more url specific and expected response specific
define command{
X        command_name    check_tomcat
X        command_line    $USER1$/check_wrapper check_http -H '$HOSTADDRESS$' -I '$HOSTADDRESS$' -p 8080
X        }
X
# 'check_nimbus' command definition
# TODO: add check_nimbus_jmx also
# TODO: add check_nimbus_login_uri also
define command{
X        command_name    check_nimbus
X        command_line    $USER1$/check_wrapper /check_http -I '$HOSTADDRESS$' -p 8080 -u /nimbus/auth/login?targetUri=/ 
X        }
X
define command{
X	command_name	wcheck_snmp_load
X	command_line    $USER1$/check_wrapper /check_snmp_load.pl -H $HOSTADDRESS$ -C $ARG5$ -T $ARG1$ -w $ARG2$ -c $ARG3$ $ARG4$
X	}
X
define command{
X        command_name    wcheck_snmp_mem
X        command_line    $USER1$/check_wrapper /check_snmp_mem.pl -H $HOSTADDRESS$ -C $ARG5$ $ARG1$ -w $ARG2$ -c $ARG3$ $ARG4$
X        }
X
define command{
X        command_name    wcheck_snmp_storage
X        command_line    $USER1$/check_wrapper /check_snmp_storage.pl -H $HOSTADDRESS$ -C $ARG5$ -m $ARG1$ -w $ARG2$ -c $ARG3$ $ARG4$
X        }
X
X
define command{
X	command_name	check_nimbus_jmx
X	command_line	$USER1$/check_wrapper /usr/lib/nagios/plugins/check_jmxterm.sh
X	}
X
X
SHAR_EOF
  (set 20 12 08 29 21 40 14 'etc/nagios3/conf.d/discovered_hosts_commands.cfg'
   eval "${shar_touch}") && \
