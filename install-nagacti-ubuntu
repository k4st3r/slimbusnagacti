  mkdir 'tmp/prebuild/check_rabbitmq_stage/scripts'
if test $? -eq 0
then ${echo} "x - created directory tmp/prebuild/check_rabbitmq_stage/scripts."
else ${echo} "x - failed to create directory tmp/prebuild/check_rabbitmq_stage/scripts."
     exit 1
fi
fi
if test -n "${keep_file}" && test -f 'tmp/prebuild/check_rabbitmq_stage/scripts/check_rabbitmq_queue'
then
${echo} "x - SKIPPING tmp/prebuild/check_rabbitmq_stage/scripts/check_rabbitmq_queue (file already exists)"
else
${echo} "x - extracting tmp/prebuild/check_rabbitmq_stage/scripts/check_rabbitmq_queue (text)"
  sed 's/^X//' << 'SHAR_EOF' > 'tmp/prebuild/check_rabbitmq_stage/scripts/check_rabbitmq_queue' &&
#!/usr/bin/env perl
#
# check_rabbitmq_overview
#
# Use the management APIs to check a queue
#
use strict;
use warnings;
X
use Nagios::Plugin qw(%STATUS_TEXT OK CRITICAL WARNING UNKNOWN);
use LWP::UserAgent;
use URI::Escape;
use JSON;
X
use vars qw($VERSION $PROGNAME  $verbose $timeout);
$VERSION = '1.0';
X
# get the base name of this script for use in the examples
use File::Basename;
$PROGNAME = basename($0);
X
my $p = Nagios::Plugin->new(
X    usage => "Usage: %s [options] -H hostname",
X    license => "",
X    version => $VERSION,
X    blurb => 'This plugin uses the RabbitMQ management API to monitor a specific queue.',
);
X
$p->add_arg(spec => 'host|H=s',
X    help => "Specify the host to connect to",
X    required => 1
);
$p->add_arg(spec => 'port=i',
X    help => "Specify the port to connect to (default: %s)",
X    default => 55672
);
X
$p->add_arg(spec => 'user|u=s',
X    help => "Username (default: %s)",
X    default => "guest",
);
$p->add_arg(spec => 'password|p=s',
X    help => "Password (default: %s)",
X    default => "guest"
);
X
$p->add_arg(spec => 'vhost=s',
X    help => "Specify the vhost where the queue resides (default: %s)",
X    default => "/"
);
$p->add_arg(spec => 'queue=s',
X    help => "Specify the queue to check",
X    required => 1
);
X
X
$p->add_arg(
X    spec => 'warning|w=s',
X    help =>
qq{-w, --warning=INTEGER,INTEGER,INTEGER
X   Warning thresholds specified in order that the metrics are returned.
X   Specify -1 if no warning threshold.},
X
);
X
$p->add_arg(
X    spec => 'critical|c=s',
X    help =>
qq{-c, --critical=INTEGER,INTEGER,INTEGER
X   Critical thresholds specified in order that the metrics are returned.
X   Specify -1 if no critical threshold.},
);
X
# Parse arguments and process standard ones (e.g. usage, help, version)
$p->getopts;
X
X
# perform sanity checking on command line options
my %warning;
if (defined $p->opts->warning) {
X    my @warning = split(',', $p->opts->warning);
X    $p->nagios_die("You should specify three ranges for --warning argument") unless $#warning == 2;
X
X    $warning{'messages'} = shift @warning;
X    $warning{'messages_ready'} = shift @warning;
X    $warning{'messages_unacknowledged'} = shift @warning;
}
X
my %critical;
if (defined $p->opts->critical) {
X    my @critical = split(',', $p->opts->critical);
X    $p->nagios_die("You should specify three ranges for --critical argument") unless $#critical == 2;
X
X    $critical{'messages'} = shift @critical;
X    $critical{'messages_ready'} = shift @critical;
X    $critical{'messages_unacknowledged'} = shift @critical;
}
X
X
##############################################################################
# check stuff.
X
my $hostname=$p->opts->host;
my $port=$p->opts->port;
my $vhost=uri_escape($p->opts->vhost);
my $queue=$p->opts->queue;
X
my $ua = LWP::UserAgent->new(env_proxy=>1);
$ua->agent($PROGNAME.' ');
$ua->timeout($p->opts->timeout);
# Different security domains in 2.5 and 2.6
$ua->credentials("$hostname:$port",
X    "Management: Web UI", $p->opts->user, $p->opts->password);
$ua->credentials("$hostname:$port",
X    "RabbitMQ Management", $p->opts->user, $p->opts->password);
X
X
my $url = sprintf("http://%s:%d/api/queues/%s/%s", $hostname, $port, $vhost, $queue);
my ($retcode, $result) = request($url);
if ($retcode != 200) {
X    $p->nagios_exit(CRITICAL, "$result : $url");
}
X
my @metrics = ("messages", "messages_ready", "messages_unacknowledged");
for my $metric (@metrics) {
X    my $warning = undef;
X    $warning = $warning{$metric} if (defined $warning{$metric} and $warning{$metric} != -1);
X    my $critical = undef;
X    $critical = $critical{$metric} if (defined $critical{$metric} and $critical{$metric} != -1);
X
X    my $value = $result->{$metric};
X    my $code = $p->check_threshold(check => $value, warning => $warning, critical=> $critical);
X    $p->add_message($code, sprintf("$metric ".$STATUS_TEXT{$code}." (%d)", $value)) ;
X    $p->add_perfdata(label=>$metric, value => $value, warning=>$warning, critical=> $critical);
}
X
my ($code, $message) = $p->check_messages(join_all=>', ');
$p->nagios_exit(return_code => $code, message => $message);
X
X
sub request {
X    my ($url) = @_;
X    my $req = HTTP::Request->new(GET => $url);
X    my $res = $ua->request($req);
X
X    if (!$res->is_success) {
X        # Deal with standard error conditions - make the messages more sensible
X        if ($res->code == 400) {
X            my $bodyref = decode_json $res->content;
X            return (400, $bodyref->{'reason'});
X
X        }
X        $res->code == 404 and return (404, "Not Found");
X        $res->code == 401 and return (401, "Access Refused");
X        $res->status_line =~ /Can\'t connect/ and return (500, "Connection Refused : $url");
X        if ($res->code < 200 or $res->code > 400 ) {
X            return ($res->code, "Received ".$res->status_line);
X        }
X    }
X    my $bodyref = decode_json $res->content;
X    return($res->code, $bodyref);
}
X
=head1 NAME
X
check_rabbitmq_queue - Nagios plugin using RabbitMQ management API to
count the messages pending on a given queue
X
=head1 SYNOPSIS
X
check_rabbitmq_overview [options] -H hostname
X
=head1 DESCRIPTION
X
Use the management interface of RabbitMQ to count the number of pending,
ready and unacknowledged messages.  These are published as performance
metrics for the check.
X
Critical and warning thresholds can be set for each of the metrics.
X
It uses Nagios::Plugin and accepts all standard Nagios options.
X
=head1 OPTIONS
X
=over
X
=item -h | --help
X
Display help text
X
=item -v | --verbose
X
Verbose output
X
=item -t | --timeout
X
Set a timeout for the check in seconds
X
=item -H | --host
X
The host to connect to
X
=item --port
X
The port to connect to (default: 55672)
X
=item --user
X
The user to connect as (default: guest)
X
=item --pass
X
The password for the user (default: guest)
X
=item -w | --warning
X
The warning levels for each count of messages, messages_ready and
messages_unacknowledged.  This field consists of three comma-separated
integers.  Specify -1 if no threshold for a particular count.
X
=item -c | --critical
X
The critical levels for each count of messages, messages_ready and
messages_unacknowledged.  This field consists of three comma-separated
integers.  Specify -1 if no threshold for a particular count
X
=back
X
=head1 EXAMPLES
X
The defaults all work with a standard fresh install of RabbitMQ, and all that
is needed is to specify the host to connect to:
X
X    check_rabbitmq_overview -H rabbit.example.com
X
This returns a standard Nagios result:
X
X    RABBITMQ_OVERVIEW OK - messages OK (25794) messages_ready OK (22971)
X      messages_unacknowledged OK (2823) | messages=25794;;
X      messages_ready=22971;; messages_unacknowledged=2823;;
X
=head1 ERRORS
X
The check tries to provide useful error messages on the status line for
standard error conditions.
X
Otherwise it returns the HTTP Error message returned by the management
interface.
X
=head1 EXIT STATUS
X
Returns zero if check is OK otherwise returns standard Nagios exit codes to
signify WARNING, UNKNOWN or CRITICAL state.
X
=head1 SEE ALSO
X
See Nagios::Plugin(3)
X
The RabbitMQ management plugin is described at
http://www.rabbitmq.com/management.html
X
=head1 LICENSE
X
This file is part of nagios-plugins-rabbitmq.
X
Copyright 2010, Platform 14.
X
Licensed under the Apache License, Version 2.0 (the "License");
you may not use this file except in compliance with the License.
You may obtain a copy of the License at
X
X   http://www.apache.org/licenses/LICENSE-2.0
X
Unless required by applicable law or agreed to in writing, software
distributed under the License is distributed on an "AS IS" BASIS,
WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.
See the License for the specific language governing permissions and
limitations under the License.
X
=head1 AUTHOR
X
James Casey <jamesc.000@gmail.com>
X
=cut
