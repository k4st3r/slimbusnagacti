X        failure_prediction_enabled      1       ; Failure prediction is enabled
X        process_perf_data               1       ; Process performance data
X        retain_status_information       1       ; Retain status information across program restarts
X        retain_nonstatus_information    1       ; Retain non-status information across program restarts
X		check_command                   check_host_alive_via_ping_or_ssh
X		max_check_attempts              10
X		notification_interval           0
X		notification_period             24x7
X		notification_options            d,u,r
X		contact_groups                  admins
X        register                        0       ; DONT REGISTER THIS DEFINITION - ITS NOT A REAL HOST, JUST A TEMPLATE!
X        }
SHAR_EOF
