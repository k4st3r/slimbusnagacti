# ============= usr/lib/nagios/plugins/jolokia-client.js ==============
if test -n "${keep_file}" && test -f 'usr/lib/nagios/plugins/jolokia-client.js'
then
${echo} "x - SKIPPING usr/lib/nagios/plugins/jolokia-client.js (file already exists)"
else
${echo} "x - extracting usr/lib/nagios/plugins/jolokia-client.js (text)"
  sed 's/^X//' << 'SHAR_EOF' > 'usr/lib/nagios/plugins/jolokia-client.js' &&
/*
X * Copyright 2011 jolira
X * 
X * Licensed under the Apache License, Version 2.0 (the "License"); you may not
X * use this file except in compliance with the License. You may obtain a copy of
X * the License at http://www.apache.org/licenses/LICENSE-2.0. Unless required by
X * applicable law or agreed to in writing, software distributed under the
X * License is distributed on an "AS IS" BASIS, WITHOUT WARRANTIES OR CONDITIONS
X * OF ANY KIND, either express or implied. See the License for the specific
X * language governing permissions and limitations under the License.
X */
X
var http = require('http');
var https = require('https');
var url = require('url');
var _ = require("underscore");
X
// Private
// ----------------------------------------------------------------------------
X
function extractEncoding(headers) {
X    var type = headers["content-type"];
X
X    if (!type) {
X        return "utf8";
X    }
X
X    var split = type.split('=');
X
X    return split.length == 2 ? split[1] : "utf8";
}
X
function requestJSON(url, content, callback) {
X    var client = url.protocol == 'https:' ? https : http;
X    var path = url.pathname;
X
X    if (url.search) {
X        path += url.search;
X    }
X
X    var request = client.request({
X        method : "POST",
X        host : url.hostname,
X        port : url.port,
X        path : path
X    }, function(response) {
X        if (response.statusCode != 200) {
X            callback(undefined, response);
X            return;
X        }
X
X        var body = "";
X        var endcoding = extractEncoding(response.headers);
X
X        response.setEncoding(endcoding);
X        response.on('data', function(chunk) {
X            body += chunk;
X        });
X        response.on('end', function() {
X            var parsed = JSON.parse(body);
X
X            callback(parsed);
X        });
X    });
X
X    content["config"] = {
X        "maxObjects" : 100000,
X        "maxCollectionSize" : 10000,
X        "ignoreErrors" : true,
X        "maxDepth" : 100
X    };
X    var _json = JSON.stringify(content);
X
X    request.on('error', function(response) {
X        callback(undefined, response);
X    });
X    request.write(_json);
X    request.end();
}
X
function doList(url, mbean, callback) {
X    requestJSON(url, {
X        "type" : "LIST",
X        "path" : mbean
X    }, function(response) {
X        callback(response);
X    });
}
X
function collectMBeans(json, alreadyFound, scope) {
X    var attr = json["attr"];
X
X    if (attr) {
X        alreadyFound.unshift(scope);
X
X        return alreadyFound;
X    }
X
X    for ( var name in json) {
X        if (name === "op" || name == "desc") {
X            continue;
X        }
X
X        var _scope = scope ? scope + ":" + name : name;
X        var _json = json[name];
X
X        collectMBeans(_json, alreadyFound, _scope);
X    }
X
X    return alreadyFound;
}
X
// Public
// ----------------------------------------------------------------------------
X
function Jolokia(target) {
X    this.url = url.parse(target, false);
}
X
Jolokia.prototype.list = function(arg1, arg2) {
X    var mbeans = arg2 ? arg1 : undefined;
X    var callback = arg2 ? arg2 : arg1;
X
X    if (!_.isArray(mbeans)) {
X        doList(this.url, mbeans, callback);
X        return;
X    }
X
X    if (mbeans.length < 1) {
X        doList(this.url, undefined, callback);
X        return;
X    }
X
X    var result = {};
X
X    for ( var idx in mbeans) {
X        var mbean = mbeans[idx];
X
X        doList(this.url, mbean, function(mbean) {
X            return function(json) {
X                result[mbean] = json;
X
X                var keys = _.keys(result);
X
X                if (keys.length === mbeans.length) {
X                    callback(result);
X                }
X            };
X        }(mbean));
X    }
};
X
Jolokia.prototype.read = function(mbean, arg2, arg3) {
X    var attribute = arg3 ? arg2 : undefined;
X    var callback = arg3 ? arg3 : arg2;
X
X    if (!_.isArray(mbean)) {
X        requestJSON(this.url, {
X            "type" : "read",
X            "mbean" : mbean,
X            "attribute" : attribute
X        }, function(response) {
X            callback(response);
X        });
X        return;
X    }
X
X    var content = [];
X
X    for ( var idx in mbean) {
X        var bean = mbean[idx];
X
X        content[idx] = _.isString(bean) ? {
X            "type" : "read",
X            "mbean" : bean,
X            "attribute" : attribute
X        } : {
X            "type" : "read",
X            "mbean" : bean.mbean,
X            "attribute" : bean.attribute ? bean.attribute : attribute
X        };
X    }
X
X    requestJSON(this.url, content, function(response) {
X        callback(response);
X    });
};
X
Jolokia.prototype.dump = function(mbean, callback) {
X    var self = this;
X
X    this.list(mbean, function(json) {
X        var mbeans = collectMBeans(json.value, [], mbean);
X
X        self.read(mbeans, callback);
X    });
};
X
// Exports
// ----------------------------------------------------------------------------
X
module.exports = Jolokia;
