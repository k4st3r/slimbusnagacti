SHAR_EOF
  else
test `LC_ALL=C wc -c < 'usr/lib/nagios/plugins/jquery-1.7.2.js'` -ne 252881 && \
  ${echo} "restoration warning:  size of 'usr/lib/nagios/plugins/jquery-1.7.2.js' is not 252881"
  fi
fi
