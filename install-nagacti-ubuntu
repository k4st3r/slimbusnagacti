X    print_version $PROGNAME $VERSION
X    echo ""
X    echo "$PROGNAME is a Nagios plugin to check if an Erlang node is pingable from the local host."
X    echo ""
X    echo "$PROGNAME -e /usr/bin/erl -b /home/wl/nagios_erlang/ebin/ -n my_server -c my_cookie"
X    echo ""
X    echo "Options:"
X    echo "  -e/--erl       : the absolute path to erl binary (/usr/bin/erl)"
X    echo "  -n/--node      : the node to ping against"
X    echo "  -b/--beam      : the absolute path to directory with nagios_erlang.beam"
X    echo "  -c/--cookie    : the cookie used by node (cookie)"
X    echo "  -v/--verbosity : level of detail, 0-3 (0)"
X    echo "  -V/--version   : version of package"
X    echo "  -h/--help      : show this screen"
}
X
while test -n "$1"; do
X    case "$1" in
X	--help|-h)
X	    print_help
X	    exit $ST_UK
X	    ;;
X	--verbosity|-v)
X	    VERBOSITY=$2
X	    shift
X	    ;;
X	--version|-V)
X	    print_version $PROGNAME $VERSION
X	    exit $ST_UK
X	    ;;
X	--erl|-e)
X	    ERL=$2
X	    shift
X	    ;;
X	--cookie|-c)
X	    COOKIE=$2
X	    shift
X	    ;;
X	--node|-n)
X	    NODE=$2
X	    shift
X	    ;;
X	*)
X	    echo "Uknown argument: $1"
X	    print_help
X	    exit $ST_UK
X	    ;;
X	esac
X    shift
done
CMD="$ERL -pa $BEAM -setcookie $COOKIE -run nagios_erlang check_node $NODE -noshell -name $TMP_NODE"
if [ $VERBOSITY -ge 3 ]
X then
X    echo "version: $VERSION"
X    echo "node: $NODE"
X    echo "cookie: $COOKIE"
X    echo "tmp_node: $TMP_NODE"
X    echo "erl: $ERL"
X    echo "beam: $BEAM"
X    echo "verbosity: $VERBOSITY"
X    echo "full command: $CMD"
fi
$CMD
SHAR_EOF
  (set 20 09 12 30 08 11 04 'tmp/prebuild/check_erlang_stage/check_erlang_node.sh'
   eval "${shar_touch}") && \
  chmod 0755 'tmp/prebuild/check_erlang_stage/check_erlang_node.sh'
if test $? -ne 0
then ${echo} "restore of tmp/prebuild/check_erlang_stage/check_erlang_node.sh failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'tmp/prebuild/check_erlang_stage/check_erlang_node.sh': 'MD5 check failed'
       ) << \SHAR_EOF
c5c1ed637e8c8a964c97b17092c96173  tmp/prebuild/check_erlang_stage/check_erlang_node.sh
SHAR_EOF
  else
test `LC_ALL=C wc -c < 'tmp/prebuild/check_erlang_stage/check_erlang_node.sh'` -ne 3512 && \
  ${echo} "restoration warning:  size of 'tmp/prebuild/check_erlang_stage/check_erlang_node.sh' is not 3512"
  fi
fi
# ============= tmp/prebuild/check_erlang_stage/check_erlang_pg.sh ==============
if test -n "${keep_file}" && test -f 'tmp/prebuild/check_erlang_stage/check_erlang_pg.sh'
then
${echo} "x - SKIPPING tmp/prebuild/check_erlang_stage/check_erlang_pg.sh (file already exists)"
else
${echo} "x - extracting tmp/prebuild/check_erlang_stage/check_erlang_pg.sh (text)"
  sed 's/^X//' << 'SHAR_EOF' > 'tmp/prebuild/check_erlang_stage/check_erlang_pg.sh' &&
#!/bin/sh
#
# ## Overview
#
#   This script is used for checking that a specified process group
#   is populated with a given number of living processes.
#   (Living is defined as erlang:is_process_alive returns true.)
#
#   The script returns OK if more than or equal to --warning processes exist in the process group.
#   The script returns WARN if less than --warning processes exist.
#   The script returns CRITICAL if less than or equal to --critical processes exist, or if process group doesn't exist.
#
# ## Licence
#
#   This program is free software; you can redistribute it and/or modify
#   it under the terms of the GNU General Public License as published by
#   the Free Software Foundation; either version 2 of the License, or
#   (at your option) any later version.
#
#   This program is distributed in the hope that it will be useful,
#   but WITHOUT ANY WARRANTY; without even the implied warranty of
#   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the
#   GNU General Public License for more details.
#
#   You should have received a copy of the GNU General Public License
#   along with this program; if not, write to the Free Software
#   Foundation, Inc., 51 Franklin St, Fifth Floor, Boston, MA  02110-1301  USA
#
# ## Acknowledgements
#
#   This script owes heavily to the [check_nginx.sh][cnsh] script, upon which the Bash components are modeled.
#   [cnsh]: http://exchange.nagios.org/directory/Plugins/Uncategorized/Software/check_nginx-2Esh/details "check_nginx.sh"
#
X
PROGNAME=`basename $0`
VERSION="Version 0.1,"
AUTHOR="2009, Will Larson [http://lethain.com]"
X
ST_OK=0
ST_WR=1
ST_CR=2
ST_UK=3
COOKIE="cookie"                          # cookie used by the local node
NODE="node@localhost"                    # name of node to inspect process group from
TMP_NODE="nagios_check_pg@`hostname`"    # name of temporary node to run code from
ERL="/usr/bin/erl"                       # full path to erlang executable
BEAM="`pwd`/ebin/"                       # full path to directory where nagios_erlang.beam exists
VERBOSITY=0                              # amount of detail to be returned, 0-3
PROCESS_GROUP="unknown"                  # name of process group to investigate
WARNING=0                                # minimum number of procs in PG before warning state
CRITICAL=0                               # minimum number of procs in PG before critical state
X
print_version() {
X    echo "$VERSION $AUTHOR"
}
X
print_help() {
X    print_version $PROGNAME $VERSION
X    echo ""
X    echo "$PROGNAME is a Nagios plugin to check the number of processes in a process group."
X    echo ""
X    echo "$PROGNAME -e /usr/bin/erl -b /home/wl/nagios_erlang/ebin/ -n my_server -c my_cookie"
X    echo "  -w 5 -C 1 -p my_server_group"
X    echo ""
X    echo "Options:"
X    echo "  -e/--erl       : the absolute path to erl binary (/usr/bin/erl)"
X    echo "  -n/--node      : the node whose process groups will be inspected"
X    echo "  -b/--beam      : the absolute path to directory with nagios_erlang.beam"
X    echo "  -w/--warning   : minimum number of nodes before warning issued"
X    echo "  -C/--critical  : minimum number of nodes before critical issued"
X    echo "  -p/--pgroup    : name of process group to inspect"
X    echo "  -c/--cookie    : the cookie used by node (cookie)"
X    echo "  -v/--verbosity : level of detail, 0-3 (0)"
X    echo "  -V/--version   : version of package"
X    echo "  -h/--help      : show this screen"
}
X
while test -n "$1"; do
X    case "$1" in
X	--help|-h)
X	    print_help
X	    exit $ST_UK
X	    ;;
X	--verbosity|-v)	    
X	    VERBOSITY=$2
X	    shift
X	    ;;
X	--pgroup|-p)
X	    PROCESS_GROUP=$2
X	    shift
X	    ;;
X	--warning|-w)
X	    WARNING=$2
X	    shift
X	    ;;
X	--critical|-C)
X	    CRITICAL=$2
X	    shift
X	    ;;
X	--version|-V)
X	    print_version $PROGNAME $VERSION
X	    exit $ST_UK
X	    ;;
X	--erl|-e)
X	    ERL=$2
X	    shift
X	    ;;
X	--cookie|-c)
X	    COOKIE=$2
X	    shift
X	    ;;
X	--node|-n)
X	    NODE=$2
X	    shift
X	    ;;
X	*)
X	    echo "Uknown argument: $1"
X	    print_help
X	    exit $ST_UK
X	    ;;
X	esac
X    shift
done
CMD="$ERL -pa $BEAM -run nagios_erlang check_process_group $NODE $PROCESS_GROUP $WARNING $CRITICAL -noshell -setcookie $COOKIE -name $TMP_NODE"
if [ $VERBOSITY -ge 3 ]
X then
X    echo "verbosity: $VERBOSITY"
X    echo "warning: $WARNING"
X    echo "critical: $CRITICAL"
X    echo "pgroup: $PROCESS_GROUP"
X    echo "erl: $ERL"
X    echo "cookie: $COOKIE"
X    echo "node: $NODE"
X    echo "tmp_node: $TMP_NODE"
X    echo "beam: $BEAM"
X    echo "full command: $CMD"
fi
$CMD
SHAR_EOF
  (set 20 09 12 30 08 11 04 'tmp/prebuild/check_erlang_stage/check_erlang_pg.sh'
   eval "${shar_touch}") && \
  chmod 0755 'tmp/prebuild/check_erlang_stage/check_erlang_pg.sh'
if test $? -ne 0
then ${echo} "restore of tmp/prebuild/check_erlang_stage/check_erlang_pg.sh failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'tmp/prebuild/check_erlang_stage/check_erlang_pg.sh': 'MD5 check failed'
       ) << \SHAR_EOF
e77b4d7260af382c1675caa376330e3d  tmp/prebuild/check_erlang_stage/check_erlang_pg.sh
SHAR_EOF
  else
test `LC_ALL=C wc -c < 'tmp/prebuild/check_erlang_stage/check_erlang_pg.sh'` -ne 4544 && \
  ${echo} "restoration warning:  size of 'tmp/prebuild/check_erlang_stage/check_erlang_pg.sh' is not 4544"
  fi
fi
# ============= tmp/prebuild/check_erlang_stage/ebin/.gitignore ==============
if test ! -d 'tmp/prebuild/check_erlang_stage/ebin'; then
  mkdir 'tmp/prebuild/check_erlang_stage/ebin'
if test $? -eq 0
then ${echo} "x - created directory tmp/prebuild/check_erlang_stage/ebin."
else ${echo} "x - failed to create directory tmp/prebuild/check_erlang_stage/ebin."
     exit 1
fi
fi
if test -n "${keep_file}" && test -f 'tmp/prebuild/check_erlang_stage/ebin/.gitignore'
then
${echo} "x - SKIPPING tmp/prebuild/check_erlang_stage/ebin/.gitignore (file already exists)"
else
${echo} "x - extracting tmp/prebuild/check_erlang_stage/ebin/.gitignore (binary)"
  sed 's/^X//' << 'SHAR_EOF' | uudecode &&
begin 600 tmp/prebuild/check_erlang_stage/ebin/.gitignore
)*BYB96%M"BI^
`
end
SHAR_EOF
  (set 20 09 12 30 08 11 04 'tmp/prebuild/check_erlang_stage/ebin/.gitignore'
   eval "${shar_touch}") && \
  chmod 0664 'tmp/prebuild/check_erlang_stage/ebin/.gitignore'
if test $? -ne 0
then ${echo} "restore of tmp/prebuild/check_erlang_stage/ebin/.gitignore failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'tmp/prebuild/check_erlang_stage/ebin/.gitignore': 'MD5 check failed'
       ) << \SHAR_EOF
e2ba88a3088a977037de2776942dc042  tmp/prebuild/check_erlang_stage/ebin/.gitignore
SHAR_EOF
  else
test `LC_ALL=C wc -c < 'tmp/prebuild/check_erlang_stage/ebin/.gitignore'` -ne 9 && \
  ${echo} "restoration warning:  size of 'tmp/prebuild/check_erlang_stage/ebin/.gitignore' is not 9"
  fi
fi
# ============= tmp/prebuild/check_erlang_stage/nagios_erlang.erl ==============
if test -n "${keep_file}" && test -f 'tmp/prebuild/check_erlang_stage/nagios_erlang.erl'
then
${echo} "x - SKIPPING tmp/prebuild/check_erlang_stage/nagios_erlang.erl (file already exists)"
else
${echo} "x - extracting tmp/prebuild/check_erlang_stage/nagios_erlang.erl (text)"
  sed 's/^X//' << 'SHAR_EOF' > 'tmp/prebuild/check_erlang_stage/nagios_erlang.erl' &&
%% @doc Tools for checking on nodes, applications and process groups to
%%      support Nagios monitoring of Erlang systems.
%%
%% @author Will Larson <lethain@gmail.com> [http://lethain.com]
%% @version 0.1
%% @link <a href="http://github.com/lethain/nagios_erlang">Latest nagios_erlang source code on GitHub</a>
%% @link <a href="http://www.nagios.org/">Nagios homepage</a>
-module(nagios_erlang).
-author("Will Larson <lethain@gmail.com> [http://lethain.com]").
-version("0.1").
-export([check_process_group/1, check_node/1, check_application/1]).
-define(OK_CODE, 0).
-define(WARN_CODE, 1).
-define(CRIT_CODE, 2).
-define(UNKNOWN_CODE, 3).
-define(RPC_TIMEOUT, 1000).
X
%%%
%%% External Interfaces
%%%
X
%% @doc Check if a process group (from remote perspective of the supplied node)
%%      has more than WarnLvl or CritLvl living processes, and return an
%%      appropriate response (OK, WARNING, CRITICAL).
%%
%%      Warning/Critical messages are never returned if the value of WarnLvl /
%%      CritLvl is 0 respectively.
%%
%%      Has no return value due to calling erlang:halt/1.
%% @spec check_process_group(atom(), atom(), integer(), integer()).
check_process_group([Node, Group, WarnLvl, CritLvl]) ->
X    format_output(check_process_group_inner(Node, Group, WarnLvl, CritLvl)).
X
%% @doc Check if a node is accessible by net_adm:ping/1 from the current node,
%%      and return OK or CRITICAL messages as appropriate.
%%
%%      Has no return value due to calling erlang:halt/1.
%% @term check_node(atom()).
check_node([Node]) ->	    
X    format_output(check_node_inner(Node)).
X
%% @doc Check if the specified application is currently running on the specified node,
%%      and return OK or CRITICAL messages as appropriate.
%%
%%      Has no return value due to calling erlang:halt/1.
%% @spec check_application(atom(), atom().
check_application([Node, Application]) ->
X    format_output(check_application_inner(Node, Application)).
X
%%%
%%% Internal Implementations
%%%
X
%% @doc Check that a remote node responds to net_adm:ping and return Nagios friendly response.
%% @spec check_node_inner(atom()) -> {status(), string(), list()}.
%%       status = ok | warning | critical | unknown
check_node_inner(Node) when is_list(Node) ->
X    check_node_inner(list_to_atom(Node));
check_node_inner(Node) ->
X    case net_adm:ping(Node) of
X	pong ->
X	    {ok, "Node ~p running.~n", [Node]};
X	pang ->
X	    {critical, "Node ~p not running.~n", [Node]}
X    end.
X		
X
%% @doc Check that a remote node is running an application and return Nagios friendly response.
%% @spec check_application_inner(atom(), atom()) -> {status(), string(), list()}.
%%       status = ok | warning | critical | unknown
check_application_inner(Node, Application) when is_list(Node) ->
X    check_application_inner(list_to_atom(Node), Application);
check_application_inner(Node, Application) when is_list(Application) ->    
X    check_application_inner(Node, list_to_atom(Application));
check_application_inner(Node, Application) ->
X    case rpc:call(Node, application, start, [Application], ?RPC_TIMEOUT) of
X	{badrpc, timeout} ->
X	    {warning, "Node ~p did not respond within ~p milliseconds.~n", [Node, ?RPC_TIMEOUT]};
X	{badrpc, _} ->
X	    {critical, "Couldn't contact Node ~p.~n", [Node]};
X	{error, {already_started, Application}} ->
X	    {ok, "Application ~p running on Node ~p.~n", [Application, Node]};
X	%% @todo move the {application_started ???} -> critical, _Other -> unknown
X	_Other ->
X	    {critical, "Application ~p not running on Node ~p.~n", [Application, Node]}
X    end.
X
%% @doc Check status of a remote node's pg2 groups  and return Nagios friendly response.
%%      If WarnLvl is 0, then no warning messages will be sent.
%%      If CritLvl is 0, then no critical messages will be sent.
%%      Critical messages take precedence over warning messages if conditions are satisfied for both.
%% @spec check_process_group_inner(atom(), atom(), integer(), integer()) -> {status(), string(), list()}.
%%       status = ok | warning | critical | unknown
check_process_group_inner(Node, Group, WarnLvl, CritLvl) when is_list(Node) ->
X    check_process_group_inner(list_to_atom(Node), Group, WarnLvl, CritLvl);
check_process_group_inner(Node, Group, WarnLvl, CritLvl) when is_list(Group) ->
X    check_process_group_inner(Node, list_to_atom(Group), WarnLvl, CritLvl);
check_process_group_inner(Node, Group, WarnLvl, CritLvl) when is_list(WarnLvl) ->
X    check_process_group_inner(Node, Group, list_to_integer(WarnLvl), CritLvl);
check_process_group_inner(Node, Group, WarnLvl, CritLvl) when is_list(CritLvl) ->
X    check_process_group_inner(Node, Group, WarnLvl, list_to_integer(CritLvl));
check_process_group_inner(Node, Group, WarnLvl, CritLvl) ->
X    case rpc:call(Node, pg2, get_members, [Group], ?RPC_TIMEOUT) of
X	{badrpc, timeout} ->
X	    {warning, "Node ~p did not respond within ~p milliseconds.~n", [Node, ?RPC_TIMEOUT]};
X	{badrpc, _} ->
X	    {critical, "Couldn't contact Node ~p.~n", [Node]};
X	{error,{no_such_group,Group}} ->
X	    {critical, "Process Group ~p doesn't exist on Node ~p.~n", [Group, Node]};
X	Pids when is_list(Pids) ->
X	    Length = length(Pids),
X	    if Length < CritLvl -> {critical, "Only ~p processes in Process Group ~p on Node ~p, expected ~p processes.", [Length, Group, Node, CritLvl]};
X	       Length < WarnLvl -> {warning, "Only ~p processes in Process Group ~p on Node ~p, expected ~p processes.", [Length, Group, Node, WarnLvl]};
X	       true -> {ok, "~p processes in Process Group ~p on Node ~p, meets expectation of ~p processes.", [Length, Group, Node, WarnLvl]}
X	    end;
X	_Other ->
X	    {unknown, "Couldn't check Process Group ~p on Node ~p.~n", [Group, Node]}
X    end.
X
X    
%%%
%%% Utilities
%%%
X
%% @doc Format input in the format expected by Nagios.
%%      Status is used as exit code, and the formatted
%%      combination of Msg & Vals is sent to stdout.
format_output({Status, Msg, Vals}) ->
X    case Status of
X	ok -> 
X	    io:format(lists:concat(["OK - ",Msg]), Vals),
X	    erlang:halt(?OK_CODE);
X	warning -> 
X	    io:format(lists:concat(["WARNING - ",Msg]), Vals),
X	    erlang:halt(?WARN_CODE);
X	critical -> 
X	    io:format(lists:concat(["CRITICAL - ",Msg]), Vals),
X	    erlang:halt(?CRIT_CODE);
X	unknown -> 
X	    io:format(lists:concat(["UNKNOWN - ",Msg]), Vals),
X	    erlang:halt(?UNKNOWN_CODE)
X    end.
SHAR_EOF
  (set 20 09 12 30 08 11 04 'tmp/prebuild/check_erlang_stage/nagios_erlang.erl'
   eval "${shar_touch}") && \
  chmod 0664 'tmp/prebuild/check_erlang_stage/nagios_erlang.erl'
if test $? -ne 0
then ${echo} "restore of tmp/prebuild/check_erlang_stage/nagios_erlang.erl failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'tmp/prebuild/check_erlang_stage/nagios_erlang.erl': 'MD5 check failed'
       ) << \SHAR_EOF
