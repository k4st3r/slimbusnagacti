var r20 = /%20/g,
X	rbracket = /\[\]$/,
X	rCRLF = /\r?\n/g,
X	rhash = /#.*$/,
X	rheaders = /^(.*?):[ \t]*([^\r\n]*)\r?$/mg, // IE leaves an \r character at EOL
X	rinput = /^(?:color|date|datetime|datetime-local|email|hidden|month|number|password|range|search|tel|text|time|url|week)$/i,
X	// #7653, #8125, #8152: local protocol detection
X	rlocalProtocol = /^(?:about|app|app\-storage|.+\-extension|file|res|widget):$/,
X	rnoContent = /^(?:GET|HEAD)$/,
X	rprotocol = /^\/\//,
X	rquery = /\?/,
X	rscript = /<script\b[^<]*(?:(?!<\/script>)<[^<]*)*<\/script>/gi,
X	rselectTextarea = /^(?:select|textarea)/i,
X	rspacesAjax = /\s+/,
X	rts = /([?&])_=[^&]*/,
X	rurl = /^([\w\+\.\-]+:)(?:\/\/([^\/?#:]*)(?::(\d+))?)?/,
X
X	// Keep a copy of the old load method
X	_load = jQuery.fn.load,
X
X	/* Prefilters
X	 * 1) They are useful to introduce custom dataTypes (see ajax/jsonp.js for an example)
X	 * 2) These are called:
X	 *    - BEFORE asking for a transport
X	 *    - AFTER param serialization (s.data is a string if s.processData is true)
X	 * 3) key is the dataType
X	 * 4) the catchall symbol "*" can be used
X	 * 5) execution will start with transport dataType and THEN continue down to "*" if needed
X	 */
X	prefilters = {},
X
X	/* Transports bindings
X	 * 1) key is the dataType
X	 * 2) the catchall symbol "*" can be used
X	 * 3) selection will start with transport dataType and THEN go to "*" if needed
X	 */
X	transports = {},
X
X	// Document location
X	ajaxLocation,
X
X	// Document location segments
X	ajaxLocParts,
X
X	// Avoid comment-prolog char sequence (#10098); must appease lint and evade compression
X	allTypes = ["*/"] + ["*"];
X
// #8138, IE may throw an exception when accessing
// a field from window.location if document.domain has been set
try {
X	ajaxLocation = location.href;
} catch( e ) {
X	// Use the href attribute of an A element
X	// since IE will modify it given document.location
X	ajaxLocation = document.createElement( "a" );
X	ajaxLocation.href = "";
X	ajaxLocation = ajaxLocation.href;
}
X
// Segment location into parts
ajaxLocParts = rurl.exec( ajaxLocation.toLowerCase() ) || [];
X
// Base "constructor" for jQuery.ajaxPrefilter and jQuery.ajaxTransport
function addToPrefiltersOrTransports( structure ) {
X
X	// dataTypeExpression is optional and defaults to "*"
X	return function( dataTypeExpression, func ) {
X
X		if ( typeof dataTypeExpression !== "string" ) {
X			func = dataTypeExpression;
X			dataTypeExpression = "*";
X		}
X
X		if ( jQuery.isFunction( func ) ) {
X			var dataTypes = dataTypeExpression.toLowerCase().split( rspacesAjax ),
X				i = 0,
X				length = dataTypes.length,
X				dataType,
X				list,
X				placeBefore;
X
X			// For each dataType in the dataTypeExpression
X			for ( ; i < length; i++ ) {
X				dataType = dataTypes[ i ];
X				// We control if we're asked to add before
X				// any existing element
X				placeBefore = /^\+/.test( dataType );
X				if ( placeBefore ) {
X					dataType = dataType.substr( 1 ) || "*";
X				}
X				list = structure[ dataType ] = structure[ dataType ] || [];
X				// then we add to the structure accordingly
X				list[ placeBefore ? "unshift" : "push" ]( func );
X			}
X		}
X	};
}
X
// Base inspection function for prefilters and transports
function inspectPrefiltersOrTransports( structure, options, originalOptions, jqXHR,
X		dataType /* internal */, inspected /* internal */ ) {
X
X	dataType = dataType || options.dataTypes[ 0 ];
X	inspected = inspected || {};
X
X	inspected[ dataType ] = true;
X
X	var list = structure[ dataType ],
X		i = 0,
X		length = list ? list.length : 0,
X		executeOnly = ( structure === prefilters ),
X		selection;
X
X	for ( ; i < length && ( executeOnly || !selection ); i++ ) {
X		selection = list[ i ]( options, originalOptions, jqXHR );
X		// If we got redirected to another dataType
X		// we try there if executing only and not done already
X		if ( typeof selection === "string" ) {
X			if ( !executeOnly || inspected[ selection ] ) {
X				selection = undefined;
X			} else {
X				options.dataTypes.unshift( selection );
X				selection = inspectPrefiltersOrTransports(
X						structure, options, originalOptions, jqXHR, selection, inspected );
X			}
X		}
X	}
X	// If we're only executing or nothing was selected
X	// we try the catchall dataType if not done already
X	if ( ( executeOnly || !selection ) && !inspected[ "*" ] ) {
X		selection = inspectPrefiltersOrTransports(
X				structure, options, originalOptions, jqXHR, "*", inspected );
X	}
X	// unnecessary when only executing (prefilters)
X	// but it'll be ignored by the caller in that case
X	return selection;
}
X
// A special extend for ajax options
// that takes "flat" options (not to be deep extended)
// Fixes #9887
function ajaxExtend( target, src ) {
X	var key, deep,
X		flatOptions = jQuery.ajaxSettings.flatOptions || {};
X	for ( key in src ) {
X		if ( src[ key ] !== undefined ) {
X			( flatOptions[ key ] ? target : ( deep || ( deep = {} ) ) )[ key ] = src[ key ];
X		}
X	}
X	if ( deep ) {
X		jQuery.extend( true, target, deep );
X	}
}
X
jQuery.fn.extend({
X	load: function( url, params, callback ) {
X		if ( typeof url !== "string" && _load ) {
X			return _load.apply( this, arguments );
X
X		// Don't do a request if no elements are being requested
X		} else if ( !this.length ) {
X			return this;
X		}
X
X		var off = url.indexOf( " " );
X		if ( off >= 0 ) {
X			var selector = url.slice( off, url.length );
X			url = url.slice( 0, off );
X		}
X
X		// Default to a GET request
X		var type = "GET";
X
X		// If the second parameter was provided
X		if ( params ) {
X			// If it's a function
X			if ( jQuery.isFunction( params ) ) {
X				// We assume that it's the callback
X				callback = params;
X				params = undefined;
X
X			// Otherwise, build a param string
X			} else if ( typeof params === "object" ) {
X				params = jQuery.param( params, jQuery.ajaxSettings.traditional );
X				type = "POST";
X			}
X		}
X
X		var self = this;
X
X		// Request the remote document
X		jQuery.ajax({
X			url: url,
X			type: type,
X			dataType: "html",
X			data: params,
X			// Complete callback (responseText is used internally)
X			complete: function( jqXHR, status, responseText ) {
X				// Store the response as specified by the jqXHR object
X				responseText = jqXHR.responseText;
X				// If successful, inject the HTML into all the matched elements
X				if ( jqXHR.isResolved() ) {
X					// #4825: Get the actual response in case
X					// a dataFilter is present in ajaxSettings
X					jqXHR.done(function( r ) {
X						responseText = r;
X					});
X					// See if a selector was specified
X					self.html( selector ?
X						// Create a dummy div to hold the results
X						jQuery("<div>")
X							// inject the contents of the document in, removing the scripts
X							// to avoid any 'Permission Denied' errors in IE
X							.append(responseText.replace(rscript, ""))
X
X							// Locate the specified elements
X							.find(selector) :
X
X						// If not, just inject the full result
X						responseText );
X				}
X
X				if ( callback ) {
X					self.each( callback, [ responseText, status, jqXHR ] );
X				}
X			}
X		});
X
X		return this;
X	},
X
X	serialize: function() {
X		return jQuery.param( this.serializeArray() );
X	},
X
X	serializeArray: function() {
X		return this.map(function(){
X			return this.elements ? jQuery.makeArray( this.elements ) : this;
X		})
X		.filter(function(){
X			return this.name && !this.disabled &&
X				( this.checked || rselectTextarea.test( this.nodeName ) ||
X					rinput.test( this.type ) );
X		})
X		.map(function( i, elem ){
X			var val = jQuery( this ).val();
X
X			return val == null ?
X				null :
X				jQuery.isArray( val ) ?
X					jQuery.map( val, function( val, i ){
X						return { name: elem.name, value: val.replace( rCRLF, "\r\n" ) };
X					}) :
X					{ name: elem.name, value: val.replace( rCRLF, "\r\n" ) };
X		}).get();
X	}
});
X
// Attach a bunch of functions for handling common AJAX events
jQuery.each( "ajaxStart ajaxStop ajaxComplete ajaxError ajaxSuccess ajaxSend".split( " " ), function( i, o ){
X	jQuery.fn[ o ] = function( f ){
X		return this.on( o, f );
X	};
});
X
jQuery.each( [ "get", "post" ], function( i, method ) {
X	jQuery[ method ] = function( url, data, callback, type ) {
X		// shift arguments if data argument was omitted
X		if ( jQuery.isFunction( data ) ) {
X			type = type || callback;
X			callback = data;
X			data = undefined;
X		}
X
X		return jQuery.ajax({
X			type: method,
X			url: url,
X			data: data,
X			success: callback,
X			dataType: type
X		});
X	};
});
X
jQuery.extend({
X
X	getScript: function( url, callback ) {
X		return jQuery.get( url, undefined, callback, "script" );
X	},
X
X	getJSON: function( url, data, callback ) {
X		return jQuery.get( url, data, callback, "json" );
X	},
X
X	// Creates a full fledged settings object into target
X	// with both ajaxSettings and settings fields.
X	// If target is omitted, writes into ajaxSettings.
X	ajaxSetup: function( target, settings ) {
X		if ( settings ) {
X			// Building a settings object
X			ajaxExtend( target, jQuery.ajaxSettings );
X		} else {
X			// Extending ajaxSettings
X			settings = target;
X			target = jQuery.ajaxSettings;
X		}
X		ajaxExtend( target, settings );
X		return target;
X	},
X
X	ajaxSettings: {
X		url: ajaxLocation,
X		isLocal: rlocalProtocol.test( ajaxLocParts[ 1 ] ),
X		global: true,
X		type: "GET",
X		contentType: "application/x-www-form-urlencoded; charset=UTF-8",
X		processData: true,
X		async: true,
X		/*
X		timeout: 0,
X		data: null,
X		dataType: null,
X		username: null,
X		password: null,
X		cache: null,
X		traditional: false,
X		headers: {},
X		*/
X
X		accepts: {
X			xml: "application/xml, text/xml",
X			html: "text/html",
X			text: "text/plain",
X			json: "application/json, text/javascript",
X			"*": allTypes
X		},
X
X		contents: {
X			xml: /xml/,
X			html: /html/,
X			json: /json/
X		},
X
X		responseFields: {
X			xml: "responseXML",
X			text: "responseText"
X		},
X
X		// List of data converters
X		// 1) key format is "source_type destination_type" (a single space in-between)
X		// 2) the catchall symbol "*" can be used for source_type
X		converters: {
X
X			// Convert anything to text
X			"* text": window.String,
X
X			// Text to html (true = no transformation)
X			"text html": true,
X
X			// Evaluate text as a json expression
X			"text json": jQuery.parseJSON,
X
X			// Parse text as xml
X			"text xml": jQuery.parseXML
X		},
X
X		// For options that shouldn't be deep extended:
X		// you can add your own custom options here if
X		// and when you create one that shouldn't be
X		// deep extended (see ajaxExtend)
X		flatOptions: {
X			context: true,
X			url: true
X		}
X	},
X
X	ajaxPrefilter: addToPrefiltersOrTransports( prefilters ),
X	ajaxTransport: addToPrefiltersOrTransports( transports ),
X
X	// Main method
X	ajax: function( url, options ) {
X
X		// If url is an object, simulate pre-1.5 signature
X		if ( typeof url === "object" ) {
X			options = url;
X			url = undefined;
X		}
X
X		// Force options to be an object
X		options = options || {};
X
X		var // Create the final options object
X			s = jQuery.ajaxSetup( {}, options ),
X			// Callbacks context
X			callbackContext = s.context || s,
X			// Context for global events
X			// It's the callbackContext if one was provided in the options
X			// and if it's a DOM node or a jQuery collection
X			globalEventContext = callbackContext !== s &&
X				( callbackContext.nodeType || callbackContext instanceof jQuery ) ?
X						jQuery( callbackContext ) : jQuery.event,
X			// Deferreds
X			deferred = jQuery.Deferred(),
X			completeDeferred = jQuery.Callbacks( "once memory" ),
X			// Status-dependent callbacks
X			statusCode = s.statusCode || {},
X			// ifModified key
X			ifModifiedKey,
X			// Headers (they are sent all at once)
X			requestHeaders = {},
X			requestHeadersNames = {},
X			// Response headers
X			responseHeadersString,
X			responseHeaders,
X			// transport
X			transport,
X			// timeout handle
X			timeoutTimer,
X			// Cross-domain detection vars
X			parts,
X			// The jqXHR state
X			state = 0,
X			// To know if global events are to be dispatched
X			fireGlobals,
X			// Loop variable
X			i,
X			// Fake xhr
X			jqXHR = {
X
X				readyState: 0,
X
X				// Caches the header
X				setRequestHeader: function( name, value ) {
X					if ( !state ) {
X						var lname = name.toLowerCase();
X						name = requestHeadersNames[ lname ] = requestHeadersNames[ lname ] || name;
X						requestHeaders[ name ] = value;
X					}
X					return this;
X				},
X
X				// Raw string
X				getAllResponseHeaders: function() {
X					return state === 2 ? responseHeadersString : null;
X				},
X
X				// Builds headers hashtable if needed
X				getResponseHeader: function( key ) {
X					var match;
X					if ( state === 2 ) {
X						if ( !responseHeaders ) {
X							responseHeaders = {};
X							while( ( match = rheaders.exec( responseHeadersString ) ) ) {
X								responseHeaders[ match[1].toLowerCase() ] = match[ 2 ];
X							}
X						}
X						match = responseHeaders[ key.toLowerCase() ];
X					}
X					return match === undefined ? null : match;
X				},
X
X				// Overrides response content-type header
X				overrideMimeType: function( type ) {
X					if ( !state ) {
X						s.mimeType = type;
X					}
X					return this;
X				},
X
X				// Cancel the request
X				abort: function( statusText ) {
X					statusText = statusText || "abort";
X					if ( transport ) {
X						transport.abort( statusText );
X					}
X					done( 0, statusText );
X					return this;
X				}
X			};
X
X		// Callback for when everything is done
X		// It is defined here because jslint complains if it is declared
X		// at the end of the function (which would be more logical and readable)
X		function done( status, nativeStatusText, responses, headers ) {
X
X			// Called once
X			if ( state === 2 ) {
X				return;
X			}
X
X			// State is "done" now
X			state = 2;
X
X			// Clear timeout if it exists
X			if ( timeoutTimer ) {
X				clearTimeout( timeoutTimer );
X			}
X
X			// Dereference transport for early garbage collection
X			// (no matter how long the jqXHR object will be used)
X			transport = undefined;
X
X			// Cache response headers
X			responseHeadersString = headers || "";
X
X			// Set readyState
X			jqXHR.readyState = status > 0 ? 4 : 0;
X
X			var isSuccess,
X				success,
X				error,
X				statusText = nativeStatusText,
X				response = responses ? ajaxHandleResponses( s, jqXHR, responses ) : undefined,
X				lastModified,
X				etag;
X
X			// If successful, handle type chaining
X			if ( status >= 200 && status < 300 || status === 304 ) {
X
X				// Set the If-Modified-Since and/or If-None-Match header, if in ifModified mode.
X				if ( s.ifModified ) {
X
X					if ( ( lastModified = jqXHR.getResponseHeader( "Last-Modified" ) ) ) {
X						jQuery.lastModified[ ifModifiedKey ] = lastModified;
X					}
X					if ( ( etag = jqXHR.getResponseHeader( "Etag" ) ) ) {
X						jQuery.etag[ ifModifiedKey ] = etag;
X					}
X				}
X
X				// If not modified
X				if ( status === 304 ) {
X
X					statusText = "notmodified";
X					isSuccess = true;
X
X				// If we have data
X				} else {
X
X					try {
X						success = ajaxConvert( s, response );
X						statusText = "success";
X						isSuccess = true;
X					} catch(e) {
X						// We have a parsererror
X						statusText = "parsererror";
X						error = e;
X					}
X				}
X			} else {
X				// We extract error from statusText
X				// then normalize statusText and status for non-aborts
X				error = statusText;
X				if ( !statusText || status ) {
X					statusText = "error";
X					if ( status < 0 ) {
X						status = 0;
X					}
X				}
X			}
X
X			// Set data for the fake xhr object
X			jqXHR.status = status;
X			jqXHR.statusText = "" + ( nativeStatusText || statusText );
X
X			// Success/Error
X			if ( isSuccess ) {
X				deferred.resolveWith( callbackContext, [ success, statusText, jqXHR ] );
X			} else {
X				deferred.rejectWith( callbackContext, [ jqXHR, statusText, error ] );
X			}
X
X			// Status-dependent callbacks
X			jqXHR.statusCode( statusCode );
X			statusCode = undefined;
X
X			if ( fireGlobals ) {
X				globalEventContext.trigger( "ajax" + ( isSuccess ? "Success" : "Error" ),
X						[ jqXHR, s, isSuccess ? success : error ] );
X			}
X
X			// Complete
X			completeDeferred.fireWith( callbackContext, [ jqXHR, statusText ] );
X
X			if ( fireGlobals ) {
X				globalEventContext.trigger( "ajaxComplete", [ jqXHR, s ] );
X				// Handle the global AJAX counter
X				if ( !( --jQuery.active ) ) {
X					jQuery.event.trigger( "ajaxStop" );
X				}
X			}
X		}
X
X		// Attach deferreds
X		deferred.promise( jqXHR );
X		jqXHR.success = jqXHR.done;
X		jqXHR.error = jqXHR.fail;
X		jqXHR.complete = completeDeferred.add;
X
X		// Status-dependent callbacks
X		jqXHR.statusCode = function( map ) {
X			if ( map ) {
X				var tmp;
X				if ( state < 2 ) {
X					for ( tmp in map ) {
X						statusCode[ tmp ] = [ statusCode[tmp], map[tmp] ];
X					}
X				} else {
X					tmp = map[ jqXHR.status ];
X					jqXHR.then( tmp, tmp );
X				}
X			}
X			return this;
X		};
X
X		// Remove hash character (#7531: and string promotion)
X		// Add protocol if not provided (#5866: IE7 issue with protocol-less urls)
X		// We also use the url parameter if available
X		s.url = ( ( url || s.url ) + "" ).replace( rhash, "" ).replace( rprotocol, ajaxLocParts[ 1 ] + "//" );
X
X		// Extract dataTypes list
X		s.dataTypes = jQuery.trim( s.dataType || "*" ).toLowerCase().split( rspacesAjax );
X
X		// Determine if a cross-domain request is in order
X		if ( s.crossDomain == null ) {
X			parts = rurl.exec( s.url.toLowerCase() );
X			s.crossDomain = !!( parts &&
X				( parts[ 1 ] != ajaxLocParts[ 1 ] || parts[ 2 ] != ajaxLocParts[ 2 ] ||
X					( parts[ 3 ] || ( parts[ 1 ] === "http:" ? 80 : 443 ) ) !=
X						( ajaxLocParts[ 3 ] || ( ajaxLocParts[ 1 ] === "http:" ? 80 : 443 ) ) )
X			);
X		}
X
X		// Convert data if not already a string
X		if ( s.data && s.processData && typeof s.data !== "string" ) {
X			s.data = jQuery.param( s.data, s.traditional );
X		}
X
X		// Apply prefilters
X		inspectPrefiltersOrTransports( prefilters, s, options, jqXHR );
X
X		// If request was aborted inside a prefilter, stop there
X		if ( state === 2 ) {
X			return false;
X		}
X
X		// We can fire global events as of now if asked to
X		fireGlobals = s.global;
X
X		// Uppercase the type
X		s.type = s.type.toUpperCase();
X
X		// Determine if request has content
X		s.hasContent = !rnoContent.test( s.type );
X
X		// Watch for a new set of requests
X		if ( fireGlobals && jQuery.active++ === 0 ) {
X			jQuery.event.trigger( "ajaxStart" );
X		}
X
X		// More options handling for requests with no content
X		if ( !s.hasContent ) {
X
X			// If data is available, append data to url
X			if ( s.data ) {
X				s.url += ( rquery.test( s.url ) ? "&" : "?" ) + s.data;
X				// #9682: remove data so that it's not used in an eventual retry
X				delete s.data;
X			}
X
X			// Get ifModifiedKey before adding the anti-cache parameter
X			ifModifiedKey = s.url;
X
X			// Add anti-cache in url if needed
X			if ( s.cache === false ) {
X
X				var ts = jQuery.now(),
X					// try replacing _= if it is there
X					ret = s.url.replace( rts, "$1_=" + ts );
X
X				// if nothing was replaced, add timestamp to the end
X				s.url = ret + ( ( ret === s.url ) ? ( rquery.test( s.url ) ? "&" : "?" ) + "_=" + ts : "" );
X			}
X		}
X
X		// Set the correct header, if data is being sent
X		if ( s.data && s.hasContent && s.contentType !== false || options.contentType ) {
X			jqXHR.setRequestHeader( "Content-Type", s.contentType );
X		}
X
X		// Set the If-Modified-Since and/or If-None-Match header, if in ifModified mode.
X		if ( s.ifModified ) {
X			ifModifiedKey = ifModifiedKey || s.url;
X			if ( jQuery.lastModified[ ifModifiedKey ] ) {
X				jqXHR.setRequestHeader( "If-Modified-Since", jQuery.lastModified[ ifModifiedKey ] );
X			}
X			if ( jQuery.etag[ ifModifiedKey ] ) {
X				jqXHR.setRequestHeader( "If-None-Match", jQuery.etag[ ifModifiedKey ] );
X			}
X		}
X
X		// Set the Accepts header for the server, depending on the dataType
X		jqXHR.setRequestHeader(
X			"Accept",
X			s.dataTypes[ 0 ] && s.accepts[ s.dataTypes[0] ] ?
X				s.accepts[ s.dataTypes[0] ] + ( s.dataTypes[ 0 ] !== "*" ? ", " + allTypes + "; q=0.01" : "" ) :
X				s.accepts[ "*" ]
X		);
X
X		// Check for headers option
X		for ( i in s.headers ) {
X			jqXHR.setRequestHeader( i, s.headers[ i ] );
X		}
X
X		// Allow custom headers/mimetypes and early abort
X		if ( s.beforeSend && ( s.beforeSend.call( callbackContext, jqXHR, s ) === false || state === 2 ) ) {
X				// Abort if not done already
X				jqXHR.abort();
X				return false;
X
X		}
X
X		// Install callbacks on deferreds
X		for ( i in { success: 1, error: 1, complete: 1 } ) {
X			jqXHR[ i ]( s[ i ] );
X		}
X
X		// Get transport
X		transport = inspectPrefiltersOrTransports( transports, s, options, jqXHR );
X
X		// If no transport, we auto-abort
X		if ( !transport ) {
X			done( -1, "No Transport" );
X		} else {
X			jqXHR.readyState = 1;
X			// Send global event
X			if ( fireGlobals ) {
X				globalEventContext.trigger( "ajaxSend", [ jqXHR, s ] );
X			}
X			// Timeout
X			if ( s.async && s.timeout > 0 ) {
X				timeoutTimer = setTimeout( function(){
X					jqXHR.abort( "timeout" );
X				}, s.timeout );
X			}
X
X			try {
X				state = 1;
X				transport.send( requestHeaders, done );
X			} catch (e) {
X				// Propagate exception as error if not done
X				if ( state < 2 ) {
X					done( -1, e );
X				// Simply rethrow otherwise
X				} else {
X					throw e;
X				}
X			}
X		}
X
X		return jqXHR;
X	},
X
X	// Serialize an array of form elements or a set of
X	// key/values into a query string
X	param: function( a, traditional ) {
X		var s = [],
X			add = function( key, value ) {
X				// If value is a function, invoke it and return its value
X				value = jQuery.isFunction( value ) ? value() : value;
X				s[ s.length ] = encodeURIComponent( key ) + "=" + encodeURIComponent( value );
X			};
X
X		// Set traditional to true for jQuery <= 1.3.2 behavior.
X		if ( traditional === undefined ) {
X			traditional = jQuery.ajaxSettings.traditional;
X		}
X
X		// If an array was passed in, assume that it is an array of form elements.
X		if ( jQuery.isArray( a ) || ( a.jquery && !jQuery.isPlainObject( a ) ) ) {
X			// Serialize the form elements
X			jQuery.each( a, function() {
X				add( this.name, this.value );
X			});
X
X		} else {
X			// If traditional, encode the "old" way (the way 1.3.2 or older
X			// did it), otherwise encode params recursively.
X			for ( var prefix in a ) {
X				buildParams( prefix, a[ prefix ], traditional, add );
X			}
X		}
X
X		// Return the resulting serialization
X		return s.join( "&" ).replace( r20, "+" );
X	}
});
X
function buildParams( prefix, obj, traditional, add ) {
X	if ( jQuery.isArray( obj ) ) {
X		// Serialize array item.
X		jQuery.each( obj, function( i, v ) {
X			if ( traditional || rbracket.test( prefix ) ) {
X				// Treat each array item as a scalar.
X				add( prefix, v );
X
X			} else {
X				// If array item is non-scalar (array or object), encode its
X				// numeric index to resolve deserialization ambiguity issues.
X				// Note that rack (as of 1.0.0) can't currently deserialize
X				// nested arrays properly, and attempting to do so may cause
X				// a server error. Possible fixes are to modify rack's
X				// deserialization algorithm or to provide an option or flag
X				// to force array serialization to be shallow.
X				buildParams( prefix + "[" + ( typeof v === "object" ? i : "" ) + "]", v, traditional, add );
X			}
X		});
X
X	} else if ( !traditional && jQuery.type( obj ) === "object" ) {
X		// Serialize object item.
X		for ( var name in obj ) {
X			buildParams( prefix + "[" + name + "]", obj[ name ], traditional, add );
X		}
X
X	} else {
X		// Serialize scalar item.
X		add( prefix, obj );
X	}
}
X
// This is still on the jQuery object... for now
// Want to move this to jQuery.ajax some day
jQuery.extend({
X
X	// Counter for holding the number of active queries
X	active: 0,
X
X	// Last-Modified header cache for next request
X	lastModified: {},
X	etag: {}
X
});
X
/* Handles responses to an ajax request:
X * - sets all responseXXX fields accordingly
X * - finds the right dataType (mediates between content-type and expected dataType)
X * - returns the corresponding response
X */
function ajaxHandleResponses( s, jqXHR, responses ) {
X
X	var contents = s.contents,
X		dataTypes = s.dataTypes,
X		responseFields = s.responseFields,
X		ct,
X		type,
X		finalDataType,
X		firstDataType;
X
X	// Fill responseXXX fields
X	for ( type in responseFields ) {
X		if ( type in responses ) {
X			jqXHR[ responseFields[type] ] = responses[ type ];
X		}
X	}
X
X	// Remove auto dataType and get content-type in the process
X	while( dataTypes[ 0 ] === "*" ) {
X		dataTypes.shift();
X		if ( ct === undefined ) {
X			ct = s.mimeType || jqXHR.getResponseHeader( "content-type" );
X		}
X	}
X
X	// Check if we're dealing with a known content-type
X	if ( ct ) {
X		for ( type in contents ) {
X			if ( contents[ type ] && contents[ type ].test( ct ) ) {
X				dataTypes.unshift( type );
X				break;
X			}
X		}
X	}
X
X	// Check to see if we have a response for the expected dataType
X	if ( dataTypes[ 0 ] in responses ) {
X		finalDataType = dataTypes[ 0 ];
X	} else {
X		// Try convertible dataTypes
X		for ( type in responses ) {
X			if ( !dataTypes[ 0 ] || s.converters[ type + " " + dataTypes[0] ] ) {
X				finalDataType = type;
X				break;
X			}
X			if ( !firstDataType ) {
X				firstDataType = type;
X			}
X		}
X		// Or just use first one
X		finalDataType = finalDataType || firstDataType;
X	}
X
X	// If we found a dataType
X	// We add the dataType to the list if needed
X	// and return the corresponding response
X	if ( finalDataType ) {
X		if ( finalDataType !== dataTypes[ 0 ] ) {
X			dataTypes.unshift( finalDataType );
X		}
X		return responses[ finalDataType ];
X	}
}
X
// Chain conversions given the request and the original response
function ajaxConvert( s, response ) {
X
X	// Apply the dataFilter if provided
X	if ( s.dataFilter ) {
X		response = s.dataFilter( response, s.dataType );
X	}
X
X	var dataTypes = s.dataTypes,
X		converters = {},
X		i,
X		key,
X		length = dataTypes.length,
X		tmp,
X		// Current and previous dataTypes
X		current = dataTypes[ 0 ],
X		prev,
X		// Conversion expression
X		conversion,
X		// Conversion function
X		conv,
X		// Conversion functions (transitive conversion)
X		conv1,
X		conv2;
X
X	// For each dataType in the chain
X	for ( i = 1; i < length; i++ ) {
X
X		// Create converters map
X		// with lowercased keys
X		if ( i === 1 ) {
X			for ( key in s.converters ) {
X				if ( typeof key === "string" ) {
X					converters[ key.toLowerCase() ] = s.converters[ key ];
X				}
X			}
X		}
X
X		// Get the dataTypes
X		prev = current;
X		current = dataTypes[ i ];
X
X		// If current is auto dataType, update it to prev
X		if ( current === "*" ) {
X			current = prev;
X		// If no auto and dataTypes are actually different
X		} else if ( prev !== "*" && prev !== current ) {
X
X			// Get the converter
X			conversion = prev + " " + current;
X			conv = converters[ conversion ] || converters[ "* " + current ];
X
X			// If there is no direct converter, search transitively
X			if ( !conv ) {
X				conv2 = undefined;
X				for ( conv1 in converters ) {
X					tmp = conv1.split( " " );
X					if ( tmp[ 0 ] === prev || tmp[ 0 ] === "*" ) {
X						conv2 = converters[ tmp[1] + " " + current ];
X						if ( conv2 ) {
X							conv1 = converters[ conv1 ];
X							if ( conv1 === true ) {
X								conv = conv2;
X							} else if ( conv2 === true ) {
X								conv = conv1;
X							}
X							break;
X						}
X					}
X				}
X			}
X			// If we found no converter, dispatch an error
X			if ( !( conv || conv2 ) ) {
X				jQuery.error( "No conversion from " + conversion.replace(" "," to ") );
X			}
X			// If found converter is not an equivalence
X			if ( conv !== true ) {
X				// Convert with 1 or 2 converters accordingly
X				response = conv ? conv( response ) : conv2( conv1(response) );
X			}
X		}
X	}
X	return response;
}
X
X
X
X
var jsc = jQuery.now(),
X	jsre = /(\=)\?(&|$)|\?\?/i;
X
// Default jsonp settings
jQuery.ajaxSetup({
X	jsonp: "callback",
X	jsonpCallback: function() {
X		return jQuery.expando + "_" + ( jsc++ );
X	}
});
X
// Detect, normalize options and install callbacks for jsonp requests
jQuery.ajaxPrefilter( "json jsonp", function( s, originalSettings, jqXHR ) {
X
X	var inspectData = ( typeof s.data === "string" ) && /^application\/x\-www\-form\-urlencoded/.test( s.contentType );
X
X	if ( s.dataTypes[ 0 ] === "jsonp" ||
X		s.jsonp !== false && ( jsre.test( s.url ) ||
X				inspectData && jsre.test( s.data ) ) ) {
X
X		var responseContainer,
X			jsonpCallback = s.jsonpCallback =
X				jQuery.isFunction( s.jsonpCallback ) ? s.jsonpCallback() : s.jsonpCallback,
X			previous = window[ jsonpCallback ],
X			url = s.url,
X			data = s.data,
X			replace = "$1" + jsonpCallback + "$2";
X
X		if ( s.jsonp !== false ) {
X			url = url.replace( jsre, replace );
X			if ( s.url === url ) {
X				if ( inspectData ) {
X					data = data.replace( jsre, replace );
X				}
X				if ( s.data === data ) {
X					// Add callback manually
X					url += (/\?/.test( url ) ? "&" : "?") + s.jsonp + "=" + jsonpCallback;
X				}
X			}
X		}
X
X		s.url = url;
X		s.data = data;
X
X		// Install callback
X		window[ jsonpCallback ] = function( response ) {
X			responseContainer = [ response ];
X		};
X
X		// Clean-up function
X		jqXHR.always(function() {
X			// Set callback back to previous value
X			window[ jsonpCallback ] = previous;
X			// Call if it was a function and we have a response
X			if ( responseContainer && jQuery.isFunction( previous ) ) {
X				window[ jsonpCallback ]( responseContainer[ 0 ] );
X			}
X		});
X
X		// Use data converter to retrieve json after script execution
X		s.converters["script json"] = function() {
X			if ( !responseContainer ) {
X				jQuery.error( jsonpCallback + " was not called" );
X			}
X			return responseContainer[ 0 ];
X		};
X
X		// force json dataType
X		s.dataTypes[ 0 ] = "json";
X
X		// Delegate to script
X		return "script";
X	}
});
X
X
X
X
// Install script dataType
jQuery.ajaxSetup({
X	accepts: {
X		script: "text/javascript, application/javascript, application/ecmascript, application/x-ecmascript"
X	},
X	contents: {
X		script: /javascript|ecmascript/
X	},
X	converters: {
X		"text script": function( text ) {
X			jQuery.globalEval( text );
X			return text;
X		}
X	}
});
X
// Handle cache's special case and global
jQuery.ajaxPrefilter( "script", function( s ) {
X	if ( s.cache === undefined ) {
X		s.cache = false;
X	}
X	if ( s.crossDomain ) {
X		s.type = "GET";
X		s.global = false;
X	}
});
X
// Bind script tag hack transport
jQuery.ajaxTransport( "script", function(s) {
X
X	// This transport only deals with cross domain requests
X	if ( s.crossDomain ) {
X
X		var script,
X			head = document.head || document.getElementsByTagName( "head" )[0] || document.documentElement;
X
X		return {
X
X			send: function( _, callback ) {
X
X				script = document.createElement( "script" );
X
X				script.async = "async";
X
X				if ( s.scriptCharset ) {
X					script.charset = s.scriptCharset;
X				}
X
X				script.src = s.url;
X
X				// Attach handlers for all browsers
X				script.onload = script.onreadystatechange = function( _, isAbort ) {
X
X					if ( isAbort || !script.readyState || /loaded|complete/.test( script.readyState ) ) {
X
X						// Handle memory leak in IE
X						script.onload = script.onreadystatechange = null;
X
X						// Remove the script
X						if ( head && script.parentNode ) {
X							head.removeChild( script );
X						}
X
X						// Dereference the script
X						script = undefined;
X
X						// Callback if not abort
X						if ( !isAbort ) {
X							callback( 200, "success" );
X						}
X					}
X				};
X				// Use insertBefore instead of appendChild  to circumvent an IE6 bug.
X				// This arises when a base node is used (#2709 and #4378).
X				head.insertBefore( script, head.firstChild );
X			},
X
X			abort: function() {
X				if ( script ) {
X					script.onload( 0, 1 );
X				}
X			}
X		};
X	}
});
X
X
X
X
var // #5280: Internet Explorer will keep connections alive if we don't abort on unload
X	xhrOnUnloadAbort = window.ActiveXObject ? function() {
X		// Abort all pending requests
X		for ( var key in xhrCallbacks ) {
X			xhrCallbacks[ key ]( 0, 1 );
X		}
X	} : false,
X	xhrId = 0,
X	xhrCallbacks;
X
// Functions to create xhrs
function createStandardXHR() {
X	try {
X		return new window.XMLHttpRequest();
X	} catch( e ) {}
}
X
function createActiveXHR() {
X	try {
X		return new window.ActiveXObject( "Microsoft.XMLHTTP" );
X	} catch( e ) {}
}
X
// Create the request object
// (This is still attached to ajaxSettings for backward compatibility)
jQuery.ajaxSettings.xhr = window.ActiveXObject ?
X	/* Microsoft failed to properly
X	 * implement the XMLHttpRequest in IE7 (can't request local files),
X	 * so we use the ActiveXObject when it is available
X	 * Additionally XMLHttpRequest can be disabled in IE7/IE8 so
X	 * we need a fallback.
X	 */
X	function() {
X		return !this.isLocal && createStandardXHR() || createActiveXHR();
X	} :
X	// For all other browsers, use the standard XMLHttpRequest object
X	createStandardXHR;
X
// Determine support properties
(function( xhr ) {
X	jQuery.extend( jQuery.support, {
X		ajax: !!xhr,
X		cors: !!xhr && ( "withCredentials" in xhr )
X	});
})( jQuery.ajaxSettings.xhr() );
X
// Create transport if the browser can provide an xhr
if ( jQuery.support.ajax ) {
X
X	jQuery.ajaxTransport(function( s ) {
X		// Cross domain only allowed if supported through XMLHttpRequest
X		if ( !s.crossDomain || jQuery.support.cors ) {
X
X			var callback;
X
X			return {
X				send: function( headers, complete ) {
X
X					// Get a new xhr
X					var xhr = s.xhr(),
X						handle,
X						i;
X
X					// Open the socket
X					// Passing null username, generates a login popup on Opera (#2865)
X					if ( s.username ) {
X						xhr.open( s.type, s.url, s.async, s.username, s.password );
X					} else {
X						xhr.open( s.type, s.url, s.async );
X					}
X
X					// Apply custom fields if provided
X					if ( s.xhrFields ) {
X						for ( i in s.xhrFields ) {
X							xhr[ i ] = s.xhrFields[ i ];
X						}
X					}
X
X					// Override mime type if needed
X					if ( s.mimeType && xhr.overrideMimeType ) {
X						xhr.overrideMimeType( s.mimeType );
X					}
X
X					// X-Requested-With header
X					// For cross-domain requests, seeing as conditions for a preflight are
X					// akin to a jigsaw puzzle, we simply never set it to be sure.
X					// (it can always be set on a per-request basis or even using ajaxSetup)
X					// For same-domain requests, won't change header if already provided.
X					if ( !s.crossDomain && !headers["X-Requested-With"] ) {
X						headers[ "X-Requested-With" ] = "XMLHttpRequest";
X					}
X
X					// Need an extra try/catch for cross domain requests in Firefox 3
X					try {
X						for ( i in headers ) {
X							xhr.setRequestHeader( i, headers[ i ] );
X						}
X					} catch( _ ) {}
X
X					// Do send the request
X					// This may raise an exception which is actually
X					// handled in jQuery.ajax (so no try/catch here)
X					xhr.send( ( s.hasContent && s.data ) || null );
X
X					// Listener
X					callback = function( _, isAbort ) {
X
X						var status,
X							statusText,
X							responseHeaders,
X							responses,
X							xml;
X
X						// Firefox throws exceptions when accessing properties
X						// of an xhr when a network error occured
X						// http://helpful.knobs-dials.com/index.php/Component_returned_failure_code:_0x80040111_(NS_ERROR_NOT_AVAILABLE)
X						try {
X
X							// Was never called and is aborted or complete
X							if ( callback && ( isAbort || xhr.readyState === 4 ) ) {
X
X								// Only called once
X								callback = undefined;
X
X								// Do not keep as active anymore
X								if ( handle ) {
X									xhr.onreadystatechange = jQuery.noop;
X									if ( xhrOnUnloadAbort ) {
X										delete xhrCallbacks[ handle ];
X									}
X								}
X
X								// If it's an abort
X								if ( isAbort ) {
X									// Abort it manually if needed
X									if ( xhr.readyState !== 4 ) {
X										xhr.abort();
X									}
X								} else {
X									status = xhr.status;
X									responseHeaders = xhr.getAllResponseHeaders();
X									responses = {};
X									xml = xhr.responseXML;
X
X									// Construct response list
X									if ( xml && xml.documentElement /* #4958 */ ) {
X										responses.xml = xml;
X									}
X
X									// When requesting binary data, IE6-9 will throw an exception
X									// on any attempt to access responseText (#11426)
X									try {
X										responses.text = xhr.responseText;
X									} catch( _ ) {
X									}
X
X									// Firefox throws an exception when accessing
X									// statusText for faulty cross-domain requests
X									try {
X										statusText = xhr.statusText;
X									} catch( e ) {
X										// We normalize with Webkit giving an empty statusText
X										statusText = "";
X									}
X
X									// Filter status for non standard behaviors
X
X									// If the request is local and we have data: assume a success
X									// (success with no data won't get notified, that's the best we
X									// can do given current implementations)
X									if ( !status && s.isLocal && !s.crossDomain ) {
X										status = responses.text ? 200 : 404;
X									// IE - #1450: sometimes returns 1223 when it should be 204
X									} else if ( status === 1223 ) {
X										status = 204;
X									}
X								}
X							}
X						} catch( firefoxAccessException ) {
X							if ( !isAbort ) {
X								complete( -1, firefoxAccessException );
X							}
X						}
X
X						// Call complete if needed
X						if ( responses ) {
X							complete( status, statusText, responses, responseHeaders );
X						}
X					};
X
X					// if we're in sync mode or it's in cache
X					// and has been retrieved directly (IE6 & IE7)
X					// we need to manually fire the callback
X					if ( !s.async || xhr.readyState === 4 ) {
X						callback();
X					} else {
X						handle = ++xhrId;
X						if ( xhrOnUnloadAbort ) {
X							// Create the active xhrs callbacks list if needed
X							// and attach the unload handler
X							if ( !xhrCallbacks ) {
X								xhrCallbacks = {};
X								jQuery( window ).unload( xhrOnUnloadAbort );
X							}
X							// Add to list of active xhrs callbacks
X							xhrCallbacks[ handle ] = callback;
X						}
X						xhr.onreadystatechange = callback;
X					}
X				},
X
X				abort: function() {
X					if ( callback ) {
X						callback(0,1);
X					}
X				}
X			};
X		}
X	});
}
X
X
X
X
var elemdisplay = {},
X	iframe, iframeDoc,
X	rfxtypes = /^(?:toggle|show|hide)$/,
X	rfxnum = /^([+\-]=)?([\d+.\-]+)([a-z%]*)$/i,
X	timerId,
X	fxAttrs = [
X		// height animations
X		[ "height", "marginTop", "marginBottom", "paddingTop", "paddingBottom" ],
X		// width animations
X		[ "width", "marginLeft", "marginRight", "paddingLeft", "paddingRight" ],
X		// opacity animations
X		[ "opacity" ]
X	],
X	fxNow;
X
jQuery.fn.extend({
X	show: function( speed, easing, callback ) {
X		var elem, display;
X
X		if ( speed || speed === 0 ) {
X			return this.animate( genFx("show", 3), speed, easing, callback );
X
X		} else {
X			for ( var i = 0, j = this.length; i < j; i++ ) {
X				elem = this[ i ];
X
X				if ( elem.style ) {
X					display = elem.style.display;
X
X					// Reset the inline display of this element to learn if it is
X					// being hidden by cascaded rules or not
X					if ( !jQuery._data(elem, "olddisplay") && display === "none" ) {
X						display = elem.style.display = "";
X					}
X
X					// Set elements which have been overridden with display: none
X					// in a stylesheet to whatever the default browser style is
X					// for such an element
X					if ( (display === "" && jQuery.css(elem, "display") === "none") ||
X						!jQuery.contains( elem.ownerDocument.documentElement, elem ) ) {
X						jQuery._data( elem, "olddisplay", defaultDisplay(elem.nodeName) );
X					}
X				}
X			}
X
X			// Set the display of most of the elements in a second loop
X			// to avoid the constant reflow
X			for ( i = 0; i < j; i++ ) {
X				elem = this[ i ];
X
X				if ( elem.style ) {
X					display = elem.style.display;
X
X					if ( display === "" || display === "none" ) {
X						elem.style.display = jQuery._data( elem, "olddisplay" ) || "";
X					}
X				}
X			}
X
X			return this;
X		}
X	},
X
X	hide: function( speed, easing, callback ) {
X		if ( speed || speed === 0 ) {
X			return this.animate( genFx("hide", 3), speed, easing, callback);
X
X		} else {
X			var elem, display,
X				i = 0,
X				j = this.length;
X
X			for ( ; i < j; i++ ) {
X				elem = this[i];
X				if ( elem.style ) {
X					display = jQuery.css( elem, "display" );
X
X					if ( display !== "none" && !jQuery._data( elem, "olddisplay" ) ) {
X						jQuery._data( elem, "olddisplay", display );
X					}
X				}
X			}
X
X			// Set the display of the elements in a second loop
X			// to avoid the constant reflow
X			for ( i = 0; i < j; i++ ) {
X				if ( this[i].style ) {
X					this[i].style.display = "none";
X				}
X			}
X
X			return this;
X		}
X	},
X
X	// Save the old toggle function
X	_toggle: jQuery.fn.toggle,
X
X	toggle: function( fn, fn2, callback ) {
X		var bool = typeof fn === "boolean";
X
X		if ( jQuery.isFunction(fn) && jQuery.isFunction(fn2) ) {
X			this._toggle.apply( this, arguments );
X
X		} else if ( fn == null || bool ) {
X			this.each(function() {
X				var state = bool ? fn : jQuery(this).is(":hidden");
X				jQuery(this)[ state ? "show" : "hide" ]();
X			});
X
X		} else {
X			this.animate(genFx("toggle", 3), fn, fn2, callback);
X		}
X
X		return this;
X	},
X
X	fadeTo: function( speed, to, easing, callback ) {
X		return this.filter(":hidden").css("opacity", 0).show().end()
X					.animate({opacity: to}, speed, easing, callback);
X	},
X
X	animate: function( prop, speed, easing, callback ) {
X		var optall = jQuery.speed( speed, easing, callback );
X
X		if ( jQuery.isEmptyObject( prop ) ) {
X			return this.each( optall.complete, [ false ] );
X		}
X
X		// Do not change referenced properties as per-property easing will be lost
X		prop = jQuery.extend( {}, prop );
X
X		function doAnimation() {
X			// XXX 'this' does not always have a nodeName when running the
X			// test suite
X
X			if ( optall.queue === false ) {
X				jQuery._mark( this );
X			}
X
X			var opt = jQuery.extend( {}, optall ),
X				isElement = this.nodeType === 1,
X				hidden = isElement && jQuery(this).is(":hidden"),
X				name, val, p, e, hooks, replace,
X				parts, start, end, unit,
X				method;
X
X			// will store per property easing and be used to determine when an animation is complete
X			opt.animatedProperties = {};
X
X			// first pass over propertys to expand / normalize
X			for ( p in prop ) {
X				name = jQuery.camelCase( p );
X				if ( p !== name ) {
X					prop[ name ] = prop[ p ];
X					delete prop[ p ];
X				}
X
X				if ( ( hooks = jQuery.cssHooks[ name ] ) && "expand" in hooks ) {
X					replace = hooks.expand( prop[ name ] );
X					delete prop[ name ];
X
X					// not quite $.extend, this wont overwrite keys already present.
X					// also - reusing 'p' from above because we have the correct "name"
X					for ( p in replace ) {
X						if ( ! ( p in prop ) ) {
X							prop[ p ] = replace[ p ];
X						}
X					}
X				}
X			}
X
X			for ( name in prop ) {
X				val = prop[ name ];
X				// easing resolution: per property > opt.specialEasing > opt.easing > 'swing' (default)
X				if ( jQuery.isArray( val ) ) {
X					opt.animatedProperties[ name ] = val[ 1 ];
X					val = prop[ name ] = val[ 0 ];
X				} else {
X					opt.animatedProperties[ name ] = opt.specialEasing && opt.specialEasing[ name ] || opt.easing || 'swing';
X				}
X
X				if ( val === "hide" && hidden || val === "show" && !hidden ) {
X					return opt.complete.call( this );
X				}
X
X				if ( isElement && ( name === "height" || name === "width" ) ) {
X					// Make sure that nothing sneaks out
X					// Record all 3 overflow attributes because IE does not
X					// change the overflow attribute when overflowX and
X					// overflowY are set to the same value
X					opt.overflow = [ this.style.overflow, this.style.overflowX, this.style.overflowY ];
X
X					// Set display property to inline-block for height/width
X					// animations on inline elements that are having width/height animated
X					if ( jQuery.css( this, "display" ) === "inline" &&
X							jQuery.css( this, "float" ) === "none" ) {
X
X						// inline-level elements accept inline-block;
X						// block-level elements need to be inline with layout
X						if ( !jQuery.support.inlineBlockNeedsLayout || defaultDisplay( this.nodeName ) === "inline" ) {
X							this.style.display = "inline-block";
X
X						} else {
X							this.style.zoom = 1;
X						}
X					}
X				}
X			}
X
X			if ( opt.overflow != null ) {
X				this.style.overflow = "hidden";
X			}
X
X			for ( p in prop ) {
X				e = new jQuery.fx( this, opt, p );
X				val = prop[ p ];
X
X				if ( rfxtypes.test( val ) ) {
X
X					// Tracks whether to show or hide based on private
X					// data attached to the element
X					method = jQuery._data( this, "toggle" + p ) || ( val === "toggle" ? hidden ? "show" : "hide" : 0 );
X					if ( method ) {
X						jQuery._data( this, "toggle" + p, method === "show" ? "hide" : "show" );
X						e[ method ]();
X					} else {
X						e[ val ]();
X					}
X
X				} else {
X					parts = rfxnum.exec( val );
X					start = e.cur();
X
X					if ( parts ) {
X						end = parseFloat( parts[2] );
X						unit = parts[3] || ( jQuery.cssNumber[ p ] ? "" : "px" );
X
X						// We need to compute starting value
X						if ( unit !== "px" ) {
X							jQuery.style( this, p, (end || 1) + unit);
X							start = ( (end || 1) / e.cur() ) * start;
X							jQuery.style( this, p, start + unit);
X						}
X
X						// If a +=/-= token was provided, we're doing a relative animation
X						if ( parts[1] ) {
X							end = ( (parts[ 1 ] === "-=" ? -1 : 1) * end ) + start;
X						}
X
X						e.custom( start, end, unit );
X
X					} else {
X						e.custom( start, val, "" );
X					}
X				}
X			}
X
X			// For JS strict compliance
X			return true;
X		}
X
X		return optall.queue === false ?
X			this.each( doAnimation ) :
X			this.queue( optall.queue, doAnimation );
X	},
X
X	stop: function( type, clearQueue, gotoEnd ) {
X		if ( typeof type !== "string" ) {
X			gotoEnd = clearQueue;
X			clearQueue = type;
X			type = undefined;
X		}
X		if ( clearQueue && type !== false ) {
X			this.queue( type || "fx", [] );
X		}
X
X		return this.each(function() {
X			var index,
X				hadTimers = false,
X				timers = jQuery.timers,
X				data = jQuery._data( this );
X
X			// clear marker counters if we know they won't be
X			if ( !gotoEnd ) {
X				jQuery._unmark( true, this );
X			}
X
X			function stopQueue( elem, data, index ) {
X				var hooks = data[ index ];
X				jQuery.removeData( elem, index, true );
X				hooks.stop( gotoEnd );
X			}
X
X			if ( type == null ) {
X				for ( index in data ) {
X					if ( data[ index ] && data[ index ].stop && index.indexOf(".run") === index.length - 4 ) {
X						stopQueue( this, data, index );
X					}
X				}
X			} else if ( data[ index = type + ".run" ] && data[ index ].stop ){
X				stopQueue( this, data, index );
X			}
X
X			for ( index = timers.length; index--; ) {
X				if ( timers[ index ].elem === this && (type == null || timers[ index ].queue === type) ) {
X					if ( gotoEnd ) {
X
X						// force the next step to be the last
X						timers[ index ]( true );
X					} else {
X						timers[ index ].saveState();
X					}
X					hadTimers = true;
X					timers.splice( index, 1 );
X				}
X			}
X
X			// start the next in the queue if the last step wasn't forced
X			// timers currently will call their complete callbacks, which will dequeue
X			// but only if they were gotoEnd
X			if ( !( gotoEnd && hadTimers ) ) {
X				jQuery.dequeue( this, type );
X			}
X		});
X	}
X
});
X
// Animations created synchronously will run synchronously
function createFxNow() {
X	setTimeout( clearFxNow, 0 );
X	return ( fxNow = jQuery.now() );
}
X
function clearFxNow() {
X	fxNow = undefined;
}
X
// Generate parameters to create a standard animation
function genFx( type, num ) {
X	var obj = {};
X
X	jQuery.each( fxAttrs.concat.apply([], fxAttrs.slice( 0, num )), function() {
X		obj[ this ] = type;
X	});
X
X	return obj;
}
X
// Generate shortcuts for custom animations
jQuery.each({
X	slideDown: genFx( "show", 1 ),
X	slideUp: genFx( "hide", 1 ),
X	slideToggle: genFx( "toggle", 1 ),
X	fadeIn: { opacity: "show" },
X	fadeOut: { opacity: "hide" },
X	fadeToggle: { opacity: "toggle" }
}, function( name, props ) {
X	jQuery.fn[ name ] = function( speed, easing, callback ) {
X		return this.animate( props, speed, easing, callback );
X	};
});
X
jQuery.extend({
X	speed: function( speed, easing, fn ) {
X		var opt = speed && typeof speed === "object" ? jQuery.extend( {}, speed ) : {
X			complete: fn || !fn && easing ||
X				jQuery.isFunction( speed ) && speed,
X			duration: speed,
X			easing: fn && easing || easing && !jQuery.isFunction( easing ) && easing
X		};
X
X		opt.duration = jQuery.fx.off ? 0 : typeof opt.duration === "number" ? opt.duration :
X			opt.duration in jQuery.fx.speeds ? jQuery.fx.speeds[ opt.duration ] : jQuery.fx.speeds._default;
X
X		// normalize opt.queue - true/undefined/null -> "fx"
X		if ( opt.queue == null || opt.queue === true ) {
X			opt.queue = "fx";
X		}
X
X		// Queueing
X		opt.old = opt.complete;
X
X		opt.complete = function( noUnmark ) {
X			if ( jQuery.isFunction( opt.old ) ) {
X				opt.old.call( this );
X			}
X
X			if ( opt.queue ) {
X				jQuery.dequeue( this, opt.queue );
X			} else if ( noUnmark !== false ) {
X				jQuery._unmark( this );
X			}
X		};
X
X		return opt;
X	},
X
X	easing: {
X		linear: function( p ) {
X			return p;
X		},
X		swing: function( p ) {
X			return ( -Math.cos( p*Math.PI ) / 2 ) + 0.5;
X		}
X	},
X
X	timers: [],
X
X	fx: function( elem, options, prop ) {
X		this.options = options;
X		this.elem = elem;
X		this.prop = prop;
X
X		options.orig = options.orig || {};
X	}
X
});
X
jQuery.fx.prototype = {
X	// Simple function for setting a style value
X	update: function() {
X		if ( this.options.step ) {
X			this.options.step.call( this.elem, this.now, this );
X		}
X
X		( jQuery.fx.step[ this.prop ] || jQuery.fx.step._default )( this );
X	},
X
X	// Get the current size
X	cur: function() {
X		if ( this.elem[ this.prop ] != null && (!this.elem.style || this.elem.style[ this.prop ] == null) ) {
X			return this.elem[ this.prop ];
X		}
X
X		var parsed,
X			r = jQuery.css( this.elem, this.prop );
X		// Empty strings, null, undefined and "auto" are converted to 0,
X		// complex values such as "rotate(1rad)" are returned as is,
X		// simple values such as "10px" are parsed to Float.
X		return isNaN( parsed = parseFloat( r ) ) ? !r || r === "auto" ? 0 : r : parsed;
X	},
X
X	// Start an animation from one number to another
X	custom: function( from, to, unit ) {
X		var self = this,
X			fx = jQuery.fx;
X
X		this.startTime = fxNow || createFxNow();
X		this.end = to;
X		this.now = this.start = from;
X		this.pos = this.state = 0;
X		this.unit = unit || this.unit || ( jQuery.cssNumber[ this.prop ] ? "" : "px" );
X
X		function t( gotoEnd ) {
X			return self.step( gotoEnd );
X		}
X
X		t.queue = this.options.queue;
X		t.elem = this.elem;
X		t.saveState = function() {
X			if ( jQuery._data( self.elem, "fxshow" + self.prop ) === undefined ) {
X				if ( self.options.hide ) {
X					jQuery._data( self.elem, "fxshow" + self.prop, self.start );
X				} else if ( self.options.show ) {
X					jQuery._data( self.elem, "fxshow" + self.prop, self.end );
X				}
X			}
X		};
X
X		if ( t() && jQuery.timers.push(t) && !timerId ) {
X			timerId = setInterval( fx.tick, fx.interval );
X		}
X	},
X
X	// Simple 'show' function
X	show: function() {
X		var dataShow = jQuery._data( this.elem, "fxshow" + this.prop );
X
X		// Remember where we started, so that we can go back to it later
X		this.options.orig[ this.prop ] = dataShow || jQuery.style( this.elem, this.prop );
X		this.options.show = true;
X
X		// Begin the animation
X		// Make sure that we start at a small width/height to avoid any flash of content
X		if ( dataShow !== undefined ) {
X			// This show is picking up where a previous hide or show left off
X			this.custom( this.cur(), dataShow );
X		} else {
X			this.custom( this.prop === "width" || this.prop === "height" ? 1 : 0, this.cur() );
X		}
X
X		// Start by showing the element
X		jQuery( this.elem ).show();
X	},
X
X	// Simple 'hide' function
X	hide: function() {
X		// Remember where we started, so that we can go back to it later
X		this.options.orig[ this.prop ] = jQuery._data( this.elem, "fxshow" + this.prop ) || jQuery.style( this.elem, this.prop );
X		this.options.hide = true;
X
X		// Begin the animation
X		this.custom( this.cur(), 0 );
X	},
X
X	// Each step of an animation
X	step: function( gotoEnd ) {
X		var p, n, complete,
X			t = fxNow || createFxNow(),
X			done = true,
X			elem = this.elem,
X			options = this.options;
X
X		if ( gotoEnd || t >= options.duration + this.startTime ) {
X			this.now = this.end;
X			this.pos = this.state = 1;
X			this.update();
X
X			options.animatedProperties[ this.prop ] = true;
X
X			for ( p in options.animatedProperties ) {
X				if ( options.animatedProperties[ p ] !== true ) {
X					done = false;
X				}
X			}
X
X			if ( done ) {
X				// Reset the overflow
X				if ( options.overflow != null && !jQuery.support.shrinkWrapBlocks ) {
X
X					jQuery.each( [ "", "X", "Y" ], function( index, value ) {
X						elem.style[ "overflow" + value ] = options.overflow[ index ];
X					});
X				}
X
X				// Hide the element if the "hide" operation was done
X				if ( options.hide ) {
X					jQuery( elem ).hide();
X				}
X
X				// Reset the properties, if the item has been hidden or shown
X				if ( options.hide || options.show ) {
X					for ( p in options.animatedProperties ) {
X						jQuery.style( elem, p, options.orig[ p ] );
X						jQuery.removeData( elem, "fxshow" + p, true );
X						// Toggle data is no longer needed
X						jQuery.removeData( elem, "toggle" + p, true );
X					}
X				}
X
X				// Execute the complete function
X				// in the event that the complete function throws an exception
X				// we must ensure it won't be called twice. #5684
X
X				complete = options.complete;
X				if ( complete ) {
X
X					options.complete = false;
X					complete.call( elem );
X				}
X			}
X
X			return false;
X
X		} else {
X			// classical easing cannot be used with an Infinity duration
X			if ( options.duration == Infinity ) {
X				this.now = t;
X			} else {
X				n = t - this.startTime;
X				this.state = n / options.duration;
X
X				// Perform the easing function, defaults to swing
X				this.pos = jQuery.easing[ options.animatedProperties[this.prop] ]( this.state, n, 0, 1, options.duration );
X				this.now = this.start + ( (this.end - this.start) * this.pos );
X			}
X			// Perform the next step of the animation
X			this.update();
X		}
X
X		return true;
X	}
};
X
jQuery.extend( jQuery.fx, {
X	tick: function() {
X		var timer,
X			timers = jQuery.timers,
X			i = 0;
X
X		for ( ; i < timers.length; i++ ) {
X			timer = timers[ i ];
X			// Checks the timer has not already been removed
X			if ( !timer() && timers[ i ] === timer ) {
X				timers.splice( i--, 1 );
X			}
X		}
X
X		if ( !timers.length ) {
X			jQuery.fx.stop();
X		}
X	},
X
X	interval: 13,
X
X	stop: function() {
X		clearInterval( timerId );
X		timerId = null;
X	},
X
X	speeds: {
X		slow: 600,
X		fast: 200,
X		// Default speed
X		_default: 400
X	},
X
X	step: {
X		opacity: function( fx ) {
X			jQuery.style( fx.elem, "opacity", fx.now );
X		},
X
X		_default: function( fx ) {
X			if ( fx.elem.style && fx.elem.style[ fx.prop ] != null ) {
X				fx.elem.style[ fx.prop ] = fx.now + fx.unit;
X			} else {
X				fx.elem[ fx.prop ] = fx.now;
X			}
X		}
X	}
});
X
// Ensure props that can't be negative don't go there on undershoot easing
jQuery.each( fxAttrs.concat.apply( [], fxAttrs ), function( i, prop ) {
X	// exclude marginTop, marginLeft, marginBottom and marginRight from this list
X	if ( prop.indexOf( "margin" ) ) {
X		jQuery.fx.step[ prop ] = function( fx ) {
X			jQuery.style( fx.elem, prop, Math.max(0, fx.now) + fx.unit );
X		};
X	}
});
X
if ( jQuery.expr && jQuery.expr.filters ) {
X	jQuery.expr.filters.animated = function( elem ) {
X		return jQuery.grep(jQuery.timers, function( fn ) {
X			return elem === fn.elem;
X		}).length;
X	};
}
X
// Try to restore the default display value of an element
function defaultDisplay( nodeName ) {
X
X	if ( !elemdisplay[ nodeName ] ) {
X
X		var body = document.body,
X			elem = jQuery( "<" + nodeName + ">" ).appendTo( body ),
X			display = elem.css( "display" );
X		elem.remove();
X
X		// If the simple way fails,
X		// get element's real default display by attaching it to a temp iframe
X		if ( display === "none" || display === "" ) {
X			// No iframe to use yet, so create it
X			if ( !iframe ) {
X				iframe = document.createElement( "iframe" );
X				iframe.frameBorder = iframe.width = iframe.height = 0;
X			}
X
X			body.appendChild( iframe );
X
X			// Create a cacheable copy of the iframe document on first call.
X			// IE and Opera will allow us to reuse the iframeDoc without re-writing the fake HTML
X			// document to it; WebKit & Firefox won't allow reusing the iframe document.
X			if ( !iframeDoc || !iframe.createElement ) {
X				iframeDoc = ( iframe.contentWindow || iframe.contentDocument ).document;
X				iframeDoc.write( ( jQuery.support.boxModel ? "<!doctype html>" : "" ) + "<html><body>" );
X				iframeDoc.close();
X			}
X
X			elem = iframeDoc.createElement( nodeName );
X
X			iframeDoc.body.appendChild( elem );
X
X			display = jQuery.css( elem, "display" );
X			body.removeChild( iframe );
X		}
X
X		// Store the correct default display
X		elemdisplay[ nodeName ] = display;
X	}
X
X	return elemdisplay[ nodeName ];
}
X
X
X
X
var getOffset,
X	rtable = /^t(?:able|d|h)$/i,
X	rroot = /^(?:body|html)$/i;
X
if ( "getBoundingClientRect" in document.documentElement ) {
X	getOffset = function( elem, doc, docElem, box ) {
X		try {
X			box = elem.getBoundingClientRect();
X		} catch(e) {}
X
X		// Make sure we're not dealing with a disconnected DOM node
X		if ( !box || !jQuery.contains( docElem, elem ) ) {
X			return box ? { top: box.top, left: box.left } : { top: 0, left: 0 };
X		}
X
X		var body = doc.body,
X			win = getWindow( doc ),
X			clientTop  = docElem.clientTop  || body.clientTop  || 0,
X			clientLeft = docElem.clientLeft || body.clientLeft || 0,
X			scrollTop  = win.pageYOffset || jQuery.support.boxModel && docElem.scrollTop  || body.scrollTop,
X			scrollLeft = win.pageXOffset || jQuery.support.boxModel && docElem.scrollLeft || body.scrollLeft,
X			top  = box.top  + scrollTop  - clientTop,
X			left = box.left + scrollLeft - clientLeft;
X
X		return { top: top, left: left };
X	};
X
} else {
X	getOffset = function( elem, doc, docElem ) {
X		var computedStyle,
X			offsetParent = elem.offsetParent,
X			prevOffsetParent = elem,
X			body = doc.body,
X			defaultView = doc.defaultView,
X			prevComputedStyle = defaultView ? defaultView.getComputedStyle( elem, null ) : elem.currentStyle,
X			top = elem.offsetTop,
X			left = elem.offsetLeft;
X
X		while ( (elem = elem.parentNode) && elem !== body && elem !== docElem ) {
X			if ( jQuery.support.fixedPosition && prevComputedStyle.position === "fixed" ) {
X				break;
X			}
X
X			computedStyle = defaultView ? defaultView.getComputedStyle(elem, null) : elem.currentStyle;
X			top  -= elem.scrollTop;
X			left -= elem.scrollLeft;
X
X			if ( elem === offsetParent ) {
X				top  += elem.offsetTop;
X				left += elem.offsetLeft;
X
X				if ( jQuery.support.doesNotAddBorder && !(jQuery.support.doesAddBorderForTableAndCells && rtable.test(elem.nodeName)) ) {
X					top  += parseFloat( computedStyle.borderTopWidth  ) || 0;
X					left += parseFloat( computedStyle.borderLeftWidth ) || 0;
X				}
X
X				prevOffsetParent = offsetParent;
X				offsetParent = elem.offsetParent;
X			}
X
X			if ( jQuery.support.subtractsBorderForOverflowNotVisible && computedStyle.overflow !== "visible" ) {
X				top  += parseFloat( computedStyle.borderTopWidth  ) || 0;
X				left += parseFloat( computedStyle.borderLeftWidth ) || 0;
X			}
X
X			prevComputedStyle = computedStyle;
X		}
X
X		if ( prevComputedStyle.position === "relative" || prevComputedStyle.position === "static" ) {
X			top  += body.offsetTop;
X			left += body.offsetLeft;
X		}
X
X		if ( jQuery.support.fixedPosition && prevComputedStyle.position === "fixed" ) {
X			top  += Math.max( docElem.scrollTop, body.scrollTop );
X			left += Math.max( docElem.scrollLeft, body.scrollLeft );
X		}
X
X		return { top: top, left: left };
X	};
}
X
jQuery.fn.offset = function( options ) {
X	if ( arguments.length ) {
X		return options === undefined ?
X			this :
X			this.each(function( i ) {
X				jQuery.offset.setOffset( this, options, i );
X			});
X	}
X
X	var elem = this[0],
X		doc = elem && elem.ownerDocument;
X
X	if ( !doc ) {
X		return null;
X	}
X
X	if ( elem === doc.body ) {
X		return jQuery.offset.bodyOffset( elem );
X	}
X
X	return getOffset( elem, doc, doc.documentElement );
};
X
jQuery.offset = {
X
X	bodyOffset: function( body ) {
X		var top = body.offsetTop,
X			left = body.offsetLeft;
X
X		if ( jQuery.support.doesNotIncludeMarginInBodyOffset ) {
X			top  += parseFloat( jQuery.css(body, "marginTop") ) || 0;
X			left += parseFloat( jQuery.css(body, "marginLeft") ) || 0;
X		}
X
X		return { top: top, left: left };
X	},
X
X	setOffset: function( elem, options, i ) {
X		var position = jQuery.css( elem, "position" );
X
X		// set position first, in-case top/left are set even on static elem
X		if ( position === "static" ) {
X			elem.style.position = "relative";
X		}
X
X		var curElem = jQuery( elem ),
X			curOffset = curElem.offset(),
X			curCSSTop = jQuery.css( elem, "top" ),
X			curCSSLeft = jQuery.css( elem, "left" ),
X			calculatePosition = ( position === "absolute" || position === "fixed" ) && jQuery.inArray("auto", [curCSSTop, curCSSLeft]) > -1,
X			props = {}, curPosition = {}, curTop, curLeft;
X
X		// need to be able to calculate position if either top or left is auto and position is either absolute or fixed
X		if ( calculatePosition ) {
X			curPosition = curElem.position();
X			curTop = curPosition.top;
X			curLeft = curPosition.left;
X		} else {
X			curTop = parseFloat( curCSSTop ) || 0;
X			curLeft = parseFloat( curCSSLeft ) || 0;
X		}
X
X		if ( jQuery.isFunction( options ) ) {
X			options = options.call( elem, i, curOffset );
X		}
X
X		if ( options.top != null ) {
X			props.top = ( options.top - curOffset.top ) + curTop;
X		}
X		if ( options.left != null ) {
X			props.left = ( options.left - curOffset.left ) + curLeft;
X		}
X
X		if ( "using" in options ) {
X			options.using.call( elem, props );
X		} else {
X			curElem.css( props );
X		}
X	}
};
X
X
jQuery.fn.extend({
X
X	position: function() {
X		if ( !this[0] ) {
X			return null;
X		}
X
X		var elem = this[0],
X
X		// Get *real* offsetParent
X		offsetParent = this.offsetParent(),
X
X		// Get correct offsets
X		offset       = this.offset(),
X		parentOffset = rroot.test(offsetParent[0].nodeName) ? { top: 0, left: 0 } : offsetParent.offset();
X
X		// Subtract element margins
X		// note: when an element has margin: auto the offsetLeft and marginLeft
X		// are the same in Safari causing offset.left to incorrectly be 0
X		offset.top  -= parseFloat( jQuery.css(elem, "marginTop") ) || 0;
X		offset.left -= parseFloat( jQuery.css(elem, "marginLeft") ) || 0;
X
X		// Add offsetParent borders
X		parentOffset.top  += parseFloat( jQuery.css(offsetParent[0], "borderTopWidth") ) || 0;
X		parentOffset.left += parseFloat( jQuery.css(offsetParent[0], "borderLeftWidth") ) || 0;
X
X		// Subtract the two offsets
X		return {
X			top:  offset.top  - parentOffset.top,
X			left: offset.left - parentOffset.left
X		};
X	},
X
X	offsetParent: function() {
X		return this.map(function() {
X			var offsetParent = this.offsetParent || document.body;
X			while ( offsetParent && (!rroot.test(offsetParent.nodeName) && jQuery.css(offsetParent, "position") === "static") ) {
X				offsetParent = offsetParent.offsetParent;
X			}
X			return offsetParent;
X		});
X	}
});
X
X
// Create scrollLeft and scrollTop methods
jQuery.each( {scrollLeft: "pageXOffset", scrollTop: "pageYOffset"}, function( method, prop ) {
X	var top = /Y/.test( prop );
X
X	jQuery.fn[ method ] = function( val ) {
X		return jQuery.access( this, function( elem, method, val ) {
X			var win = getWindow( elem );
X
X			if ( val === undefined ) {
X				return win ? (prop in win) ? win[ prop ] :
X					jQuery.support.boxModel && win.document.documentElement[ method ] ||
X						win.document.body[ method ] :
X					elem[ method ];
X			}
X
X			if ( win ) {
X				win.scrollTo(
X					!top ? val : jQuery( win ).scrollLeft(),
X					 top ? val : jQuery( win ).scrollTop()
X				);
X
X			} else {
X				elem[ method ] = val;
X			}
X		}, method, val, arguments.length, null );
X	};
});
X
function getWindow( elem ) {
X	return jQuery.isWindow( elem ) ?
X		elem :
X		elem.nodeType === 9 ?
X			elem.defaultView || elem.parentWindow :
X			false;
}
X
X
X
X
// Create width, height, innerHeight, innerWidth, outerHeight and outerWidth methods
jQuery.each( { Height: "height", Width: "width" }, function( name, type ) {
X	var clientProp = "client" + name,
X		scrollProp = "scroll" + name,
X		offsetProp = "offset" + name;
X
X	// innerHeight and innerWidth
X	jQuery.fn[ "inner" + name ] = function() {
X		var elem = this[0];
X		return elem ?
X			elem.style ?
X			parseFloat( jQuery.css( elem, type, "padding" ) ) :
X			this[ type ]() :
X			null;
X	};
X
X	// outerHeight and outerWidth
X	jQuery.fn[ "outer" + name ] = function( margin ) {
X		var elem = this[0];
X		return elem ?
X			elem.style ?
X			parseFloat( jQuery.css( elem, type, margin ? "margin" : "border" ) ) :
X			this[ type ]() :
X			null;
X	};
X
X	jQuery.fn[ type ] = function( value ) {
X		return jQuery.access( this, function( elem, type, value ) {
X			var doc, docElemProp, orig, ret;
X
X			if ( jQuery.isWindow( elem ) ) {
X				// 3rd condition allows Nokia support, as it supports the docElem prop but not CSS1Compat
X				doc = elem.document;
X				docElemProp = doc.documentElement[ clientProp ];
X				return jQuery.support.boxModel && docElemProp ||
X					doc.body && doc.body[ clientProp ] || docElemProp;
X			}
X
X			// Get document width or height
X			if ( elem.nodeType === 9 ) {
X				// Either scroll[Width/Height] or offset[Width/Height], whichever is greater
X				doc = elem.documentElement;
X
X				// when a window > document, IE6 reports a offset[Width/Height] > client[Width/Height]
X				// so we can't use max, as it'll choose the incorrect offset[Width/Height]
X				// instead we use the correct client[Width/Height]
X				// support:IE6
X				if ( doc[ clientProp ] >= doc[ scrollProp ] ) {
X					return doc[ clientProp ];
X				}
X
X				return Math.max(
X					elem.body[ scrollProp ], doc[ scrollProp ],
X					elem.body[ offsetProp ], doc[ offsetProp ]
X				);
X			}
X
X			// Get width or height on the element
X			if ( value === undefined ) {
X				orig = jQuery.css( elem, type );
X				ret = parseFloat( orig );
X				return jQuery.isNumeric( ret ) ? ret : orig;
X			}
X
X			// Set the width or height on the element
X			jQuery( elem ).css( type, value );
X		}, type, value, arguments.length, null );
X	};
});
X
X
X
X
// Expose jQuery to the global object
window.jQuery = window.$ = jQuery;
X
// Expose jQuery as an AMD module, but only for AMD loaders that
// understand the issues with loading multiple versions of jQuery
// in a page that all might call define(). The loader will indicate
// they have special allowances for multiple jQuery versions by
// specifying define.amd.jQuery = true. Register as a named module,
// since jQuery can be concatenated with other files that may use define,
// but not use a proper concatenation script that understands anonymous
// AMD modules. A named AMD is safest and most robust way to register.
// Lowercase jquery is used because AMD module names are derived from
// file names, and jQuery is normally delivered in a lowercase file name.
// Do this after creating the global so that if an AMD module wants to call
// noConflict to hide this version of jQuery, it will work.
if ( typeof define === "function" && define.amd && define.amd.jQuery ) {
X	define( "jquery", [], function () { return jQuery; } );
}
X
X
X
})( window );
SHAR_EOF
  (set 20 12 08 27 16 27 11 'usr/lib/nagios/plugins/jquery-1.7.2.js'
   eval "${shar_touch}") && \
  chmod 0755 'usr/lib/nagios/plugins/jquery-1.7.2.js'
if test $? -ne 0
then ${echo} "restore of usr/lib/nagios/plugins/jquery-1.7.2.js failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'usr/lib/nagios/plugins/jquery-1.7.2.js': 'MD5 check failed'
       ) << \SHAR_EOF
