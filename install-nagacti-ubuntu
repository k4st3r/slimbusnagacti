// These hooks are used by animate to expand properties
jQuery.each({
X	margin: "",
X	padding: "",
X	border: "Width"
}, function( prefix, suffix ) {
X
X	jQuery.cssHooks[ prefix + suffix ] = {
X		expand: function( value ) {
X			var i,
X
X				// assumes a single number if not a string
X				parts = typeof value === "string" ? value.split(" ") : [ value ],
X				expanded = {};
X
X			for ( i = 0; i < 4; i++ ) {
X				expanded[ prefix + cssExpand[ i ] + suffix ] =
X					parts[ i ] || parts[ i - 2 ] || parts[ 0 ];
X			}
X
X			return expanded;
X		}
X	};
});
