X
// String to Object flags format cache
var flagsCache = {};
X
// Convert String-formatted flags into Object-formatted ones and store in cache
function createFlags( flags ) {
X	var object = flagsCache[ flags ] = {},
X		i, length;
X	flags = flags.split( /\s+/ );
X	for ( i = 0, length = flags.length; i < length; i++ ) {
X		object[ flags[i] ] = true;
X	}
X	return object;
}
X
/*
X * Create a callback list using the following parameters:
X *
X *	flags:	an optional list of space-separated flags that will change how
X *			the callback list behaves
X *
X * By default a callback list will act like an event callback list and can be
X * "fired" multiple times.
X *
X * Possible flags:
X *
X *	once:			will ensure the callback list can only be fired once (like a Deferred)
X *
X *	memory:			will keep track of previous values and will call any callback added
X *					after the list has been fired right away with the latest "memorized"
X *					values (like a Deferred)
X *
X *	unique:			will ensure a callback can only be added once (no duplicate in the list)
X *
X *	stopOnFalse:	interrupt callings when a callback returns false
X *
X */
jQuery.Callbacks = function( flags ) {
X
X	// Convert flags from String-formatted to Object-formatted
X	// (we check in cache first)
X	flags = flags ? ( flagsCache[ flags ] || createFlags( flags ) ) : {};
X
X	var // Actual callback list
X		list = [],
X		// Stack of fire calls for repeatable lists
X		stack = [],
X		// Last fire value (for non-forgettable lists)
X		memory,
X		// Flag to know if list was already fired
X		fired,
X		// Flag to know if list is currently firing
X		firing,
X		// First callback to fire (used internally by add and fireWith)
X		firingStart,
X		// End of the loop when firing
X		firingLength,
X		// Index of currently firing callback (modified by remove if needed)
X		firingIndex,
X		// Add one or several callbacks to the list
X		add = function( args ) {
X			var i,
X				length,
X				elem,
X				type,
X				actual;
X			for ( i = 0, length = args.length; i < length; i++ ) {
X				elem = args[ i ];
X				type = jQuery.type( elem );
X				if ( type === "array" ) {
X					// Inspect recursively
X					add( elem );
X				} else if ( type === "function" ) {
X					// Add if not in unique mode and callback is not in
X					if ( !flags.unique || !self.has( elem ) ) {
X						list.push( elem );
X					}
X				}
X			}
X		},
X		// Fire callbacks
X		fire = function( context, args ) {
X			args = args || [];
X			memory = !flags.memory || [ context, args ];
X			fired = true;
X			firing = true;
X			firingIndex = firingStart || 0;
X			firingStart = 0;
X			firingLength = list.length;
X			for ( ; list && firingIndex < firingLength; firingIndex++ ) {
X				if ( list[ firingIndex ].apply( context, args ) === false && flags.stopOnFalse ) {
X					memory = true; // Mark as halted
X					break;
X				}
X			}
X			firing = false;
X			if ( list ) {
X				if ( !flags.once ) {
X					if ( stack && stack.length ) {
X						memory = stack.shift();
X						self.fireWith( memory[ 0 ], memory[ 1 ] );
X					}
X				} else if ( memory === true ) {
X					self.disable();
X				} else {
X					list = [];
X				}
X			}
X		},
X		// Actual Callbacks object
X		self = {
X			// Add a callback or a collection of callbacks to the list
X			add: function() {
X				if ( list ) {
X					var length = list.length;
X					add( arguments );
X					// Do we need to add the callbacks to the
X					// current firing batch?
X					if ( firing ) {
X						firingLength = list.length;
X					// With memory, if we're not firing then
X					// we should call right away, unless previous
X					// firing was halted (stopOnFalse)
X					} else if ( memory && memory !== true ) {
X						firingStart = length;
X						fire( memory[ 0 ], memory[ 1 ] );
X					}
X				}
X				return this;
X			},
X			// Remove a callback from the list
X			remove: function() {
X				if ( list ) {
X					var args = arguments,
X						argIndex = 0,
X						argLength = args.length;
X					for ( ; argIndex < argLength ; argIndex++ ) {
X						for ( var i = 0; i < list.length; i++ ) {
X							if ( args[ argIndex ] === list[ i ] ) {
X								// Handle firingIndex and firingLength
X								if ( firing ) {
X									if ( i <= firingLength ) {
X										firingLength--;
X										if ( i <= firingIndex ) {
X											firingIndex--;
X										}
X									}
X								}
X								// Remove the element
X								list.splice( i--, 1 );
X								// If we have some unicity property then
X								// we only need to do this once
X								if ( flags.unique ) {
X									break;
X								}
X							}
X						}
X					}
X				}
X				return this;
X			},
X			// Control if a given callback is in the list
X			has: function( fn ) {
X				if ( list ) {
X					var i = 0,
X						length = list.length;
X					for ( ; i < length; i++ ) {
X						if ( fn === list[ i ] ) {
X							return true;
X						}
X					}
X				}
X				return false;
X			},
X			// Remove all callbacks from the list
X			empty: function() {
X				list = [];
X				return this;
X			},
X			// Have the list do nothing anymore
X			disable: function() {
X				list = stack = memory = undefined;
X				return this;
X			},
X			// Is it disabled?
X			disabled: function() {
X				return !list;
X			},
X			// Lock the list in its current state
X			lock: function() {
X				stack = undefined;
X				if ( !memory || memory === true ) {
X					self.disable();
X				}
X				return this;
X			},
X			// Is it locked?
X			locked: function() {
X				return !stack;
X			},
X			// Call all callbacks with the given context and arguments
X			fireWith: function( context, args ) {
X				if ( stack ) {
X					if ( firing ) {
X						if ( !flags.once ) {
X							stack.push( [ context, args ] );
X						}
X					} else if ( !( flags.once && memory ) ) {
X						fire( context, args );
X					}
X				}
X				return this;
X			},
X			// Call all the callbacks with the given arguments
X			fire: function() {
X				self.fireWith( this, arguments );
X				return this;
X			},
X			// To know if the callbacks have already been called at least once
X			fired: function() {
X				return !!fired;
X			}
X		};
X
X	return self;
};
X
X
X
X
var // Static reference to slice
X	sliceDeferred = [].slice;
X
jQuery.extend({
X
X	Deferred: function( func ) {
X		var doneList = jQuery.Callbacks( "once memory" ),
X			failList = jQuery.Callbacks( "once memory" ),
X			progressList = jQuery.Callbacks( "memory" ),
X			state = "pending",
X			lists = {
X				resolve: doneList,
X				reject: failList,
X				notify: progressList
X			},
X			promise = {
X				done: doneList.add,
X				fail: failList.add,
X				progress: progressList.add,
X
X				state: function() {
X					return state;
X				},
X
X				// Deprecated
X				isResolved: doneList.fired,
X				isRejected: failList.fired,
X
X				then: function( doneCallbacks, failCallbacks, progressCallbacks ) {
X					deferred.done( doneCallbacks ).fail( failCallbacks ).progress( progressCallbacks );
X					return this;
X				},
X				always: function() {
X					deferred.done.apply( deferred, arguments ).fail.apply( deferred, arguments );
X					return this;
X				},
X				pipe: function( fnDone, fnFail, fnProgress ) {
X					return jQuery.Deferred(function( newDefer ) {
X						jQuery.each( {
X							done: [ fnDone, "resolve" ],
X							fail: [ fnFail, "reject" ],
X							progress: [ fnProgress, "notify" ]
X						}, function( handler, data ) {
X							var fn = data[ 0 ],
X								action = data[ 1 ],
X								returned;
X							if ( jQuery.isFunction( fn ) ) {
X								deferred[ handler ](function() {
X									returned = fn.apply( this, arguments );
X									if ( returned && jQuery.isFunction( returned.promise ) ) {
X										returned.promise().then( newDefer.resolve, newDefer.reject, newDefer.notify );
X									} else {
X										newDefer[ action + "With" ]( this === deferred ? newDefer : this, [ returned ] );
X									}
X								});
X							} else {
X								deferred[ handler ]( newDefer[ action ] );
X							}
X						});
X					}).promise();
X				},
X				// Get a promise for this deferred
X				// If obj is provided, the promise aspect is added to the object
X				promise: function( obj ) {
X					if ( obj == null ) {
X						obj = promise;
X					} else {
X						for ( var key in promise ) {
X							obj[ key ] = promise[ key ];
X						}
X					}
X					return obj;
X				}
X			},
X			deferred = promise.promise({}),
X			key;
X
X		for ( key in lists ) {
X			deferred[ key ] = lists[ key ].fire;
X			deferred[ key + "With" ] = lists[ key ].fireWith;
X		}
X
X		// Handle state
X		deferred.done( function() {
X			state = "resolved";
X		}, failList.disable, progressList.lock ).fail( function() {
X			state = "rejected";
X		}, doneList.disable, progressList.lock );
X
X		// Call given func if any
X		if ( func ) {
X			func.call( deferred, deferred );
X		}
X
X		// All done!
X		return deferred;
X	},
X
X	// Deferred helper
X	when: function( firstParam ) {
X		var args = sliceDeferred.call( arguments, 0 ),
X			i = 0,
X			length = args.length,
X			pValues = new Array( length ),
X			count = length,
X			pCount = length,
X			deferred = length <= 1 && firstParam && jQuery.isFunction( firstParam.promise ) ?
X				firstParam :
X				jQuery.Deferred(),
X			promise = deferred.promise();
X		function resolveFunc( i ) {
X			return function( value ) {
X				args[ i ] = arguments.length > 1 ? sliceDeferred.call( arguments, 0 ) : value;
X				if ( !( --count ) ) {
X					deferred.resolveWith( deferred, args );
X				}
X			};
X		}
X		function progressFunc( i ) {
X			return function( value ) {
X				pValues[ i ] = arguments.length > 1 ? sliceDeferred.call( arguments, 0 ) : value;
X				deferred.notifyWith( promise, pValues );
X			};
X		}
X		if ( length > 1 ) {
X			for ( ; i < length; i++ ) {
X				if ( args[ i ] && args[ i ].promise && jQuery.isFunction( args[ i ].promise ) ) {
X					args[ i ].promise().then( resolveFunc(i), deferred.reject, progressFunc(i) );
X				} else {
X					--count;
X				}
X			}
X			if ( !count ) {
X				deferred.resolveWith( deferred, args );
X			}
X		} else if ( deferred !== firstParam ) {
X			deferred.resolveWith( deferred, length ? [ firstParam ] : [] );
X		}
X		return promise;
X	}
});
X
X
X
X
jQuery.support = (function() {
X
X	var support,
X		all,
X		a,
X		select,
X		opt,
X		input,
X		fragment,
X		tds,
X		events,
X		eventName,
X		i,
X		isSupported,
X		div = document.createElement( "div" ),
X		documentElement = document.documentElement;
X
X	// Preliminary tests
X	div.setAttribute("className", "t");
X	div.innerHTML = "   <link/><table></table><a href='/a' style='top:1px;float:left;opacity:.55;'>a</a><input type='checkbox'/>";
X
X	all = div.getElementsByTagName( "*" );
X	a = div.getElementsByTagName( "a" )[ 0 ];
X
X	// Can't get basic test support
X	if ( !all || !all.length || !a ) {
X		return {};
X	}
X
X	// First batch of supports tests
X	select = document.createElement( "select" );
X	opt = select.appendChild( document.createElement("option") );
X	input = div.getElementsByTagName( "input" )[ 0 ];
X
X	support = {
X		// IE strips leading whitespace when .innerHTML is used
X		leadingWhitespace: ( div.firstChild.nodeType === 3 ),
X
X		// Make sure that tbody elements aren't automatically inserted
X		// IE will insert them into empty tables
X		tbody: !div.getElementsByTagName("tbody").length,
X
X		// Make sure that link elements get serialized correctly by innerHTML
X		// This requires a wrapper element in IE
X		htmlSerialize: !!div.getElementsByTagName("link").length,
X
X		// Get the style information from getAttribute
X		// (IE uses .cssText instead)
X		style: /top/.test( a.getAttribute("style") ),
X
X		// Make sure that URLs aren't manipulated
X		// (IE normalizes it by default)
X		hrefNormalized: ( a.getAttribute("href") === "/a" ),
X
X		// Make sure that element opacity exists
X		// (IE uses filter instead)
X		// Use a regex to work around a WebKit issue. See #5145
X		opacity: /^0.55/.test( a.style.opacity ),
X
X		// Verify style float existence
X		// (IE uses styleFloat instead of cssFloat)
X		cssFloat: !!a.style.cssFloat,
X
X		// Make sure that if no value is specified for a checkbox
X		// that it defaults to "on".
X		// (WebKit defaults to "" instead)
X		checkOn: ( input.value === "on" ),
X
X		// Make sure that a selected-by-default option has a working selected property.
X		// (WebKit defaults to false instead of true, IE too, if it's in an optgroup)
X		optSelected: opt.selected,
X
X		// Test setAttribute on camelCase class. If it works, we need attrFixes when doing get/setAttribute (ie6/7)
X		getSetAttribute: div.className !== "t",
X
X		// Tests for enctype support on a form(#6743)
X		enctype: !!document.createElement("form").enctype,
X
X		// Makes sure cloning an html5 element does not cause problems
X		// Where outerHTML is undefined, this still works
X		html5Clone: document.createElement("nav").cloneNode( true ).outerHTML !== "<:nav></:nav>",
X
X		// Will be defined later
X		submitBubbles: true,
X		changeBubbles: true,
X		focusinBubbles: false,
X		deleteExpando: true,
X		noCloneEvent: true,
X		inlineBlockNeedsLayout: false,
X		shrinkWrapBlocks: false,
X		reliableMarginRight: true,
X		pixelMargin: true
X	};
X
X	// jQuery.boxModel DEPRECATED in 1.3, use jQuery.support.boxModel instead
X	jQuery.boxModel = support.boxModel = (document.compatMode === "CSS1Compat");
X
X	// Make sure checked status is properly cloned
X	input.checked = true;
X	support.noCloneChecked = input.cloneNode( true ).checked;
X
X	// Make sure that the options inside disabled selects aren't marked as disabled
X	// (WebKit marks them as disabled)
X	select.disabled = true;
X	support.optDisabled = !opt.disabled;
X
X	// Test to see if it's possible to delete an expando from an element
X	// Fails in Internet Explorer
X	try {
X		delete div.test;
X	} catch( e ) {
X		support.deleteExpando = false;
X	}
X
X	if ( !div.addEventListener && div.attachEvent && div.fireEvent ) {
X		div.attachEvent( "onclick", function() {
X			// Cloning a node shouldn't copy over any
X			// bound event handlers (IE does this)
X			support.noCloneEvent = false;
X		});
X		div.cloneNode( true ).fireEvent( "onclick" );
X	}
X
X	// Check if a radio maintains its value
X	// after being appended to the DOM
X	input = document.createElement("input");
X	input.value = "t";
X	input.setAttribute("type", "radio");
X	support.radioValue = input.value === "t";
X
X	input.setAttribute("checked", "checked");
X
X	// #11217 - WebKit loses check when the name is after the checked attribute
X	input.setAttribute( "name", "t" );
X
X	div.appendChild( input );
X	fragment = document.createDocumentFragment();
X	fragment.appendChild( div.lastChild );
X
X	// WebKit doesn't clone checked state correctly in fragments
X	support.checkClone = fragment.cloneNode( true ).cloneNode( true ).lastChild.checked;
X
X	// Check if a disconnected checkbox will retain its checked
X	// value of true after appended to the DOM (IE6/7)
X	support.appendChecked = input.checked;
X
X	fragment.removeChild( input );
X	fragment.appendChild( div );
X
X	// Technique from Juriy Zaytsev
X	// http://perfectionkills.com/detecting-event-support-without-browser-sniffing/
X	// We only care about the case where non-standard event systems
X	// are used, namely in IE. Short-circuiting here helps us to
X	// avoid an eval call (in setAttribute) which can cause CSP
X	// to go haywire. See: https://developer.mozilla.org/en/Security/CSP
X	if ( div.attachEvent ) {
X		for ( i in {
X			submit: 1,
X			change: 1,
X			focusin: 1
X		}) {
X			eventName = "on" + i;
X			isSupported = ( eventName in div );
X			if ( !isSupported ) {
X				div.setAttribute( eventName, "return;" );
X				isSupported = ( typeof div[ eventName ] === "function" );
X			}
X			support[ i + "Bubbles" ] = isSupported;
X		}
X	}
X
X	fragment.removeChild( div );
X
X	// Null elements to avoid leaks in IE
X	fragment = select = opt = div = input = null;
X
X	// Run tests that need a body at doc ready
X	jQuery(function() {
X		var container, outer, inner, table, td, offsetSupport,
X			marginDiv, conMarginTop, style, html, positionTopLeftWidthHeight,
X			paddingMarginBorderVisibility, paddingMarginBorder,
X			body = document.getElementsByTagName("body")[0];
X
X		if ( !body ) {
X			// Return for frameset docs that don't have a body
X			return;
X		}
X
X		conMarginTop = 1;
X		paddingMarginBorder = "padding:0;margin:0;border:";
X		positionTopLeftWidthHeight = "position:absolute;top:0;left:0;width:1px;height:1px;";
X		paddingMarginBorderVisibility = paddingMarginBorder + "0;visibility:hidden;";
X		style = "style='" + positionTopLeftWidthHeight + paddingMarginBorder + "5px solid #000;";
X		html = "<div " + style + "display:block;'><div style='" + paddingMarginBorder + "0;display:block;overflow:hidden;'></div></div>" +
X			"<table " + style + "' cellpadding='0' cellspacing='0'>" +
X			"<tr><td></td></tr></table>";
X
X		container = document.createElement("div");
X		container.style.cssText = paddingMarginBorderVisibility + "width:0;height:0;position:static;top:0;margin-top:" + conMarginTop + "px";
X		body.insertBefore( container, body.firstChild );
X
X		// Construct the test element
X		div = document.createElement("div");
X		container.appendChild( div );
X
X		// Check if table cells still have offsetWidth/Height when they are set
X		// to display:none and there are still other visible table cells in a
X		// table row; if so, offsetWidth/Height are not reliable for use when
X		// determining if an element has been hidden directly using
X		// display:none (it is still safe to use offsets if a parent element is
X		// hidden; don safety goggles and see bug #4512 for more information).
X		// (only IE 8 fails this test)
X		div.innerHTML = "<table><tr><td style='" + paddingMarginBorder + "0;display:none'></td><td>t</td></tr></table>";
X		tds = div.getElementsByTagName( "td" );
X		isSupported = ( tds[ 0 ].offsetHeight === 0 );
X
X		tds[ 0 ].style.display = "";
X		tds[ 1 ].style.display = "none";
X
X		// Check if empty table cells still have offsetWidth/Height
X		// (IE <= 8 fail this test)
X		support.reliableHiddenOffsets = isSupported && ( tds[ 0 ].offsetHeight === 0 );
X
X		// Check if div with explicit width and no margin-right incorrectly
X		// gets computed margin-right based on width of container. For more
X		// info see bug #3333
X		// Fails in WebKit before Feb 2011 nightlies
X		// WebKit Bug 13343 - getComputedStyle returns wrong value for margin-right
X		if ( window.getComputedStyle ) {
X			div.innerHTML = "";
X			marginDiv = document.createElement( "div" );
X			marginDiv.style.width = "0";
X			marginDiv.style.marginRight = "0";
X			div.style.width = "2px";
X			div.appendChild( marginDiv );
X			support.reliableMarginRight =
X				( parseInt( ( window.getComputedStyle( marginDiv, null ) || { marginRight: 0 } ).marginRight, 10 ) || 0 ) === 0;
X		}
X
X		if ( typeof div.style.zoom !== "undefined" ) {
X			// Check if natively block-level elements act like inline-block
X			// elements when setting their display to 'inline' and giving
X			// them layout
X			// (IE < 8 does this)
X			div.innerHTML = "";
X			div.style.width = div.style.padding = "1px";
X			div.style.border = 0;
X			div.style.overflow = "hidden";
X			div.style.display = "inline";
X			div.style.zoom = 1;
X			support.inlineBlockNeedsLayout = ( div.offsetWidth === 3 );
X
X			// Check if elements with layout shrink-wrap their children
X			// (IE 6 does this)
X			div.style.display = "block";
X			div.style.overflow = "visible";
X			div.innerHTML = "<div style='width:5px;'></div>";
X			support.shrinkWrapBlocks = ( div.offsetWidth !== 3 );
X		}
X
X		div.style.cssText = positionTopLeftWidthHeight + paddingMarginBorderVisibility;
X		div.innerHTML = html;
X
X		outer = div.firstChild;
X		inner = outer.firstChild;
X		td = outer.nextSibling.firstChild.firstChild;
X
X		offsetSupport = {
X			doesNotAddBorder: ( inner.offsetTop !== 5 ),
X			doesAddBorderForTableAndCells: ( td.offsetTop === 5 )
X		};
X
X		inner.style.position = "fixed";
X		inner.style.top = "20px";
X
X		// safari subtracts parent border width here which is 5px
X		offsetSupport.fixedPosition = ( inner.offsetTop === 20 || inner.offsetTop === 15 );
X		inner.style.position = inner.style.top = "";
X
X		outer.style.overflow = "hidden";
X		outer.style.position = "relative";
X
X		offsetSupport.subtractsBorderForOverflowNotVisible = ( inner.offsetTop === -5 );
X		offsetSupport.doesNotIncludeMarginInBodyOffset = ( body.offsetTop !== conMarginTop );
X
X		if ( window.getComputedStyle ) {
X			div.style.marginTop = "1%";
X			support.pixelMargin = ( window.getComputedStyle( div, null ) || { marginTop: 0 } ).marginTop !== "1%";
X		}
X
X		if ( typeof container.style.zoom !== "undefined" ) {
X			container.style.zoom = 1;
X		}
X
X		body.removeChild( container );
X		marginDiv = div = container = null;
X
X		jQuery.extend( support, offsetSupport );
X	});
X
X	return support;
})();
X
X
X
X
var rbrace = /^(?:\{.*\}|\[.*\])$/,
X	rmultiDash = /([A-Z])/g;
X
jQuery.extend({
X	cache: {},
X
X	// Please use with caution
X	uuid: 0,
X
X	// Unique for each copy of jQuery on the page
X	// Non-digits removed to match rinlinejQuery
X	expando: "jQuery" + ( jQuery.fn.jquery + Math.random() ).replace( /\D/g, "" ),
X
X	// The following elements throw uncatchable exceptions if you
X	// attempt to add expando properties to them.
X	noData: {
X		"embed": true,
X		// Ban all objects except for Flash (which handle expandos)
X		"object": "clsid:D27CDB6E-AE6D-11cf-96B8-444553540000",
X		"applet": true
X	},
X
X	hasData: function( elem ) {
X		elem = elem.nodeType ? jQuery.cache[ elem[jQuery.expando] ] : elem[ jQuery.expando ];
X		return !!elem && !isEmptyDataObject( elem );
X	},
X
X	data: function( elem, name, data, pvt /* Internal Use Only */ ) {
X		if ( !jQuery.acceptData( elem ) ) {
X			return;
X		}
X
X		var privateCache, thisCache, ret,
X			internalKey = jQuery.expando,
X			getByName = typeof name === "string",
X
X			// We have to handle DOM nodes and JS objects differently because IE6-7
X			// can't GC object references properly across the DOM-JS boundary
X			isNode = elem.nodeType,
X
X			// Only DOM nodes need the global jQuery cache; JS object data is
X			// attached directly to the object so GC can occur automatically
X			cache = isNode ? jQuery.cache : elem,
X
X			// Only defining an ID for JS objects if its cache already exists allows
X			// the code to shortcut on the same path as a DOM node with no cache
X			id = isNode ? elem[ internalKey ] : elem[ internalKey ] && internalKey,
X			isEvents = name === "events";
X
X		// Avoid doing any more work than we need to when trying to get data on an
X		// object that has no data at all
X		if ( (!id || !cache[id] || (!isEvents && !pvt && !cache[id].data)) && getByName && data === undefined ) {
X			return;
X		}
X
X		if ( !id ) {
X			// Only DOM nodes need a new unique ID for each element since their data
X			// ends up in the global cache
X			if ( isNode ) {
X				elem[ internalKey ] = id = ++jQuery.uuid;
X			} else {
X				id = internalKey;
X			}
X		}
X
X		if ( !cache[ id ] ) {
X			cache[ id ] = {};
X
X			// Avoids exposing jQuery metadata on plain JS objects when the object
X			// is serialized using JSON.stringify
X			if ( !isNode ) {
X				cache[ id ].toJSON = jQuery.noop;
X			}
X		}
X
X		// An object can be passed to jQuery.data instead of a key/value pair; this gets
X		// shallow copied over onto the existing cache
X		if ( typeof name === "object" || typeof name === "function" ) {
X			if ( pvt ) {
X				cache[ id ] = jQuery.extend( cache[ id ], name );
X			} else {
X				cache[ id ].data = jQuery.extend( cache[ id ].data, name );
X			}
X		}
X
X		privateCache = thisCache = cache[ id ];
X
X		// jQuery data() is stored in a separate object inside the object's internal data
X		// cache in order to avoid key collisions between internal data and user-defined
X		// data.
X		if ( !pvt ) {
X			if ( !thisCache.data ) {
X				thisCache.data = {};
X			}
X
X			thisCache = thisCache.data;
X		}
X
X		if ( data !== undefined ) {
X			thisCache[ jQuery.camelCase( name ) ] = data;
X		}
X
X		// Users should not attempt to inspect the internal events object using jQuery.data,
X		// it is undocumented and subject to change. But does anyone listen? No.
X		if ( isEvents && !thisCache[ name ] ) {
X			return privateCache.events;
X		}
X
X		// Check for both converted-to-camel and non-converted data property names
X		// If a data property was specified
X		if ( getByName ) {
X
X			// First Try to find as-is property data
X			ret = thisCache[ name ];
X
X			// Test for null|undefined property data
X			if ( ret == null ) {
X
X				// Try to find the camelCased property
X				ret = thisCache[ jQuery.camelCase( name ) ];
X			}
X		} else {
X			ret = thisCache;
X		}
X
X		return ret;
X	},
X
X	removeData: function( elem, name, pvt /* Internal Use Only */ ) {
X		if ( !jQuery.acceptData( elem ) ) {
X			return;
X		}
X
X		var thisCache, i, l,
X
X			// Reference to internal data cache key
X			internalKey = jQuery.expando,
X
X			isNode = elem.nodeType,
X
X			// See jQuery.data for more information
X			cache = isNode ? jQuery.cache : elem,
X
X			// See jQuery.data for more information
X			id = isNode ? elem[ internalKey ] : internalKey;
X
X		// If there is already no cache entry for this object, there is no
X		// purpose in continuing
X		if ( !cache[ id ] ) {
X			return;
X		}
X
X		if ( name ) {
X
X			thisCache = pvt ? cache[ id ] : cache[ id ].data;
X
X			if ( thisCache ) {
X
X				// Support array or space separated string names for data keys
X				if ( !jQuery.isArray( name ) ) {
X
X					// try the string as a key before any manipulation
X					if ( name in thisCache ) {
X						name = [ name ];
X					} else {
X
X						// split the camel cased version by spaces unless a key with the spaces exists
X						name = jQuery.camelCase( name );
X						if ( name in thisCache ) {
X							name = [ name ];
X						} else {
X							name = name.split( " " );
X						}
X					}
X				}
X
X				for ( i = 0, l = name.length; i < l; i++ ) {
X					delete thisCache[ name[i] ];
X				}
X
X				// If there is no data left in the cache, we want to continue
X				// and let the cache object itself get destroyed
X				if ( !( pvt ? isEmptyDataObject : jQuery.isEmptyObject )( thisCache ) ) {
X					return;
X				}
X			}
X		}
X
X		// See jQuery.data for more information
X		if ( !pvt ) {
X			delete cache[ id ].data;
X
X			// Don't destroy the parent cache unless the internal data object
X			// had been the only thing left in it
X			if ( !isEmptyDataObject(cache[ id ]) ) {
X				return;
X			}
X		}
X
X		// Browsers that fail expando deletion also refuse to delete expandos on
X		// the window, but it will allow it on all other JS objects; other browsers
X		// don't care
X		// Ensure that `cache` is not a window object #10080
X		if ( jQuery.support.deleteExpando || !cache.setInterval ) {
X			delete cache[ id ];
X		} else {
X			cache[ id ] = null;
X		}
X
X		// We destroyed the cache and need to eliminate the expando on the node to avoid
X		// false lookups in the cache for entries that no longer exist
X		if ( isNode ) {
X			// IE does not allow us to delete expando properties from nodes,
X			// nor does it have a removeAttribute function on Document nodes;
X			// we must handle all of these cases
X			if ( jQuery.support.deleteExpando ) {
X				delete elem[ internalKey ];
X			} else if ( elem.removeAttribute ) {
X				elem.removeAttribute( internalKey );
X			} else {
X				elem[ internalKey ] = null;
X			}
X		}
X	},
X
X	// For internal use only.
X	_data: function( elem, name, data ) {
X		return jQuery.data( elem, name, data, true );
X	},
X
X	// A method for determining if a DOM node can handle the data expando
X	acceptData: function( elem ) {
X		if ( elem.nodeName ) {
X			var match = jQuery.noData[ elem.nodeName.toLowerCase() ];
X
X			if ( match ) {
X				return !(match === true || elem.getAttribute("classid") !== match);
X			}
X		}
X
X		return true;
X	}
});
X
jQuery.fn.extend({
X	data: function( key, value ) {
X		var parts, part, attr, name, l,
X			elem = this[0],
X			i = 0,
X			data = null;
X
X		// Gets all values
X		if ( key === undefined ) {
X			if ( this.length ) {
X				data = jQuery.data( elem );
X
X				if ( elem.nodeType === 1 && !jQuery._data( elem, "parsedAttrs" ) ) {
X					attr = elem.attributes;
X					for ( l = attr.length; i < l; i++ ) {
X						name = attr[i].name;
X
X						if ( name.indexOf( "data-" ) === 0 ) {
X							name = jQuery.camelCase( name.substring(5) );
X
X							dataAttr( elem, name, data[ name ] );
X						}
X					}
X					jQuery._data( elem, "parsedAttrs", true );
X				}
X			}
X
X			return data;
X		}
X
X		// Sets multiple values
X		if ( typeof key === "object" ) {
X			return this.each(function() {
X				jQuery.data( this, key );
X			});
X		}
X
X		parts = key.split( ".", 2 );
X		parts[1] = parts[1] ? "." + parts[1] : "";
X		part = parts[1] + "!";
X
X		return jQuery.access( this, function( value ) {
X
X			if ( value === undefined ) {
X				data = this.triggerHandler( "getData" + part, [ parts[0] ] );
X
X				// Try to fetch any internally stored data first
X				if ( data === undefined && elem ) {
X					data = jQuery.data( elem, key );
X					data = dataAttr( elem, key, data );
X				}
X
X				return data === undefined && parts[1] ?
X					this.data( parts[0] ) :
X					data;
X			}
X
X			parts[1] = value;
X			this.each(function() {
X				var self = jQuery( this );
X
X				self.triggerHandler( "setData" + part, parts );
X				jQuery.data( this, key, value );
X				self.triggerHandler( "changeData" + part, parts );
X			});
X		}, null, value, arguments.length > 1, null, false );
X	},
X
X	removeData: function( key ) {
X		return this.each(function() {
X			jQuery.removeData( this, key );
X		});
X	}
});
X
function dataAttr( elem, key, data ) {
X	// If nothing was found internally, try to fetch any
X	// data from the HTML5 data-* attribute
X	if ( data === undefined && elem.nodeType === 1 ) {
X
X		var name = "data-" + key.replace( rmultiDash, "-$1" ).toLowerCase();
X
X		data = elem.getAttribute( name );
X
X		if ( typeof data === "string" ) {
X			try {
X				data = data === "true" ? true :
X				data === "false" ? false :
X				data === "null" ? null :
X				jQuery.isNumeric( data ) ? +data :
X					rbrace.test( data ) ? jQuery.parseJSON( data ) :
X					data;
X			} catch( e ) {}
X
X			// Make sure we set the data so it isn't changed later
X			jQuery.data( elem, key, data );
X
X		} else {
X			data = undefined;
X		}
X	}
X
X	return data;
}
X
// checks a cache object for emptiness
function isEmptyDataObject( obj ) {
X	for ( var name in obj ) {
X
X		// if the public data object is empty, the private is still empty
X		if ( name === "data" && jQuery.isEmptyObject( obj[name] ) ) {
X			continue;
X		}
X		if ( name !== "toJSON" ) {
X			return false;
X		}
X	}
X
X	return true;
}
X
X
X
X
function handleQueueMarkDefer( elem, type, src ) {
X	var deferDataKey = type + "defer",
X		queueDataKey = type + "queue",
X		markDataKey = type + "mark",
X		defer = jQuery._data( elem, deferDataKey );
X	if ( defer &&
X		( src === "queue" || !jQuery._data(elem, queueDataKey) ) &&
X		( src === "mark" || !jQuery._data(elem, markDataKey) ) ) {
X		// Give room for hard-coded callbacks to fire first
X		// and eventually mark/queue something else on the element
X		setTimeout( function() {
X			if ( !jQuery._data( elem, queueDataKey ) &&
X				!jQuery._data( elem, markDataKey ) ) {
X				jQuery.removeData( elem, deferDataKey, true );
X				defer.fire();
X			}
X		}, 0 );
X	}
}
X
jQuery.extend({
X
X	_mark: function( elem, type ) {
X		if ( elem ) {
X			type = ( type || "fx" ) + "mark";
X			jQuery._data( elem, type, (jQuery._data( elem, type ) || 0) + 1 );
X		}
X	},
X
X	_unmark: function( force, elem, type ) {
X		if ( force !== true ) {
X			type = elem;
X			elem = force;
X			force = false;
X		}
X		if ( elem ) {
X			type = type || "fx";
X			var key = type + "mark",
X				count = force ? 0 : ( (jQuery._data( elem, key ) || 1) - 1 );
X			if ( count ) {
X				jQuery._data( elem, key, count );
X			} else {
X				jQuery.removeData( elem, key, true );
X				handleQueueMarkDefer( elem, type, "mark" );
X			}
X		}
X	},
X
X	queue: function( elem, type, data ) {
X		var q;
X		if ( elem ) {
X			type = ( type || "fx" ) + "queue";
X			q = jQuery._data( elem, type );
X
X			// Speed up dequeue by getting out quickly if this is just a lookup
X			if ( data ) {
X				if ( !q || jQuery.isArray(data) ) {
X					q = jQuery._data( elem, type, jQuery.makeArray(data) );
X				} else {
X					q.push( data );
X				}
X			}
X			return q || [];
X		}
X	},
X
X	dequeue: function( elem, type ) {
X		type = type || "fx";
X
X		var queue = jQuery.queue( elem, type ),
X			fn = queue.shift(),
X			hooks = {};
X
X		// If the fx queue is dequeued, always remove the progress sentinel
X		if ( fn === "inprogress" ) {
X			fn = queue.shift();
X		}
X
X		if ( fn ) {
X			// Add a progress sentinel to prevent the fx queue from being
X			// automatically dequeued
X			if ( type === "fx" ) {
X				queue.unshift( "inprogress" );
X			}
X
X			jQuery._data( elem, type + ".run", hooks );
X			fn.call( elem, function() {
X				jQuery.dequeue( elem, type );
X			}, hooks );
X		}
X
X		if ( !queue.length ) {
X			jQuery.removeData( elem, type + "queue " + type + ".run", true );
X			handleQueueMarkDefer( elem, type, "queue" );
X		}
X	}
});
X
jQuery.fn.extend({
X	queue: function( type, data ) {
X		var setter = 2;
X
X		if ( typeof type !== "string" ) {
X			data = type;
X			type = "fx";
X			setter--;
X		}
X
X		if ( arguments.length < setter ) {
X			return jQuery.queue( this[0], type );
X		}
X
X		return data === undefined ?
X			this :
X			this.each(function() {
X				var queue = jQuery.queue( this, type, data );
X
X				if ( type === "fx" && queue[0] !== "inprogress" ) {
X					jQuery.dequeue( this, type );
X				}
X			});
X	},
X	dequeue: function( type ) {
X		return this.each(function() {
X			jQuery.dequeue( this, type );
X		});
X	},
X	// Based off of the plugin by Clint Helfers, with permission.
X	// http://blindsignals.com/index.php/2009/07/jquery-delay/
X	delay: function( time, type ) {
X		time = jQuery.fx ? jQuery.fx.speeds[ time ] || time : time;
X		type = type || "fx";
X
X		return this.queue( type, function( next, hooks ) {
X			var timeout = setTimeout( next, time );
X			hooks.stop = function() {
X				clearTimeout( timeout );
X			};
X		});
X	},
X	clearQueue: function( type ) {
X		return this.queue( type || "fx", [] );
X	},
X	// Get a promise resolved when queues of a certain type
X	// are emptied (fx is the type by default)
X	promise: function( type, object ) {
X		if ( typeof type !== "string" ) {
X			object = type;
X			type = undefined;
X		}
X		type = type || "fx";
X		var defer = jQuery.Deferred(),
X			elements = this,
X			i = elements.length,
X			count = 1,
X			deferDataKey = type + "defer",
X			queueDataKey = type + "queue",
X			markDataKey = type + "mark",
X			tmp;
X		function resolve() {
X			if ( !( --count ) ) {
X				defer.resolveWith( elements, [ elements ] );
X			}
X		}
X		while( i-- ) {
X			if (( tmp = jQuery.data( elements[ i ], deferDataKey, undefined, true ) ||
X					( jQuery.data( elements[ i ], queueDataKey, undefined, true ) ||
X						jQuery.data( elements[ i ], markDataKey, undefined, true ) ) &&
X					jQuery.data( elements[ i ], deferDataKey, jQuery.Callbacks( "once memory" ), true ) )) {
X				count++;
X				tmp.add( resolve );
X			}
X		}
X		resolve();
X		return defer.promise( object );
X	}
});
X
X
X
X
var rclass = /[\n\t\r]/g,
X	rspace = /\s+/,
X	rreturn = /\r/g,
X	rtype = /^(?:button|input)$/i,
X	rfocusable = /^(?:button|input|object|select|textarea)$/i,
X	rclickable = /^a(?:rea)?$/i,
X	rboolean = /^(?:autofocus|autoplay|async|checked|controls|defer|disabled|hidden|loop|multiple|open|readonly|required|scoped|selected)$/i,
X	getSetAttribute = jQuery.support.getSetAttribute,
X	nodeHook, boolHook, fixSpecified;
X
jQuery.fn.extend({
X	attr: function( name, value ) {
X		return jQuery.access( this, jQuery.attr, name, value, arguments.length > 1 );
X	},
X
X	removeAttr: function( name ) {
X		return this.each(function() {
X			jQuery.removeAttr( this, name );
X		});
X	},
X
X	prop: function( name, value ) {
X		return jQuery.access( this, jQuery.prop, name, value, arguments.length > 1 );
X	},
X
X	removeProp: function( name ) {
X		name = jQuery.propFix[ name ] || name;
X		return this.each(function() {
X			// try/catch handles cases where IE balks (such as removing a property on window)
X			try {
X				this[ name ] = undefined;
X				delete this[ name ];
X			} catch( e ) {}
X		});
X	},
X
X	addClass: function( value ) {
X		var classNames, i, l, elem,
X			setClass, c, cl;
X
X		if ( jQuery.isFunction( value ) ) {
X			return this.each(function( j ) {
X				jQuery( this ).addClass( value.call(this, j, this.className) );
X			});
X		}
X
X		if ( value && typeof value === "string" ) {
X			classNames = value.split( rspace );
X
X			for ( i = 0, l = this.length; i < l; i++ ) {
X				elem = this[ i ];
X
X				if ( elem.nodeType === 1 ) {
X					if ( !elem.className && classNames.length === 1 ) {
X						elem.className = value;
X
X					} else {
X						setClass = " " + elem.className + " ";
X
X						for ( c = 0, cl = classNames.length; c < cl; c++ ) {
X							if ( !~setClass.indexOf( " " + classNames[ c ] + " " ) ) {
X								setClass += classNames[ c ] + " ";
X							}
X						}
X						elem.className = jQuery.trim( setClass );
X					}
X				}
X			}
X		}
X
X		return this;
X	},
X
X	removeClass: function( value ) {
X		var classNames, i, l, elem, className, c, cl;
X
X		if ( jQuery.isFunction( value ) ) {
X			return this.each(function( j ) {
X				jQuery( this ).removeClass( value.call(this, j, this.className) );
X			});
X		}
X
X		if ( (value && typeof value === "string") || value === undefined ) {
X			classNames = ( value || "" ).split( rspace );
X
X			for ( i = 0, l = this.length; i < l; i++ ) {
X				elem = this[ i ];
X
X				if ( elem.nodeType === 1 && elem.className ) {
X					if ( value ) {
X						className = (" " + elem.className + " ").replace( rclass, " " );
X						for ( c = 0, cl = classNames.length; c < cl; c++ ) {
X							className = className.replace(" " + classNames[ c ] + " ", " ");
X						}
X						elem.className = jQuery.trim( className );
X
X					} else {
X						elem.className = "";
X					}
X				}
X			}
X		}
X
X		return this;
X	},
X
X	toggleClass: function( value, stateVal ) {
X		var type = typeof value,
X			isBool = typeof stateVal === "boolean";
X
X		if ( jQuery.isFunction( value ) ) {
X			return this.each(function( i ) {
X				jQuery( this ).toggleClass( value.call(this, i, this.className, stateVal), stateVal );
X			});
X		}
X
X		return this.each(function() {
X			if ( type === "string" ) {
X				// toggle individual class names
X				var className,
X					i = 0,
X					self = jQuery( this ),
X					state = stateVal,
X					classNames = value.split( rspace );
X
X				while ( (className = classNames[ i++ ]) ) {
X					// check each className given, space seperated list
X					state = isBool ? state : !self.hasClass( className );
X					self[ state ? "addClass" : "removeClass" ]( className );
X				}
X
X			} else if ( type === "undefined" || type === "boolean" ) {
X				if ( this.className ) {
X					// store className if set
X					jQuery._data( this, "__className__", this.className );
X				}
X
X				// toggle whole className
X				this.className = this.className || value === false ? "" : jQuery._data( this, "__className__" ) || "";
X			}
X		});
X	},
X
X	hasClass: function( selector ) {
X		var className = " " + selector + " ",
X			i = 0,
X			l = this.length;
X		for ( ; i < l; i++ ) {
X			if ( this[i].nodeType === 1 && (" " + this[i].className + " ").replace(rclass, " ").indexOf( className ) > -1 ) {
X				return true;
X			}
X		}
X
X		return false;
X	},
X
X	val: function( value ) {
X		var hooks, ret, isFunction,
X			elem = this[0];
X
X		if ( !arguments.length ) {
X			if ( elem ) {
X				hooks = jQuery.valHooks[ elem.type ] || jQuery.valHooks[ elem.nodeName.toLowerCase() ];
X
X				if ( hooks && "get" in hooks && (ret = hooks.get( elem, "value" )) !== undefined ) {
X					return ret;
X				}
X
X				ret = elem.value;
X
X				return typeof ret === "string" ?
X					// handle most common string cases
X					ret.replace(rreturn, "") :
X					// handle cases where value is null/undef or number
X					ret == null ? "" : ret;
X			}
X
X			return;
X		}
X
X		isFunction = jQuery.isFunction( value );
X
X		return this.each(function( i ) {
X			var self = jQuery(this), val;
X
X			if ( this.nodeType !== 1 ) {
X				return;
X			}
X
X			if ( isFunction ) {
X				val = value.call( this, i, self.val() );
X			} else {
X				val = value;
X			}
X
X			// Treat null/undefined as ""; convert numbers to string
X			if ( val == null ) {
X				val = "";
X			} else if ( typeof val === "number" ) {
X				val += "";
X			} else if ( jQuery.isArray( val ) ) {
X				val = jQuery.map(val, function ( value ) {
X					return value == null ? "" : value + "";
X				});
X			}
X
X			hooks = jQuery.valHooks[ this.type ] || jQuery.valHooks[ this.nodeName.toLowerCase() ];
X
X			// If set returns undefined, fall back to normal setting
X			if ( !hooks || !("set" in hooks) || hooks.set( this, val, "value" ) === undefined ) {
X				this.value = val;
X			}
X		});
X	}
});
X
jQuery.extend({
X	valHooks: {
X		option: {
X			get: function( elem ) {
X				// attributes.value is undefined in Blackberry 4.7 but
X				// uses .value. See #6932
X				var val = elem.attributes.value;
X				return !val || val.specified ? elem.value : elem.text;
X			}
X		},
X		select: {
X			get: function( elem ) {
X				var value, i, max, option,
X					index = elem.selectedIndex,
X					values = [],
X					options = elem.options,
X					one = elem.type === "select-one";
X
X				// Nothing was selected
X				if ( index < 0 ) {
X					return null;
X				}
X
X				// Loop through all the selected options
X				i = one ? index : 0;
X				max = one ? index + 1 : options.length;
X				for ( ; i < max; i++ ) {
X					option = options[ i ];
X
X					// Don't return options that are disabled or in a disabled optgroup
X					if ( option.selected && (jQuery.support.optDisabled ? !option.disabled : option.getAttribute("disabled") === null) &&
X							(!option.parentNode.disabled || !jQuery.nodeName( option.parentNode, "optgroup" )) ) {
X
X						// Get the specific value for the option
X						value = jQuery( option ).val();
X
X						// We don't need an array for one selects
X						if ( one ) {
X							return value;
X						}
X
X						// Multi-Selects return an array
X						values.push( value );
X					}
X				}
X
X				// Fixes Bug #2551 -- select.val() broken in IE after form.reset()
X				if ( one && !values.length && options.length ) {
X					return jQuery( options[ index ] ).val();
X				}
X
X				return values;
X			},
X
X			set: function( elem, value ) {
X				var values = jQuery.makeArray( value );
X
X				jQuery(elem).find("option").each(function() {
X					this.selected = jQuery.inArray( jQuery(this).val(), values ) >= 0;
X				});
X
X				if ( !values.length ) {
X					elem.selectedIndex = -1;
X				}
X				return values;
X			}
X		}
X	},
X
X	attrFn: {
X		val: true,
X		css: true,
X		html: true,
X		text: true,
X		data: true,
X		width: true,
X		height: true,
X		offset: true
X	},
X
X	attr: function( elem, name, value, pass ) {
X		var ret, hooks, notxml,
X			nType = elem.nodeType;
X
X		// don't get/set attributes on text, comment and attribute nodes
X		if ( !elem || nType === 3 || nType === 8 || nType === 2 ) {
X			return;
X		}
X
X		if ( pass && name in jQuery.attrFn ) {
X			return jQuery( elem )[ name ]( value );
X		}
X
X		// Fallback to prop when attributes are not supported
X		if ( typeof elem.getAttribute === "undefined" ) {
X			return jQuery.prop( elem, name, value );
X		}
X
X		notxml = nType !== 1 || !jQuery.isXMLDoc( elem );
X
X		// All attributes are lowercase
X		// Grab necessary hook if one is defined
X		if ( notxml ) {
X			name = name.toLowerCase();
X			hooks = jQuery.attrHooks[ name ] || ( rboolean.test( name ) ? boolHook : nodeHook );
X		}
X
X		if ( value !== undefined ) {
X
X			if ( value === null ) {
X				jQuery.removeAttr( elem, name );
X				return;
X
X			} else if ( hooks && "set" in hooks && notxml && (ret = hooks.set( elem, value, name )) !== undefined ) {
X				return ret;
X
X			} else {
X				elem.setAttribute( name, "" + value );
X				return value;
X			}
X
X		} else if ( hooks && "get" in hooks && notxml && (ret = hooks.get( elem, name )) !== null ) {
X			return ret;
X
X		} else {
X
X			ret = elem.getAttribute( name );
X
X			// Non-existent attributes return null, we normalize to undefined
X			return ret === null ?
X				undefined :
X				ret;
X		}
X	},
X
X	removeAttr: function( elem, value ) {
X		var propName, attrNames, name, l, isBool,
X			i = 0;
X
X		if ( value && elem.nodeType === 1 ) {
X			attrNames = value.toLowerCase().split( rspace );
X			l = attrNames.length;
X
X			for ( ; i < l; i++ ) {
X				name = attrNames[ i ];
X
X				if ( name ) {
X					propName = jQuery.propFix[ name ] || name;
X					isBool = rboolean.test( name );
X
X					// See #9699 for explanation of this approach (setting first, then removal)
X					// Do not do this for boolean attributes (see #10870)
X					if ( !isBool ) {
X						jQuery.attr( elem, name, "" );
X					}
X					elem.removeAttribute( getSetAttribute ? name : propName );
X
X					// Set corresponding property to false for boolean attributes
X					if ( isBool && propName in elem ) {
X						elem[ propName ] = false;
X					}
X				}
X			}
X		}
X	},
X
X	attrHooks: {
X		type: {
X			set: function( elem, value ) {
X				// We can't allow the type property to be changed (since it causes problems in IE)
X				if ( rtype.test( elem.nodeName ) && elem.parentNode ) {
X					jQuery.error( "type property can't be changed" );
X				} else if ( !jQuery.support.radioValue && value === "radio" && jQuery.nodeName(elem, "input") ) {
X					// Setting the type on a radio button after the value resets the value in IE6-9
X					// Reset value to it's default in case type is set after value
X					// This is for element creation
X					var val = elem.value;
X					elem.setAttribute( "type", value );
X					if ( val ) {
X						elem.value = val;
X					}
X					return value;
X				}
X			}
X		},
X		// Use the value property for back compat
X		// Use the nodeHook for button elements in IE6/7 (#1954)
X		value: {
X			get: function( elem, name ) {
X				if ( nodeHook && jQuery.nodeName( elem, "button" ) ) {
X					return nodeHook.get( elem, name );
X				}
X				return name in elem ?
X					elem.value :
X					null;
X			},
X			set: function( elem, value, name ) {
X				if ( nodeHook && jQuery.nodeName( elem, "button" ) ) {
X					return nodeHook.set( elem, value, name );
X				}
X				// Does not return so that setAttribute is also used
X				elem.value = value;
X			}
X		}
X	},
X
X	propFix: {
X		tabindex: "tabIndex",
X		readonly: "readOnly",
X		"for": "htmlFor",
X		"class": "className",
X		maxlength: "maxLength",
X		cellspacing: "cellSpacing",
X		cellpadding: "cellPadding",
X		rowspan: "rowSpan",
X		colspan: "colSpan",
X		usemap: "useMap",
X		frameborder: "frameBorder",
X		contenteditable: "contentEditable"
X	},
X
X	prop: function( elem, name, value ) {
X		var ret, hooks, notxml,
X			nType = elem.nodeType;
X
X		// don't get/set properties on text, comment and attribute nodes
X		if ( !elem || nType === 3 || nType === 8 || nType === 2 ) {
X			return;
X		}
X
X		notxml = nType !== 1 || !jQuery.isXMLDoc( elem );
X
X		if ( notxml ) {
X			// Fix name and attach hooks
X			name = jQuery.propFix[ name ] || name;
X			hooks = jQuery.propHooks[ name ];
X		}
X
X		if ( value !== undefined ) {
X			if ( hooks && "set" in hooks && (ret = hooks.set( elem, value, name )) !== undefined ) {
X				return ret;
X
X			} else {
X				return ( elem[ name ] = value );
X			}
X
X		} else {
X			if ( hooks && "get" in hooks && (ret = hooks.get( elem, name )) !== null ) {
X				return ret;
X
X			} else {
X				return elem[ name ];
X			}
X		}
X	},
X
X	propHooks: {
X		tabIndex: {
X			get: function( elem ) {
X				// elem.tabIndex doesn't always return the correct value when it hasn't been explicitly set
X				// http://fluidproject.org/blog/2008/01/09/getting-setting-and-removing-tabindex-values-with-javascript/
X				var attributeNode = elem.getAttributeNode("tabindex");
X
X				return attributeNode && attributeNode.specified ?
X					parseInt( attributeNode.value, 10 ) :
X					rfocusable.test( elem.nodeName ) || rclickable.test( elem.nodeName ) && elem.href ?
X						0 :
X						undefined;
X			}
X		}
X	}
});
X
// Add the tabIndex propHook to attrHooks for back-compat (different case is intentional)
jQuery.attrHooks.tabindex = jQuery.propHooks.tabIndex;
X
// Hook for boolean attributes
boolHook = {
X	get: function( elem, name ) {
X		// Align boolean attributes with corresponding properties
X		// Fall back to attribute presence where some booleans are not supported
X		var attrNode,
X			property = jQuery.prop( elem, name );
X		return property === true || typeof property !== "boolean" && ( attrNode = elem.getAttributeNode(name) ) && attrNode.nodeValue !== false ?
X			name.toLowerCase() :
X			undefined;
X	},
X	set: function( elem, value, name ) {
X		var propName;
X		if ( value === false ) {
X			// Remove boolean attributes when set to false
X			jQuery.removeAttr( elem, name );
X		} else {
X			// value is true since we know at this point it's type boolean and not false
X			// Set boolean attributes to the same name and set the DOM property
X			propName = jQuery.propFix[ name ] || name;
X			if ( propName in elem ) {
X				// Only set the IDL specifically if it already exists on the element
X				elem[ propName ] = true;
X			}
X
X			elem.setAttribute( name, name.toLowerCase() );
X		}
X		return name;
X	}
};
X
// IE6/7 do not support getting/setting some attributes with get/setAttribute
if ( !getSetAttribute ) {
X
X	fixSpecified = {
X		name: true,
X		id: true,
X		coords: true
X	};
X
X	// Use this for any attribute in IE6/7
X	// This fixes almost every IE6/7 issue
X	nodeHook = jQuery.valHooks.button = {
X		get: function( elem, name ) {
X			var ret;
X			ret = elem.getAttributeNode( name );
X			return ret && ( fixSpecified[ name ] ? ret.nodeValue !== "" : ret.specified ) ?
X				ret.nodeValue :
X				undefined;
X		},
X		set: function( elem, value, name ) {
X			// Set the existing or create a new attribute node
X			var ret = elem.getAttributeNode( name );
X			if ( !ret ) {
X				ret = document.createAttribute( name );
X				elem.setAttributeNode( ret );
X			}
X			return ( ret.nodeValue = value + "" );
X		}
X	};
X
X	// Apply the nodeHook to tabindex
X	jQuery.attrHooks.tabindex.set = nodeHook.set;
X
X	// Set width and height to auto instead of 0 on empty string( Bug #8150 )
X	// This is for removals
X	jQuery.each([ "width", "height" ], function( i, name ) {
X		jQuery.attrHooks[ name ] = jQuery.extend( jQuery.attrHooks[ name ], {
X			set: function( elem, value ) {
X				if ( value === "" ) {
X					elem.setAttribute( name, "auto" );
X					return value;
X				}
X			}
X		});
X	});
X
X	// Set contenteditable to false on removals(#10429)
X	// Setting to empty string throws an error as an invalid value
X	jQuery.attrHooks.contenteditable = {
X		get: nodeHook.get,
X		set: function( elem, value, name ) {
X			if ( value === "" ) {
X				value = "false";
X			}
X			nodeHook.set( elem, value, name );
X		}
X	};
}
X
X
// Some attributes require a special call on IE
if ( !jQuery.support.hrefNormalized ) {
X	jQuery.each([ "href", "src", "width", "height" ], function( i, name ) {
X		jQuery.attrHooks[ name ] = jQuery.extend( jQuery.attrHooks[ name ], {
X			get: function( elem ) {
X				var ret = elem.getAttribute( name, 2 );
X				return ret === null ? undefined : ret;
X			}
X		});
X	});
}
X
if ( !jQuery.support.style ) {
X	jQuery.attrHooks.style = {
X		get: function( elem ) {
X			// Return undefined in the case of empty string
X			// Normalize to lowercase since IE uppercases css property names
X			return elem.style.cssText.toLowerCase() || undefined;
X		},
X		set: function( elem, value ) {
X			return ( elem.style.cssText = "" + value );
X		}
X	};
}
X
// Safari mis-reports the default selected property of an option
// Accessing the parent's selectedIndex property fixes it
if ( !jQuery.support.optSelected ) {
X	jQuery.propHooks.selected = jQuery.extend( jQuery.propHooks.selected, {
X		get: function( elem ) {
X			var parent = elem.parentNode;
X
X			if ( parent ) {
X				parent.selectedIndex;
X
X				// Make sure that it also works with optgroups, see #5701
X				if ( parent.parentNode ) {
X					parent.parentNode.selectedIndex;
X				}
X			}
X			return null;
X		}
X	});
}
X
// IE6/7 call enctype encoding
if ( !jQuery.support.enctype ) {
X	jQuery.propFix.enctype = "encoding";
}
X
// Radios and checkboxes getter/setter
if ( !jQuery.support.checkOn ) {
X	jQuery.each([ "radio", "checkbox" ], function() {
X		jQuery.valHooks[ this ] = {
X			get: function( elem ) {
X				// Handle the case where in Webkit "" is returned instead of "on" if a value isn't specified
X				return elem.getAttribute("value") === null ? "on" : elem.value;
X			}
X		};
X	});
}
jQuery.each([ "radio", "checkbox" ], function() {
X	jQuery.valHooks[ this ] = jQuery.extend( jQuery.valHooks[ this ], {
X		set: function( elem, value ) {
X			if ( jQuery.isArray( value ) ) {
X				return ( elem.checked = jQuery.inArray( jQuery(elem).val(), value ) >= 0 );
X			}
X		}
X	});
});
X
X
X
X
var rformElems = /^(?:textarea|input|select)$/i,
X	rtypenamespace = /^([^\.]*)?(?:\.(.+))?$/,
X	rhoverHack = /(?:^|\s)hover(\.\S+)?\b/,
X	rkeyEvent = /^key/,
X	rmouseEvent = /^(?:mouse|contextmenu)|click/,
X	rfocusMorph = /^(?:focusinfocus|focusoutblur)$/,
X	rquickIs = /^(\w*)(?:#([\w\-]+))?(?:\.([\w\-]+))?$/,
X	quickParse = function( selector ) {
X		var quick = rquickIs.exec( selector );
X		if ( quick ) {
X			//   0  1    2   3
X			// [ _, tag, id, class ]
X			quick[1] = ( quick[1] || "" ).toLowerCase();
X			quick[3] = quick[3] && new RegExp( "(?:^|\\s)" + quick[3] + "(?:\\s|$)" );
X		}
X		return quick;
X	},
X	quickIs = function( elem, m ) {
X		var attrs = elem.attributes || {};
X		return (
X			(!m[1] || elem.nodeName.toLowerCase() === m[1]) &&
X			(!m[2] || (attrs.id || {}).value === m[2]) &&
X			(!m[3] || m[3].test( (attrs[ "class" ] || {}).value ))
X		);
X	},
X	hoverHack = function( events ) {
X		return jQuery.event.special.hover ? events : events.replace( rhoverHack, "mouseenter$1 mouseleave$1" );
X	};
X
/*
X * Helper functions for managing events -- not part of the public interface.
X * Props to Dean Edwards' addEvent library for many of the ideas.
X */
jQuery.event = {
X
X	add: function( elem, types, handler, data, selector ) {
X
X		var elemData, eventHandle, events,
X			t, tns, type, namespaces, handleObj,
X			handleObjIn, quick, handlers, special;
X
X		// Don't attach events to noData or text/comment nodes (allow plain objects tho)
X		if ( elem.nodeType === 3 || elem.nodeType === 8 || !types || !handler || !(elemData = jQuery._data( elem )) ) {
X			return;
X		}
X
X		// Caller can pass in an object of custom data in lieu of the handler
X		if ( handler.handler ) {
X			handleObjIn = handler;
X			handler = handleObjIn.handler;
X			selector = handleObjIn.selector;
X		}
X
X		// Make sure that the handler has a unique ID, used to find/remove it later
X		if ( !handler.guid ) {
X			handler.guid = jQuery.guid++;
X		}
X
X		// Init the element's event structure and main handler, if this is the first
X		events = elemData.events;
X		if ( !events ) {
X			elemData.events = events = {};
X		}
X		eventHandle = elemData.handle;
X		if ( !eventHandle ) {
X			elemData.handle = eventHandle = function( e ) {
X				// Discard the second event of a jQuery.event.trigger() and
X				// when an event is called after a page has unloaded
X				return typeof jQuery !== "undefined" && (!e || jQuery.event.triggered !== e.type) ?
X					jQuery.event.dispatch.apply( eventHandle.elem, arguments ) :
X					undefined;
X			};
X			// Add elem as a property of the handle fn to prevent a memory leak with IE non-native events
X			eventHandle.elem = elem;
X		}
X
X		// Handle multiple events separated by a space
X		// jQuery(...).bind("mouseover mouseout", fn);
X		types = jQuery.trim( hoverHack(types) ).split( " " );
X		for ( t = 0; t < types.length; t++ ) {
X
X			tns = rtypenamespace.exec( types[t] ) || [];
X			type = tns[1];
X			namespaces = ( tns[2] || "" ).split( "." ).sort();
X
X			// If event changes its type, use the special event handlers for the changed type
X			special = jQuery.event.special[ type ] || {};
X
X			// If selector defined, determine special event api type, otherwise given type
X			type = ( selector ? special.delegateType : special.bindType ) || type;
X
X			// Update special based on newly reset type
X			special = jQuery.event.special[ type ] || {};
X
X			// handleObj is passed to all event handlers
X			handleObj = jQuery.extend({
X				type: type,
X				origType: tns[1],
X				data: data,
X				handler: handler,
X				guid: handler.guid,
X				selector: selector,
X				quick: selector && quickParse( selector ),
X				namespace: namespaces.join(".")
X			}, handleObjIn );
X
X			// Init the event handler queue if we're the first
X			handlers = events[ type ];
X			if ( !handlers ) {
X				handlers = events[ type ] = [];
X				handlers.delegateCount = 0;
X
X				// Only use addEventListener/attachEvent if the special events handler returns false
X				if ( !special.setup || special.setup.call( elem, data, namespaces, eventHandle ) === false ) {
X					// Bind the global event handler to the element
X					if ( elem.addEventListener ) {
X						elem.addEventListener( type, eventHandle, false );
X
X					} else if ( elem.attachEvent ) {
X						elem.attachEvent( "on" + type, eventHandle );
X					}
X				}
X			}
X
X			if ( special.add ) {
X				special.add.call( elem, handleObj );
X
X				if ( !handleObj.handler.guid ) {
X					handleObj.handler.guid = handler.guid;
X				}
X			}
X
X			// Add to the element's handler list, delegates in front
X			if ( selector ) {
X				handlers.splice( handlers.delegateCount++, 0, handleObj );
X			} else {
X				handlers.push( handleObj );
X			}
X
X			// Keep track of which events have ever been used, for event optimization
X			jQuery.event.global[ type ] = true;
X		}
X
X		// Nullify elem to prevent memory leaks in IE
X		elem = null;
X	},
X
X	global: {},
X
X	// Detach an event or set of events from an element
X	remove: function( elem, types, handler, selector, mappedTypes ) {
X
X		var elemData = jQuery.hasData( elem ) && jQuery._data( elem ),
X			t, tns, type, origType, namespaces, origCount,
X			j, events, special, handle, eventType, handleObj;
X
X		if ( !elemData || !(events = elemData.events) ) {
X			return;
X		}
X
X		// Once for each type.namespace in types; type may be omitted
X		types = jQuery.trim( hoverHack( types || "" ) ).split(" ");
X		for ( t = 0; t < types.length; t++ ) {
X			tns = rtypenamespace.exec( types[t] ) || [];
X			type = origType = tns[1];
X			namespaces = tns[2];
X
X			// Unbind all events (on this namespace, if provided) for the element
X			if ( !type ) {
X				for ( type in events ) {
X					jQuery.event.remove( elem, type + types[ t ], handler, selector, true );
X				}
X				continue;
X			}
X
X			special = jQuery.event.special[ type ] || {};
X			type = ( selector? special.delegateType : special.bindType ) || type;
X			eventType = events[ type ] || [];
X			origCount = eventType.length;
X			namespaces = namespaces ? new RegExp("(^|\\.)" + namespaces.split(".").sort().join("\\.(?:.*\\.)?") + "(\\.|$)") : null;
X
X			// Remove matching events
X			for ( j = 0; j < eventType.length; j++ ) {
X				handleObj = eventType[ j ];
X
X				if ( ( mappedTypes || origType === handleObj.origType ) &&
X					 ( !handler || handler.guid === handleObj.guid ) &&
X					 ( !namespaces || namespaces.test( handleObj.namespace ) ) &&
X					 ( !selector || selector === handleObj.selector || selector === "**" && handleObj.selector ) ) {
X					eventType.splice( j--, 1 );
X
X					if ( handleObj.selector ) {
X						eventType.delegateCount--;
X					}
X					if ( special.remove ) {
X						special.remove.call( elem, handleObj );
X					}
X				}
X			}
X
X			// Remove generic event handler if we removed something and no more handlers exist
X			// (avoids potential for endless recursion during removal of special event handlers)
X			if ( eventType.length === 0 && origCount !== eventType.length ) {
X				if ( !special.teardown || special.teardown.call( elem, namespaces ) === false ) {
X					jQuery.removeEvent( elem, type, elemData.handle );
X				}
X
X				delete events[ type ];
X			}
X		}
X
X		// Remove the expando if it's no longer used
X		if ( jQuery.isEmptyObject( events ) ) {
X			handle = elemData.handle;
X			if ( handle ) {
X				handle.elem = null;
X			}
X
X			// removeData also checks for emptiness and clears the expando if empty
X			// so use it instead of delete
X			jQuery.removeData( elem, [ "events", "handle" ], true );
X		}
X	},
X
X	// Events that are safe to short-circuit if no handlers are attached.
X	// Native DOM events should not be added, they may have inline handlers.
X	customEvent: {
X		"getData": true,
X		"setData": true,
X		"changeData": true
X	},
X
X	trigger: function( event, data, elem, onlyHandlers ) {
X		// Don't do events on text and comment nodes
X		if ( elem && (elem.nodeType === 3 || elem.nodeType === 8) ) {
X			return;
X		}
X
X		// Event object or event type
X		var type = event.type || event,
X			namespaces = [],
X			cache, exclusive, i, cur, old, ontype, special, handle, eventPath, bubbleType;
X
X		// focus/blur morphs to focusin/out; ensure we're not firing them right now
X		if ( rfocusMorph.test( type + jQuery.event.triggered ) ) {
X			return;
X		}
X
X		if ( type.indexOf( "!" ) >= 0 ) {
X			// Exclusive events trigger only for the exact event (no namespaces)
X			type = type.slice(0, -1);
X			exclusive = true;
X		}
X
X		if ( type.indexOf( "." ) >= 0 ) {
X			// Namespaced trigger; create a regexp to match event type in handle()
X			namespaces = type.split(".");
X			type = namespaces.shift();
X			namespaces.sort();
X		}
X
X		if ( (!elem || jQuery.event.customEvent[ type ]) && !jQuery.event.global[ type ] ) {
X			// No jQuery handlers for this event type, and it can't have inline handlers
X			return;
X		}
X
X		// Caller can pass in an Event, Object, or just an event type string
X		event = typeof event === "object" ?
X			// jQuery.Event object
X			event[ jQuery.expando ] ? event :
X			// Object literal
X			new jQuery.Event( type, event ) :
X			// Just the event type (string)
X			new jQuery.Event( type );
X
X		event.type = type;
X		event.isTrigger = true;
X		event.exclusive = exclusive;
X		event.namespace = namespaces.join( "." );
X		event.namespace_re = event.namespace? new RegExp("(^|\\.)" + namespaces.join("\\.(?:.*\\.)?") + "(\\.|$)") : null;
X		ontype = type.indexOf( ":" ) < 0 ? "on" + type : "";
X
X		// Handle a global trigger
X		if ( !elem ) {
X
X			// TODO: Stop taunting the data cache; remove global events and always attach to document
X			cache = jQuery.cache;
X			for ( i in cache ) {
X				if ( cache[ i ].events && cache[ i ].events[ type ] ) {
X					jQuery.event.trigger( event, data, cache[ i ].handle.elem, true );
X				}
X			}
X			return;
X		}
X
X		// Clean up the event in case it is being reused
X		event.result = undefined;
X		if ( !event.target ) {
X			event.target = elem;
X		}
X
X		// Clone any incoming data and prepend the event, creating the handler arg list
X		data = data != null ? jQuery.makeArray( data ) : [];
X		data.unshift( event );
X
X		// Allow special events to draw outside the lines
X		special = jQuery.event.special[ type ] || {};
X		if ( special.trigger && special.trigger.apply( elem, data ) === false ) {
X			return;
X		}
X
X		// Determine event propagation path in advance, per W3C events spec (#9951)
X		// Bubble up to document, then to window; watch for a global ownerDocument var (#9724)
X		eventPath = [[ elem, special.bindType || type ]];
X		if ( !onlyHandlers && !special.noBubble && !jQuery.isWindow( elem ) ) {
X
X			bubbleType = special.delegateType || type;
X			cur = rfocusMorph.test( bubbleType + type ) ? elem : elem.parentNode;
X			old = null;
X			for ( ; cur; cur = cur.parentNode ) {
X				eventPath.push([ cur, bubbleType ]);
X				old = cur;
X			}
X
X			// Only add window if we got to document (e.g., not plain obj or detached DOM)
X			if ( old && old === elem.ownerDocument ) {
X				eventPath.push([ old.defaultView || old.parentWindow || window, bubbleType ]);
X			}
X		}
X
X		// Fire handlers on the event path
X		for ( i = 0; i < eventPath.length && !event.isPropagationStopped(); i++ ) {
X
X			cur = eventPath[i][0];
X			event.type = eventPath[i][1];
X
X			handle = ( jQuery._data( cur, "events" ) || {} )[ event.type ] && jQuery._data( cur, "handle" );
X			if ( handle ) {
X				handle.apply( cur, data );
X			}
X			// Note that this is a bare JS function and not a jQuery handler
X			handle = ontype && cur[ ontype ];
X			if ( handle && jQuery.acceptData( cur ) && handle.apply( cur, data ) === false ) {
X				event.preventDefault();
X			}
X		}
X		event.type = type;
X
X		// If nobody prevented the default action, do it now
X		if ( !onlyHandlers && !event.isDefaultPrevented() ) {
X
X			if ( (!special._default || special._default.apply( elem.ownerDocument, data ) === false) &&
X				!(type === "click" && jQuery.nodeName( elem, "a" )) && jQuery.acceptData( elem ) ) {
X
X				// Call a native DOM method on the target with the same name name as the event.
X				// Can't use an .isFunction() check here because IE6/7 fails that test.
X				// Don't do default actions on window, that's where global variables be (#6170)
X				// IE<9 dies on focus/blur to hidden element (#1486)
X				if ( ontype && elem[ type ] && ((type !== "focus" && type !== "blur") || event.target.offsetWidth !== 0) && !jQuery.isWindow( elem ) ) {
X
X					// Don't re-trigger an onFOO event when we call its FOO() method
X					old = elem[ ontype ];
X
X					if ( old ) {
X						elem[ ontype ] = null;
X					}
X
X					// Prevent re-triggering of the same event, since we already bubbled it above
X					jQuery.event.triggered = type;
X					elem[ type ]();
X					jQuery.event.triggered = undefined;
X
X					if ( old ) {
X						elem[ ontype ] = old;
X					}
X				}
X			}
X		}
X
X		return event.result;
X	},
X
X	dispatch: function( event ) {
X
X		// Make a writable jQuery.Event from the native event object
X		event = jQuery.event.fix( event || window.event );
X
X		var handlers = ( (jQuery._data( this, "events" ) || {} )[ event.type ] || []),
X			delegateCount = handlers.delegateCount,
X			args = [].slice.call( arguments, 0 ),
X			run_all = !event.exclusive && !event.namespace,
X			special = jQuery.event.special[ event.type ] || {},
X			handlerQueue = [],
X			i, j, cur, jqcur, ret, selMatch, matched, matches, handleObj, sel, related;
X
X		// Use the fix-ed jQuery.Event rather than the (read-only) native event
X		args[0] = event;
X		event.delegateTarget = this;
X
X		// Call the preDispatch hook for the mapped type, and let it bail if desired
X		if ( special.preDispatch && special.preDispatch.call( this, event ) === false ) {
X			return;
X		}
X
X		// Determine handlers that should run if there are delegated events
X		// Avoid non-left-click bubbling in Firefox (#3861)
X		if ( delegateCount && !(event.button && event.type === "click") ) {
X
X			// Pregenerate a single jQuery object for reuse with .is()
X			jqcur = jQuery(this);
X			jqcur.context = this.ownerDocument || this;
X
X			for ( cur = event.target; cur != this; cur = cur.parentNode || this ) {
X
X				// Don't process events on disabled elements (#6911, #8165)
X				if ( cur.disabled !== true ) {
X					selMatch = {};
X					matches = [];
X					jqcur[0] = cur;
X					for ( i = 0; i < delegateCount; i++ ) {
X						handleObj = handlers[ i ];
X						sel = handleObj.selector;
X
X						if ( selMatch[ sel ] === undefined ) {
X							selMatch[ sel ] = (
X								handleObj.quick ? quickIs( cur, handleObj.quick ) : jqcur.is( sel )
X							);
X						}
X						if ( selMatch[ sel ] ) {
X							matches.push( handleObj );
X						}
X					}
X					if ( matches.length ) {
X						handlerQueue.push({ elem: cur, matches: matches });
X					}
X				}
X			}
X		}
X
X		// Add the remaining (directly-bound) handlers
X		if ( handlers.length > delegateCount ) {
X			handlerQueue.push({ elem: this, matches: handlers.slice( delegateCount ) });
X		}
X
X		// Run delegates first; they may want to stop propagation beneath us
X		for ( i = 0; i < handlerQueue.length && !event.isPropagationStopped(); i++ ) {
X			matched = handlerQueue[ i ];
X			event.currentTarget = matched.elem;
X
X			for ( j = 0; j < matched.matches.length && !event.isImmediatePropagationStopped(); j++ ) {
X				handleObj = matched.matches[ j ];
X
X				// Triggered event must either 1) be non-exclusive and have no namespace, or
X				// 2) have namespace(s) a subset or equal to those in the bound event (both can have no namespace).
X				if ( run_all || (!event.namespace && !handleObj.namespace) || event.namespace_re && event.namespace_re.test( handleObj.namespace ) ) {
X
X					event.data = handleObj.data;
X					event.handleObj = handleObj;
X
X					ret = ( (jQuery.event.special[ handleObj.origType ] || {}).handle || handleObj.handler )
X							.apply( matched.elem, args );
X
X					if ( ret !== undefined ) {
X						event.result = ret;
X						if ( ret === false ) {
X							event.preventDefault();
X							event.stopPropagation();
X						}
X					}
X				}
X			}
X		}
X
X		// Call the postDispatch hook for the mapped type
X		if ( special.postDispatch ) {
X			special.postDispatch.call( this, event );
X		}
X
X		return event.result;
X	},
X
X	// Includes some event props shared by KeyEvent and MouseEvent
X	// *** attrChange attrName relatedNode srcElement  are not normalized, non-W3C, deprecated, will be removed in 1.8 ***
X	props: "attrChange attrName relatedNode srcElement altKey bubbles cancelable ctrlKey currentTarget eventPhase metaKey relatedTarget shiftKey target timeStamp view which".split(" "),
X
X	fixHooks: {},
X
X	keyHooks: {
X		props: "char charCode key keyCode".split(" "),
X		filter: function( event, original ) {
X
X			// Add which for key events
X			if ( event.which == null ) {
X				event.which = original.charCode != null ? original.charCode : original.keyCode;
X			}
X
X			return event;
X		}
X	},
X
X	mouseHooks: {
X		props: "button buttons clientX clientY fromElement offsetX offsetY pageX pageY screenX screenY toElement".split(" "),
X		filter: function( event, original ) {
X			var eventDoc, doc, body,
X				button = original.button,
X				fromElement = original.fromElement;
X
X			// Calculate pageX/Y if missing and clientX/Y available
X			if ( event.pageX == null && original.clientX != null ) {
X				eventDoc = event.target.ownerDocument || document;
X				doc = eventDoc.documentElement;
X				body = eventDoc.body;
X
X				event.pageX = original.clientX + ( doc && doc.scrollLeft || body && body.scrollLeft || 0 ) - ( doc && doc.clientLeft || body && body.clientLeft || 0 );
X				event.pageY = original.clientY + ( doc && doc.scrollTop  || body && body.scrollTop  || 0 ) - ( doc && doc.clientTop  || body && body.clientTop  || 0 );
X			}
X
X			// Add relatedTarget, if necessary
X			if ( !event.relatedTarget && fromElement ) {
X				event.relatedTarget = fromElement === event.target ? original.toElement : fromElement;
X			}
X
X			// Add which for click: 1 === left; 2 === middle; 3 === right
X			// Note: button is not normalized, so don't use it
X			if ( !event.which && button !== undefined ) {
X				event.which = ( button & 1 ? 1 : ( button & 2 ? 3 : ( button & 4 ? 2 : 0 ) ) );
X			}
X
X			return event;
X		}
X	},
X
X	fix: function( event ) {
X		if ( event[ jQuery.expando ] ) {
X			return event;
X		}
X
X		// Create a writable copy of the event object and normalize some properties
X		var i, prop,
X			originalEvent = event,
X			fixHook = jQuery.event.fixHooks[ event.type ] || {},
X			copy = fixHook.props ? this.props.concat( fixHook.props ) : this.props;
X
X		event = jQuery.Event( originalEvent );
X
X		for ( i = copy.length; i; ) {
X			prop = copy[ --i ];
X			event[ prop ] = originalEvent[ prop ];
X		}
X
X		// Fix target property, if necessary (#1925, IE 6/7/8 & Safari2)
X		if ( !event.target ) {
X			event.target = originalEvent.srcElement || document;
X		}
X
X		// Target should not be a text node (#504, Safari)
X		if ( event.target.nodeType === 3 ) {
X			event.target = event.target.parentNode;
X		}
X
X		// For mouse/key events; add metaKey if it's not there (#3368, IE6/7/8)
X		if ( event.metaKey === undefined ) {
X			event.metaKey = event.ctrlKey;
X		}
X
X		return fixHook.filter? fixHook.filter( event, originalEvent ) : event;
X	},
X
X	special: {
X		ready: {
X			// Make sure the ready event is setup
X			setup: jQuery.bindReady
X		},
X
X		load: {
X			// Prevent triggered image.load events from bubbling to window.load
X			noBubble: true
X		},
X
X		focus: {
X			delegateType: "focusin"
X		},
X		blur: {
X			delegateType: "focusout"
X		},
X
X		beforeunload: {
X			setup: function( data, namespaces, eventHandle ) {
X				// We only want to do this special case on windows
X				if ( jQuery.isWindow( this ) ) {
X					this.onbeforeunload = eventHandle;
X				}
X			},
X
X			teardown: function( namespaces, eventHandle ) {
X				if ( this.onbeforeunload === eventHandle ) {
X					this.onbeforeunload = null;
X				}
X			}
X		}
X	},
X
X	simulate: function( type, elem, event, bubble ) {
X		// Piggyback on a donor event to simulate a different one.
X		// Fake originalEvent to avoid donor's stopPropagation, but if the
X		// simulated event prevents default then we do the same on the donor.
X		var e = jQuery.extend(
X			new jQuery.Event(),
X			event,
X			{ type: type,
X				isSimulated: true,
X				originalEvent: {}
X			}
X		);
X		if ( bubble ) {
X			jQuery.event.trigger( e, null, elem );
X		} else {
X			jQuery.event.dispatch.call( elem, e );
X		}
X		if ( e.isDefaultPrevented() ) {
X			event.preventDefault();
X		}
X	}
};
X
// Some plugins are using, but it's undocumented/deprecated and will be removed.
// The 1.7 special event interface should provide all the hooks needed now.
jQuery.event.handle = jQuery.event.dispatch;
X
jQuery.removeEvent = document.removeEventListener ?
X	function( elem, type, handle ) {
X		if ( elem.removeEventListener ) {
X			elem.removeEventListener( type, handle, false );
X		}
X	} :
X	function( elem, type, handle ) {
X		if ( elem.detachEvent ) {
X			elem.detachEvent( "on" + type, handle );
X		}
X	};
X
jQuery.Event = function( src, props ) {
X	// Allow instantiation without the 'new' keyword
X	if ( !(this instanceof jQuery.Event) ) {
X		return new jQuery.Event( src, props );
X	}
X
X	// Event object
X	if ( src && src.type ) {
X		this.originalEvent = src;
X		this.type = src.type;
X
X		// Events bubbling up the document may have been marked as prevented
X		// by a handler lower down the tree; reflect the correct value.
X		this.isDefaultPrevented = ( src.defaultPrevented || src.returnValue === false ||
X			src.getPreventDefault && src.getPreventDefault() ) ? returnTrue : returnFalse;
X
X	// Event type
X	} else {
X		this.type = src;
X	}
X
X	// Put explicitly provided properties onto the event object
X	if ( props ) {
X		jQuery.extend( this, props );
X	}
X
X	// Create a timestamp if incoming event doesn't have one
X	this.timeStamp = src && src.timeStamp || jQuery.now();
X
X	// Mark it as fixed
X	this[ jQuery.expando ] = true;
};
X
function returnFalse() {
X	return false;
}
function returnTrue() {
X	return true;
}
X
// jQuery.Event is based on DOM3 Events as specified by the ECMAScript Language Binding
// http://www.w3.org/TR/2003/WD-DOM-Level-3-Events-20030331/ecma-script-binding.html
jQuery.Event.prototype = {
X	preventDefault: function() {
X		this.isDefaultPrevented = returnTrue;
X
X		var e = this.originalEvent;
X		if ( !e ) {
X			return;
X		}
X
X		// if preventDefault exists run it on the original event
X		if ( e.preventDefault ) {
X			e.preventDefault();
X
X		// otherwise set the returnValue property of the original event to false (IE)
X		} else {
X			e.returnValue = false;
X		}
X	},
X	stopPropagation: function() {
X		this.isPropagationStopped = returnTrue;
X
X		var e = this.originalEvent;
X		if ( !e ) {
X			return;
X		}
X		// if stopPropagation exists run it on the original event
X		if ( e.stopPropagation ) {
X			e.stopPropagation();
X		}
X		// otherwise set the cancelBubble property of the original event to true (IE)
X		e.cancelBubble = true;
X	},
X	stopImmediatePropagation: function() {
X		this.isImmediatePropagationStopped = returnTrue;
X		this.stopPropagation();
X	},
X	isDefaultPrevented: returnFalse,
X	isPropagationStopped: returnFalse,
X	isImmediatePropagationStopped: returnFalse
};
X
// Create mouseenter/leave events using mouseover/out and event-time checks
jQuery.each({
X	mouseenter: "mouseover",
X	mouseleave: "mouseout"
}, function( orig, fix ) {
X	jQuery.event.special[ orig ] = {
X		delegateType: fix,
X		bindType: fix,
X
X		handle: function( event ) {
X			var target = this,
X				related = event.relatedTarget,
X				handleObj = event.handleObj,
X				selector = handleObj.selector,
X				ret;
X
X			// For mousenter/leave call the handler if related is outside the target.
X			// NB: No relatedTarget if the mouse left/entered the browser window
X			if ( !related || (related !== target && !jQuery.contains( target, related )) ) {
X				event.type = handleObj.origType;
X				ret = handleObj.handler.apply( this, arguments );
X				event.type = fix;
X			}
X			return ret;
X		}
X	};
});
X
// IE submit delegation
if ( !jQuery.support.submitBubbles ) {
X
X	jQuery.event.special.submit = {
X		setup: function() {
X			// Only need this for delegated form submit events
X			if ( jQuery.nodeName( this, "form" ) ) {
X				return false;
X			}
X
X			// Lazy-add a submit handler when a descendant form may potentially be submitted
X			jQuery.event.add( this, "click._submit keypress._submit", function( e ) {
X				// Node name check avoids a VML-related crash in IE (#9807)
X				var elem = e.target,
X					form = jQuery.nodeName( elem, "input" ) || jQuery.nodeName( elem, "button" ) ? elem.form : undefined;
X				if ( form && !form._submit_attached ) {
X					jQuery.event.add( form, "submit._submit", function( event ) {
X						event._submit_bubble = true;
X					});
X					form._submit_attached = true;
X				}
X			});
X			// return undefined since we don't need an event listener
X		},
X		
X		postDispatch: function( event ) {
X			// If form was submitted by the user, bubble the event up the tree
X			if ( event._submit_bubble ) {
X				delete event._submit_bubble;
X				if ( this.parentNode && !event.isTrigger ) {
X					jQuery.event.simulate( "submit", this.parentNode, event, true );
X				}
X			}
X		},
X
X		teardown: function() {
X			// Only need this for delegated form submit events
X			if ( jQuery.nodeName( this, "form" ) ) {
X				return false;
X			}
X
X			// Remove delegated handlers; cleanData eventually reaps submit handlers attached above
X			jQuery.event.remove( this, "._submit" );
X		}
X	};
}
X
// IE change delegation and checkbox/radio fix
if ( !jQuery.support.changeBubbles ) {
X
X	jQuery.event.special.change = {
X
X		setup: function() {
X
X			if ( rformElems.test( this.nodeName ) ) {
X				// IE doesn't fire change on a check/radio until blur; trigger it on click
X				// after a propertychange. Eat the blur-change in special.change.handle.
X				// This still fires onchange a second time for check/radio after blur.
X				if ( this.type === "checkbox" || this.type === "radio" ) {
X					jQuery.event.add( this, "propertychange._change", function( event ) {
X						if ( event.originalEvent.propertyName === "checked" ) {
X							this._just_changed = true;
X						}
X					});
X					jQuery.event.add( this, "click._change", function( event ) {
X						if ( this._just_changed && !event.isTrigger ) {
X							this._just_changed = false;
X							jQuery.event.simulate( "change", this, event, true );
X						}
X					});
X				}
X				return false;
X			}
X			// Delegated event; lazy-add a change handler on descendant inputs
X			jQuery.event.add( this, "beforeactivate._change", function( e ) {
X				var elem = e.target;
X
X				if ( rformElems.test( elem.nodeName ) && !elem._change_attached ) {
X					jQuery.event.add( elem, "change._change", function( event ) {
X						if ( this.parentNode && !event.isSimulated && !event.isTrigger ) {
X							jQuery.event.simulate( "change", this.parentNode, event, true );
X						}
X					});
X					elem._change_attached = true;
X				}
X			});
X		},
X
X		handle: function( event ) {
X			var elem = event.target;
X
X			// Swallow native change events from checkbox/radio, we already triggered them above
X			if ( this !== elem || event.isSimulated || event.isTrigger || (elem.type !== "radio" && elem.type !== "checkbox") ) {
X				return event.handleObj.handler.apply( this, arguments );
X			}
X		},
X
X		teardown: function() {
X			jQuery.event.remove( this, "._change" );
X
X			return rformElems.test( this.nodeName );
X		}
X	};
}
X
// Create "bubbling" focus and blur events
if ( !jQuery.support.focusinBubbles ) {
X	jQuery.each({ focus: "focusin", blur: "focusout" }, function( orig, fix ) {
X
X		// Attach a single capturing handler while someone wants focusin/focusout
X		var attaches = 0,
X			handler = function( event ) {
X				jQuery.event.simulate( fix, event.target, jQuery.event.fix( event ), true );
X			};
X
X		jQuery.event.special[ fix ] = {
X			setup: function() {
X				if ( attaches++ === 0 ) {
X					document.addEventListener( orig, handler, true );
X				}
X			},
X			teardown: function() {
X				if ( --attaches === 0 ) {
X					document.removeEventListener( orig, handler, true );
X				}
X			}
X		};
X	});
}
X
jQuery.fn.extend({
X
X	on: function( types, selector, data, fn, /*INTERNAL*/ one ) {
X		var origFn, type;
X
X		// Types can be a map of types/handlers
X		if ( typeof types === "object" ) {
X			// ( types-Object, selector, data )
X			if ( typeof selector !== "string" ) { // && selector != null
X				// ( types-Object, data )
X				data = data || selector;
X				selector = undefined;
X			}
X			for ( type in types ) {
X				this.on( type, selector, data, types[ type ], one );
X			}
X			return this;
X		}
X
X		if ( data == null && fn == null ) {
X			// ( types, fn )
X			fn = selector;
X			data = selector = undefined;
X		} else if ( fn == null ) {
X			if ( typeof selector === "string" ) {
X				// ( types, selector, fn )
X				fn = data;
X				data = undefined;
X			} else {
X				// ( types, data, fn )
X				fn = data;
X				data = selector;
X				selector = undefined;
X			}
X		}
X		if ( fn === false ) {
X			fn = returnFalse;
X		} else if ( !fn ) {
X			return this;
X		}
X
X		if ( one === 1 ) {
X			origFn = fn;
X			fn = function( event ) {
X				// Can use an empty set, since event contains the info
X				jQuery().off( event );
X				return origFn.apply( this, arguments );
X			};
X			// Use same guid so caller can remove using origFn
X			fn.guid = origFn.guid || ( origFn.guid = jQuery.guid++ );
X		}
X		return this.each( function() {
X			jQuery.event.add( this, types, fn, data, selector );
X		});
X	},
X	one: function( types, selector, data, fn ) {
X		return this.on( types, selector, data, fn, 1 );
X	},
X	off: function( types, selector, fn ) {
X		if ( types && types.preventDefault && types.handleObj ) {
X			// ( event )  dispatched jQuery.Event
X			var handleObj = types.handleObj;
X			jQuery( types.delegateTarget ).off(
X				handleObj.namespace ? handleObj.origType + "." + handleObj.namespace : handleObj.origType,
X				handleObj.selector,
X				handleObj.handler
X			);
X			return this;
X		}
X		if ( typeof types === "object" ) {
X			// ( types-object [, selector] )
X			for ( var type in types ) {
X				this.off( type, selector, types[ type ] );
X			}
X			return this;
X		}
X		if ( selector === false || typeof selector === "function" ) {
X			// ( types [, fn] )
X			fn = selector;
X			selector = undefined;
X		}
X		if ( fn === false ) {
X			fn = returnFalse;
X		}
X		return this.each(function() {
X			jQuery.event.remove( this, types, fn, selector );
X		});
X	},
X
X	bind: function( types, data, fn ) {
X		return this.on( types, null, data, fn );
X	},
X	unbind: function( types, fn ) {
X		return this.off( types, null, fn );
X	},
X
X	live: function( types, data, fn ) {
X		jQuery( this.context ).on( types, this.selector, data, fn );
X		return this;
X	},
X	die: function( types, fn ) {
X		jQuery( this.context ).off( types, this.selector || "**", fn );
X		return this;
X	},
X
X	delegate: function( selector, types, data, fn ) {
X		return this.on( types, selector, data, fn );
X	},
X	undelegate: function( selector, types, fn ) {
X		// ( namespace ) or ( selector, types [, fn] )
X		return arguments.length == 1? this.off( selector, "**" ) : this.off( types, selector, fn );
X	},
X
X	trigger: function( type, data ) {
X		return this.each(function() {
X			jQuery.event.trigger( type, data, this );
X		});
X	},
X	triggerHandler: function( type, data ) {
X		if ( this[0] ) {
X			return jQuery.event.trigger( type, data, this[0], true );
X		}
X	},
X
X	toggle: function( fn ) {
X		// Save reference to arguments for access in closure
X		var args = arguments,
X			guid = fn.guid || jQuery.guid++,
X			i = 0,
X			toggler = function( event ) {
X				// Figure out which function to execute
X				var lastToggle = ( jQuery._data( this, "lastToggle" + fn.guid ) || 0 ) % i;
X				jQuery._data( this, "lastToggle" + fn.guid, lastToggle + 1 );
X
X				// Make sure that clicks stop
X				event.preventDefault();
X
X				// and execute the function
X				return args[ lastToggle ].apply( this, arguments ) || false;
X			};
X
X		// link all the functions, so any of them can unbind this click handler
X		toggler.guid = guid;
X		while ( i < args.length ) {
X			args[ i++ ].guid = guid;
X		}
X
X		return this.click( toggler );
X	},
X
X	hover: function( fnOver, fnOut ) {
X		return this.mouseenter( fnOver ).mouseleave( fnOut || fnOver );
X	}
});
X
jQuery.each( ("blur focus focusin focusout load resize scroll unload click dblclick " +
X	"mousedown mouseup mousemove mouseover mouseout mouseenter mouseleave " +
X	"change select submit keydown keypress keyup error contextmenu").split(" "), function( i, name ) {
X
X	// Handle event binding
X	jQuery.fn[ name ] = function( data, fn ) {
X		if ( fn == null ) {
X			fn = data;
X			data = null;
X		}
X
X		return arguments.length > 0 ?
X			this.on( name, null, data, fn ) :
X			this.trigger( name );
X	};
X
X	if ( jQuery.attrFn ) {
X		jQuery.attrFn[ name ] = true;
X	}
X
X	if ( rkeyEvent.test( name ) ) {
X		jQuery.event.fixHooks[ name ] = jQuery.event.keyHooks;
X	}
X
X	if ( rmouseEvent.test( name ) ) {
X		jQuery.event.fixHooks[ name ] = jQuery.event.mouseHooks;
X	}
});
X
X
X
/*!
X * Sizzle CSS Selector Engine
X *  Copyright 2011, The Dojo Foundation
X *  Released under the MIT, BSD, and GPL Licenses.
X *  More information: http://sizzlejs.com/
X */
(function(){
X
var chunker = /((?:\((?:\([^()]+\)|[^()]+)+\)|\[(?:\[[^\[\]]*\]|['"][^'"]*['"]|[^\[\]'"]+)+\]|\\.|[^ >+~,(\[\\]+)+|[>+~])(\s*,\s*)?((?:.|\r|\n)*)/g,
X	expando = "sizcache" + (Math.random() + '').replace('.', ''),
X	done = 0,
X	toString = Object.prototype.toString,
X	hasDuplicate = false,
X	baseHasDuplicate = true,
X	rBackslash = /\\/g,
X	rReturn = /\r\n/g,
X	rNonWord = /\W/;
X
// Here we check if the JavaScript engine is using some sort of
// optimization where it does not always call our comparision
// function. If that is the case, discard the hasDuplicate value.
//   Thus far that includes Google Chrome.
[0, 0].sort(function() {
X	baseHasDuplicate = false;
X	return 0;
});
X
var Sizzle = function( selector, context, results, seed ) {
X	results = results || [];
X	context = context || document;
X
X	var origContext = context;
X
X	if ( context.nodeType !== 1 && context.nodeType !== 9 ) {
X		return [];
X	}
X
X	if ( !selector || typeof selector !== "string" ) {
X		return results;
X	}
X
X	var m, set, checkSet, extra, ret, cur, pop, i,
X		prune = true,
X		contextXML = Sizzle.isXML( context ),
X		parts = [],
X		soFar = selector;
X
X	// Reset the position of the chunker regexp (start from head)
X	do {
X		chunker.exec( "" );
X		m = chunker.exec( soFar );
X
X		if ( m ) {
X			soFar = m[3];
X
X			parts.push( m[1] );
X
X			if ( m[2] ) {
X				extra = m[3];
X				break;
X			}
X		}
X	} while ( m );
X
X	if ( parts.length > 1 && origPOS.exec( selector ) ) {
X
X		if ( parts.length === 2 && Expr.relative[ parts[0] ] ) {
X			set = posProcess( parts[0] + parts[1], context, seed );
X
X		} else {
X			set = Expr.relative[ parts[0] ] ?
X				[ context ] :
X				Sizzle( parts.shift(), context );
X
X			while ( parts.length ) {
X				selector = parts.shift();
X
X				if ( Expr.relative[ selector ] ) {
X					selector += parts.shift();
X				}
X
X				set = posProcess( selector, set, seed );
X			}
X		}
X
X	} else {
X		// Take a shortcut and set the context if the root selector is an ID
X		// (but not if it'll be faster if the inner selector is an ID)
X		if ( !seed && parts.length > 1 && context.nodeType === 9 && !contextXML &&
X				Expr.match.ID.test(parts[0]) && !Expr.match.ID.test(parts[parts.length - 1]) ) {
X
X			ret = Sizzle.find( parts.shift(), context, contextXML );
X			context = ret.expr ?
X				Sizzle.filter( ret.expr, ret.set )[0] :
X				ret.set[0];
X		}
X
X		if ( context ) {
X			ret = seed ?
X				{ expr: parts.pop(), set: makeArray(seed) } :
X				Sizzle.find( parts.pop(), parts.length === 1 && (parts[0] === "~" || parts[0] === "+") && context.parentNode ? context.parentNode : context, contextXML );
X
X			set = ret.expr ?
X				Sizzle.filter( ret.expr, ret.set ) :
X				ret.set;
X
X			if ( parts.length > 0 ) {
X				checkSet = makeArray( set );
X
X			} else {
X				prune = false;
X			}
X
X			while ( parts.length ) {
X				cur = parts.pop();
X				pop = cur;
X
X				if ( !Expr.relative[ cur ] ) {
X					cur = "";
X				} else {
X					pop = parts.pop();
X				}
X
X				if ( pop == null ) {
X					pop = context;
X				}
X
X				Expr.relative[ cur ]( checkSet, pop, contextXML );
X			}
X
X		} else {
X			checkSet = parts = [];
X		}
X	}
X
X	if ( !checkSet ) {
X		checkSet = set;
X	}
X
X	if ( !checkSet ) {
X		Sizzle.error( cur || selector );
X	}
X
X	if ( toString.call(checkSet) === "[object Array]" ) {
X		if ( !prune ) {
X			results.push.apply( results, checkSet );
X
X		} else if ( context && context.nodeType === 1 ) {
X			for ( i = 0; checkSet[i] != null; i++ ) {
X				if ( checkSet[i] && (checkSet[i] === true || checkSet[i].nodeType === 1 && Sizzle.contains(context, checkSet[i])) ) {
X					results.push( set[i] );
X				}
X			}
X
X		} else {
X			for ( i = 0; checkSet[i] != null; i++ ) {
X				if ( checkSet[i] && checkSet[i].nodeType === 1 ) {
X					results.push( set[i] );
X				}
X			}
X		}
X
X	} else {
X		makeArray( checkSet, results );
X	}
X
X	if ( extra ) {
X		Sizzle( extra, origContext, results, seed );
X		Sizzle.uniqueSort( results );
X	}
X
X	return results;
};
X
Sizzle.uniqueSort = function( results ) {
X	if ( sortOrder ) {
X		hasDuplicate = baseHasDuplicate;
X		results.sort( sortOrder );
X
X		if ( hasDuplicate ) {
X			for ( var i = 1; i < results.length; i++ ) {
X				if ( results[i] === results[ i - 1 ] ) {
X					results.splice( i--, 1 );
X				}
X			}
X		}
X	}
X
X	return results;
};
X
Sizzle.matches = function( expr, set ) {
X	return Sizzle( expr, null, null, set );
};
X
Sizzle.matchesSelector = function( node, expr ) {
X	return Sizzle( expr, null, null, [node] ).length > 0;
};
X
Sizzle.find = function( expr, context, isXML ) {
X	var set, i, len, match, type, left;
X
X	if ( !expr ) {
X		return [];
X	}
X
X	for ( i = 0, len = Expr.order.length; i < len; i++ ) {
X		type = Expr.order[i];
X
X		if ( (match = Expr.leftMatch[ type ].exec( expr )) ) {
X			left = match[1];
X			match.splice( 1, 1 );
X
X			if ( left.substr( left.length - 1 ) !== "\\" ) {
X				match[1] = (match[1] || "").replace( rBackslash, "" );
X				set = Expr.find[ type ]( match, context, isXML );
X
X				if ( set != null ) {
X					expr = expr.replace( Expr.match[ type ], "" );
X					break;
X				}
X			}
X		}
X	}
X
X	if ( !set ) {
X		set = typeof context.getElementsByTagName !== "undefined" ?
X			context.getElementsByTagName( "*" ) :
X			[];
X	}
X
X	return { set: set, expr: expr };
};
X
Sizzle.filter = function( expr, set, inplace, not ) {
X	var match, anyFound,
X		type, found, item, filter, left,
X		i, pass,
X		old = expr,
X		result = [],
X		curLoop = set,
X		isXMLFilter = set && set[0] && Sizzle.isXML( set[0] );
X
X	while ( expr && set.length ) {
X		for ( type in Expr.filter ) {
X			if ( (match = Expr.leftMatch[ type ].exec( expr )) != null && match[2] ) {
X				filter = Expr.filter[ type ];
X				left = match[1];
X
X				anyFound = false;
X
X				match.splice(1,1);
X
X				if ( left.substr( left.length - 1 ) === "\\" ) {
X					continue;
X				}
X
X				if ( curLoop === result ) {
X					result = [];
X				}
X
X				if ( Expr.preFilter[ type ] ) {
X					match = Expr.preFilter[ type ]( match, curLoop, inplace, result, not, isXMLFilter );
X
X					if ( !match ) {
X						anyFound = found = true;
X
X					} else if ( match === true ) {
X						continue;
X					}
X				}
X
X				if ( match ) {
X					for ( i = 0; (item = curLoop[i]) != null; i++ ) {
X						if ( item ) {
X							found = filter( item, match, i, curLoop );
X							pass = not ^ found;
X
X							if ( inplace && found != null ) {
X								if ( pass ) {
X									anyFound = true;
X
X								} else {
X									curLoop[i] = false;
X								}
X
X							} else if ( pass ) {
X								result.push( item );
X								anyFound = true;
X							}
X						}
X					}
X				}
X
X				if ( found !== undefined ) {
X					if ( !inplace ) {
X						curLoop = result;
X					}
X
X					expr = expr.replace( Expr.match[ type ], "" );
X
X					if ( !anyFound ) {
X						return [];
X					}
X
X					break;
X				}
X			}
X		}
X
X		// Improper expression
X		if ( expr === old ) {
X			if ( anyFound == null ) {
X				Sizzle.error( expr );
X
X			} else {
X				break;
X			}
X		}
X
X		old = expr;
X	}
X
X	return curLoop;
};
X
Sizzle.error = function( msg ) {
X	throw new Error( "Syntax error, unrecognized expression: " + msg );
};
X
/**
X * Utility function for retreiving the text value of an array of DOM nodes
X * @param {Array|Element} elem
X */
var getText = Sizzle.getText = function( elem ) {
X    var i, node,
X		nodeType = elem.nodeType,
X		ret = "";
X
X	if ( nodeType ) {
X		if ( nodeType === 1 || nodeType === 9 || nodeType === 11 ) {
X			// Use textContent || innerText for elements
X			if ( typeof elem.textContent === 'string' ) {
X				return elem.textContent;
X			} else if ( typeof elem.innerText === 'string' ) {
X				// Replace IE's carriage returns
X				return elem.innerText.replace( rReturn, '' );
X			} else {
X				// Traverse it's children
X				for ( elem = elem.firstChild; elem; elem = elem.nextSibling) {
X					ret += getText( elem );
X				}
X			}
X		} else if ( nodeType === 3 || nodeType === 4 ) {
X			return elem.nodeValue;
X		}
X	} else {
X
X		// If no nodeType, this is expected to be an array
X		for ( i = 0; (node = elem[i]); i++ ) {
X			// Do not traverse comment nodes
X			if ( node.nodeType !== 8 ) {
X				ret += getText( node );
X			}
X		}
X	}
X	return ret;
};
X
var Expr = Sizzle.selectors = {
X	order: [ "ID", "NAME", "TAG" ],
X
X	match: {
X		ID: /#((?:[\w\u00c0-\uFFFF\-]|\\.)+)/,
X		CLASS: /\.((?:[\w\u00c0-\uFFFF\-]|\\.)+)/,
X		NAME: /\[name=['"]*((?:[\w\u00c0-\uFFFF\-]|\\.)+)['"]*\]/,
X		ATTR: /\[\s*((?:[\w\u00c0-\uFFFF\-]|\\.)+)\s*(?:(\S?=)\s*(?:(['"])(.*?)\3|(#?(?:[\w\u00c0-\uFFFF\-]|\\.)*)|)|)\s*\]/,
X		TAG: /^((?:[\w\u00c0-\uFFFF\*\-]|\\.)+)/,
X		CHILD: /:(only|nth|last|first)-child(?:\(\s*(even|odd|(?:[+\-]?\d+|(?:[+\-]?\d*)?n\s*(?:[+\-]\s*\d+)?))\s*\))?/,
X		POS: /:(nth|eq|gt|lt|first|last|even|odd)(?:\((\d*)\))?(?=[^\-]|$)/,
X		PSEUDO: /:((?:[\w\u00c0-\uFFFF\-]|\\.)+)(?:\((['"]?)((?:\([^\)]+\)|[^\(\)]*)+)\2\))?/
X	},
X
X	leftMatch: {},
X
X	attrMap: {
X		"class": "className",
X		"for": "htmlFor"
X	},
X
X	attrHandle: {
X		href: function( elem ) {
X			return elem.getAttribute( "href" );
X		},
X		type: function( elem ) {
X			return elem.getAttribute( "type" );
X		}
X	},
X
X	relative: {
X		"+": function(checkSet, part){
X			var isPartStr = typeof part === "string",
X				isTag = isPartStr && !rNonWord.test( part ),
X				isPartStrNotTag = isPartStr && !isTag;
X
X			if ( isTag ) {
X				part = part.toLowerCase();
X			}
X
X			for ( var i = 0, l = checkSet.length, elem; i < l; i++ ) {
X				if ( (elem = checkSet[i]) ) {
X					while ( (elem = elem.previousSibling) && elem.nodeType !== 1 ) {}
X
X					checkSet[i] = isPartStrNotTag || elem && elem.nodeName.toLowerCase() === part ?
X						elem || false :
X						elem === part;
X				}
X			}
X
X			if ( isPartStrNotTag ) {
X				Sizzle.filter( part, checkSet, true );
X			}
X		},
X
X		">": function( checkSet, part ) {
X			var elem,
X				isPartStr = typeof part === "string",
X				i = 0,
X				l = checkSet.length;
X
X			if ( isPartStr && !rNonWord.test( part ) ) {
X				part = part.toLowerCase();
X
X				for ( ; i < l; i++ ) {
X					elem = checkSet[i];
X
X					if ( elem ) {
X						var parent = elem.parentNode;
X						checkSet[i] = parent.nodeName.toLowerCase() === part ? parent : false;
X					}
X				}
X
X			} else {
X				for ( ; i < l; i++ ) {
X					elem = checkSet[i];
X
X					if ( elem ) {
X						checkSet[i] = isPartStr ?
X							elem.parentNode :
X							elem.parentNode === part;
X					}
X				}
X
X				if ( isPartStr ) {
X					Sizzle.filter( part, checkSet, true );
X				}
X			}
X		},
X
X		"": function(checkSet, part, isXML){
X			var nodeCheck,
X				doneName = done++,
X				checkFn = dirCheck;
X
X			if ( typeof part === "string" && !rNonWord.test( part ) ) {
X				part = part.toLowerCase();
X				nodeCheck = part;
X				checkFn = dirNodeCheck;
X			}
X
X			checkFn( "parentNode", part, doneName, checkSet, nodeCheck, isXML );
X		},
X
X		"~": function( checkSet, part, isXML ) {
X			var nodeCheck,
X				doneName = done++,
X				checkFn = dirCheck;
X
X			if ( typeof part === "string" && !rNonWord.test( part ) ) {
X				part = part.toLowerCase();
X				nodeCheck = part;
X				checkFn = dirNodeCheck;
X			}
X
X			checkFn( "previousSibling", part, doneName, checkSet, nodeCheck, isXML );
X		}
X	},
X
X	find: {
X		ID: function( match, context, isXML ) {
X			if ( typeof context.getElementById !== "undefined" && !isXML ) {
X				var m = context.getElementById(match[1]);
X				// Check parentNode to catch when Blackberry 4.6 returns
X				// nodes that are no longer in the document #6963
X				return m && m.parentNode ? [m] : [];
X			}
X		},
X
X		NAME: function( match, context ) {
X			if ( typeof context.getElementsByName !== "undefined" ) {
X				var ret = [],
X					results = context.getElementsByName( match[1] );
X
X				for ( var i = 0, l = results.length; i < l; i++ ) {
X					if ( results[i].getAttribute("name") === match[1] ) {
X						ret.push( results[i] );
X					}
X				}
X
X				return ret.length === 0 ? null : ret;
X			}
X		},
X
X		TAG: function( match, context ) {
X			if ( typeof context.getElementsByTagName !== "undefined" ) {
X				return context.getElementsByTagName( match[1] );
X			}
X		}
X	},
X	preFilter: {
X		CLASS: function( match, curLoop, inplace, result, not, isXML ) {
X			match = " " + match[1].replace( rBackslash, "" ) + " ";
X
X			if ( isXML ) {
X				return match;
X			}
X
X			for ( var i = 0, elem; (elem = curLoop[i]) != null; i++ ) {
X				if ( elem ) {
X					if ( not ^ (elem.className && (" " + elem.className + " ").replace(/[\t\n\r]/g, " ").indexOf(match) >= 0) ) {
X						if ( !inplace ) {
X							result.push( elem );
X						}
X
X					} else if ( inplace ) {
X						curLoop[i] = false;
X					}
X				}
X			}
X
X			return false;
X		},
X
X		ID: function( match ) {
X			return match[1].replace( rBackslash, "" );
X		},
X
X		TAG: function( match, curLoop ) {
X			return match[1].replace( rBackslash, "" ).toLowerCase();
X		},
X
X		CHILD: function( match ) {
X			if ( match[1] === "nth" ) {
X				if ( !match[2] ) {
X					Sizzle.error( match[0] );
X				}
X
X				match[2] = match[2].replace(/^\+|\s*/g, '');
X
X				// parse equations like 'even', 'odd', '5', '2n', '3n+2', '4n-1', '-n+6'
X				var test = /(-?)(\d*)(?:n([+\-]?\d*))?/.exec(
X					match[2] === "even" && "2n" || match[2] === "odd" && "2n+1" ||
X					!/\D/.test( match[2] ) && "0n+" + match[2] || match[2]);
X
X				// calculate the numbers (first)n+(last) including if they are negative
X				match[2] = (test[1] + (test[2] || 1)) - 0;
X				match[3] = test[3] - 0;
X			}
X			else if ( match[2] ) {
X				Sizzle.error( match[0] );
X			}
X
X			// TODO: Move to normal caching system
X			match[0] = done++;
X
X			return match;
X		},
X
X		ATTR: function( match, curLoop, inplace, result, not, isXML ) {
X			var name = match[1] = match[1].replace( rBackslash, "" );
X
X			if ( !isXML && Expr.attrMap[name] ) {
X				match[1] = Expr.attrMap[name];
X			}
X
X			// Handle if an un-quoted value was used
X			match[4] = ( match[4] || match[5] || "" ).replace( rBackslash, "" );
X
X			if ( match[2] === "~=" ) {
X				match[4] = " " + match[4] + " ";
X			}
X
X			return match;
X		},
X
X		PSEUDO: function( match, curLoop, inplace, result, not ) {
X			if ( match[1] === "not" ) {
X				// If we're dealing with a complex expression, or a simple one
X				if ( ( chunker.exec(match[3]) || "" ).length > 1 || /^\w/.test(match[3]) ) {
X					match[3] = Sizzle(match[3], null, null, curLoop);
X
X				} else {
X					var ret = Sizzle.filter(match[3], curLoop, inplace, true ^ not);
X
X					if ( !inplace ) {
X						result.push.apply( result, ret );
X					}
X
X					return false;
X				}
X
X			} else if ( Expr.match.POS.test( match[0] ) || Expr.match.CHILD.test( match[0] ) ) {
X				return true;
X			}
X
X			return match;
X		},
X
X		POS: function( match ) {
X			match.unshift( true );
X
X			return match;
X		}
X	},
X
X	filters: {
X		enabled: function( elem ) {
X			return elem.disabled === false && elem.type !== "hidden";
X		},
X
X		disabled: function( elem ) {
X			return elem.disabled === true;
X		},
X
X		checked: function( elem ) {
X			return elem.checked === true;
X		},
X
X		selected: function( elem ) {
X			// Accessing this property makes selected-by-default
X			// options in Safari work properly
X			if ( elem.parentNode ) {
X				elem.parentNode.selectedIndex;
X			}
X
X			return elem.selected === true;
X		},
X
X		parent: function( elem ) {
X			return !!elem.firstChild;
X		},
X
X		empty: function( elem ) {
X			return !elem.firstChild;
X		},
X
X		has: function( elem, i, match ) {
X			return !!Sizzle( match[3], elem ).length;
X		},
X
X		header: function( elem ) {
X			return (/h\d/i).test( elem.nodeName );
X		},
X
X		text: function( elem ) {
X			var attr = elem.getAttribute( "type" ), type = elem.type;
X			// IE6 and 7 will map elem.type to 'text' for new HTML5 types (search, etc)
X			// use getAttribute instead to test this case
X			return elem.nodeName.toLowerCase() === "input" && "text" === type && ( attr === type || attr === null );
X		},
X
X		radio: function( elem ) {
X			return elem.nodeName.toLowerCase() === "input" && "radio" === elem.type;
X		},
X
X		checkbox: function( elem ) {
X			return elem.nodeName.toLowerCase() === "input" && "checkbox" === elem.type;
X		},
X
X		file: function( elem ) {
X			return elem.nodeName.toLowerCase() === "input" && "file" === elem.type;
X		},
X
X		password: function( elem ) {
X			return elem.nodeName.toLowerCase() === "input" && "password" === elem.type;
X		},
X
X		submit: function( elem ) {
X			var name = elem.nodeName.toLowerCase();
X			return (name === "input" || name === "button") && "submit" === elem.type;
X		},
X
X		image: function( elem ) {
X			return elem.nodeName.toLowerCase() === "input" && "image" === elem.type;
X		},
X
X		reset: function( elem ) {
X			var name = elem.nodeName.toLowerCase();
X			return (name === "input" || name === "button") && "reset" === elem.type;
X		},
X
X		button: function( elem ) {
X			var name = elem.nodeName.toLowerCase();
X			return name === "input" && "button" === elem.type || name === "button";
X		},
X
X		input: function( elem ) {
X			return (/input|select|textarea|button/i).test( elem.nodeName );
X		},
X
X		focus: function( elem ) {
X			return elem === elem.ownerDocument.activeElement;
X		}
X	},
X	setFilters: {
X		first: function( elem, i ) {
X			return i === 0;
X		},
X
X		last: function( elem, i, match, array ) {
X			return i === array.length - 1;
X		},
X
X		even: function( elem, i ) {
X			return i % 2 === 0;
X		},
X
X		odd: function( elem, i ) {
X			return i % 2 === 1;
X		},
X
X		lt: function( elem, i, match ) {
X			return i < match[3] - 0;
X		},
X
X		gt: function( elem, i, match ) {
X			return i > match[3] - 0;
X		},
X
X		nth: function( elem, i, match ) {
X			return match[3] - 0 === i;
X		},
X
X		eq: function( elem, i, match ) {
X			return match[3] - 0 === i;
X		}
X	},
X	filter: {
X		PSEUDO: function( elem, match, i, array ) {
X			var name = match[1],
X				filter = Expr.filters[ name ];
X
X			if ( filter ) {
X				return filter( elem, i, match, array );
X
X			} else if ( name === "contains" ) {
X				return (elem.textContent || elem.innerText || getText([ elem ]) || "").indexOf(match[3]) >= 0;
X
X			} else if ( name === "not" ) {
X				var not = match[3];
X
X				for ( var j = 0, l = not.length; j < l; j++ ) {
X					if ( not[j] === elem ) {
X						return false;
X					}
X				}
X
X				return true;
X
X			} else {
X				Sizzle.error( name );
X			}
X		},
X
X		CHILD: function( elem, match ) {
X			var first, last,
X				doneName, parent, cache,
X				count, diff,
X				type = match[1],
X				node = elem;
X
X			switch ( type ) {
X				case "only":
X				case "first":
X					while ( (node = node.previousSibling) ) {
X						if ( node.nodeType === 1 ) {
X							return false;
X						}
X					}
X
X					if ( type === "first" ) {
X						return true;
X					}
X
X					node = elem;
X
X					/* falls through */
X				case "last":
X					while ( (node = node.nextSibling) ) {
X						if ( node.nodeType === 1 ) {
X							return false;
X						}
X					}
X
X					return true;
X
X				case "nth":
X					first = match[2];
X					last = match[3];
X
X					if ( first === 1 && last === 0 ) {
X						return true;
X					}
X
X					doneName = match[0];
X					parent = elem.parentNode;
X
X					if ( parent && (parent[ expando ] !== doneName || !elem.nodeIndex) ) {
X						count = 0;
X
X						for ( node = parent.firstChild; node; node = node.nextSibling ) {
X							if ( node.nodeType === 1 ) {
X								node.nodeIndex = ++count;
X							}
X						}
X
X						parent[ expando ] = doneName;
X					}
X
X					diff = elem.nodeIndex - last;
X
X					if ( first === 0 ) {
X						return diff === 0;
X
X					} else {
X						return ( diff % first === 0 && diff / first >= 0 );
X					}
X			}
X		},
X
X		ID: function( elem, match ) {
X			return elem.nodeType === 1 && elem.getAttribute("id") === match;
X		},
X
X		TAG: function( elem, match ) {
X			return (match === "*" && elem.nodeType === 1) || !!elem.nodeName && elem.nodeName.toLowerCase() === match;
X		},
X
X		CLASS: function( elem, match ) {
X			return (" " + (elem.className || elem.getAttribute("class")) + " ")
X				.indexOf( match ) > -1;
X		},
X
X		ATTR: function( elem, match ) {
X			var name = match[1],
X				result = Sizzle.attr ?
X					Sizzle.attr( elem, name ) :
X					Expr.attrHandle[ name ] ?
X					Expr.attrHandle[ name ]( elem ) :
X					elem[ name ] != null ?
X						elem[ name ] :
X						elem.getAttribute( name ),
X				value = result + "",
X				type = match[2],
X				check = match[4];
X
X			return result == null ?
X				type === "!=" :
X				!type && Sizzle.attr ?
X				result != null :
X				type === "=" ?
X				value === check :
X				type === "*=" ?
X				value.indexOf(check) >= 0 :
X				type === "~=" ?
X				(" " + value + " ").indexOf(check) >= 0 :
X				!check ?
X				value && result !== false :
X				type === "!=" ?
X				value !== check :
X				type === "^=" ?
X				value.indexOf(check) === 0 :
X				type === "$=" ?
X				value.substr(value.length - check.length) === check :
X				type === "|=" ?
X				value === check || value.substr(0, check.length + 1) === check + "-" :
X				false;
X		},
X
X		POS: function( elem, match, i, array ) {
X			var name = match[2],
X				filter = Expr.setFilters[ name ];
X
X			if ( filter ) {
X				return filter( elem, i, match, array );
X			}
X		}
X	}
};
X
var origPOS = Expr.match.POS,
X	fescape = function(all, num){
X		return "\\" + (num - 0 + 1);
X	};
X
for ( var type in Expr.match ) {
X	Expr.match[ type ] = new RegExp( Expr.match[ type ].source + (/(?![^\[]*\])(?![^\(]*\))/.source) );
X	Expr.leftMatch[ type ] = new RegExp( /(^(?:.|\r|\n)*?)/.source + Expr.match[ type ].source.replace(/\\(\d+)/g, fescape) );
}
// Expose origPOS
// "global" as in regardless of relation to brackets/parens
Expr.match.globalPOS = origPOS;
X
var makeArray = function( array, results ) {
X	array = Array.prototype.slice.call( array, 0 );
X
X	if ( results ) {
X		results.push.apply( results, array );
X		return results;
X	}
X
X	return array;
};
X
// Perform a simple check to determine if the browser is capable of
// converting a NodeList to an array using builtin methods.
// Also verifies that the returned array holds DOM nodes
// (which is not the case in the Blackberry browser)
try {
X	Array.prototype.slice.call( document.documentElement.childNodes, 0 )[0].nodeType;
X
// Provide a fallback method if it does not work
} catch( e ) {
X	makeArray = function( array, results ) {
X		var i = 0,
X			ret = results || [];
X
X		if ( toString.call(array) === "[object Array]" ) {
X			Array.prototype.push.apply( ret, array );
X
X		} else {
X			if ( typeof array.length === "number" ) {
X				for ( var l = array.length; i < l; i++ ) {
X					ret.push( array[i] );
X				}
X
X			} else {
X				for ( ; array[i]; i++ ) {
X					ret.push( array[i] );
X				}
X			}
X		}
X
X		return ret;
X	};
}
X
var sortOrder, siblingCheck;
X
if ( document.documentElement.compareDocumentPosition ) {
X	sortOrder = function( a, b ) {
X		if ( a === b ) {
X			hasDuplicate = true;
X			return 0;
X		}
X
X		if ( !a.compareDocumentPosition || !b.compareDocumentPosition ) {
X			return a.compareDocumentPosition ? -1 : 1;
X		}
X
X		return a.compareDocumentPosition(b) & 4 ? -1 : 1;
X	};
X
} else {
X	sortOrder = function( a, b ) {
X		// The nodes are identical, we can exit early
X		if ( a === b ) {
X			hasDuplicate = true;
X			return 0;
X
X		// Fallback to using sourceIndex (in IE) if it's available on both nodes
X		} else if ( a.sourceIndex && b.sourceIndex ) {
X			return a.sourceIndex - b.sourceIndex;
X		}
X
X		var al, bl,
X			ap = [],
X			bp = [],
X			aup = a.parentNode,
X			bup = b.parentNode,
X			cur = aup;
X
X		// If the nodes are siblings (or identical) we can do a quick check
X		if ( aup === bup ) {
X			return siblingCheck( a, b );
X
X		// If no parents were found then the nodes are disconnected
X		} else if ( !aup ) {
X			return -1;
X
X		} else if ( !bup ) {
X			return 1;
X		}
X
X		// Otherwise they're somewhere else in the tree so we need
X		// to build up a full list of the parentNodes for comparison
X		while ( cur ) {
X			ap.unshift( cur );
X			cur = cur.parentNode;
X		}
X
X		cur = bup;
X
X		while ( cur ) {
X			bp.unshift( cur );
X			cur = cur.parentNode;
X		}
X
X		al = ap.length;
X		bl = bp.length;
X
X		// Start walking down the tree looking for a discrepancy
X		for ( var i = 0; i < al && i < bl; i++ ) {
X			if ( ap[i] !== bp[i] ) {
X				return siblingCheck( ap[i], bp[i] );
X			}
X		}
X
X		// We ended someplace up the tree so do a sibling check
X		return i === al ?
X			siblingCheck( a, bp[i], -1 ) :
X			siblingCheck( ap[i], b, 1 );
X	};
X
X	siblingCheck = function( a, b, ret ) {
X		if ( a === b ) {
X			return ret;
X		}
X
X		var cur = a.nextSibling;
X
X		while ( cur ) {
X			if ( cur === b ) {
X				return -1;
X			}
X
X			cur = cur.nextSibling;
X		}
X
X		return 1;
X	};
}
X
// Check to see if the browser returns elements by name when
// querying by getElementById (and provide a workaround)
(function(){
X	// We're going to inject a fake input element with a specified name
X	var form = document.createElement("div"),
X		id = "script" + (new Date()).getTime(),
X		root = document.documentElement;
X
X	form.innerHTML = "<a name='" + id + "'/>";
X
X	// Inject it into the root element, check its status, and remove it quickly
X	root.insertBefore( form, root.firstChild );
X
X	// The workaround has to do additional checks after a getElementById
X	// Which slows things down for other browsers (hence the branching)
X	if ( document.getElementById( id ) ) {
X		Expr.find.ID = function( match, context, isXML ) {
X			if ( typeof context.getElementById !== "undefined" && !isXML ) {
X				var m = context.getElementById(match[1]);
X
X				return m ?
X					m.id === match[1] || typeof m.getAttributeNode !== "undefined" && m.getAttributeNode("id").nodeValue === match[1] ?
X						[m] :
X						undefined :
X					[];
X			}
X		};
X
X		Expr.filter.ID = function( elem, match ) {
X			var node = typeof elem.getAttributeNode !== "undefined" && elem.getAttributeNode("id");
X
X			return elem.nodeType === 1 && node && node.nodeValue === match;
X		};
X	}
X
X	root.removeChild( form );
X
X	// release memory in IE
X	root = form = null;
})();
X
(function(){
X	// Check to see if the browser returns only elements
X	// when doing getElementsByTagName("*")
X
X	// Create a fake element
X	var div = document.createElement("div");
X	div.appendChild( document.createComment("") );
X
X	// Make sure no comments are found
X	if ( div.getElementsByTagName("*").length > 0 ) {
X		Expr.find.TAG = function( match, context ) {
X			var results = context.getElementsByTagName( match[1] );
X
X			// Filter out possible comments
X			if ( match[1] === "*" ) {
X				var tmp = [];
X
X				for ( var i = 0; results[i]; i++ ) {
X					if ( results[i].nodeType === 1 ) {
X						tmp.push( results[i] );
X					}
X				}
X
X				results = tmp;
X			}
X
X			return results;
X		};
X	}
X
X	// Check to see if an attribute returns normalized href attributes
X	div.innerHTML = "<a href='#'></a>";
X
X	if ( div.firstChild && typeof div.firstChild.getAttribute !== "undefined" &&
X			div.firstChild.getAttribute("href") !== "#" ) {
X
X		Expr.attrHandle.href = function( elem ) {
X			return elem.getAttribute( "href", 2 );
X		};
X	}
X
X	// release memory in IE
X	div = null;
})();
X
if ( document.querySelectorAll ) {
X	(function(){
X		var oldSizzle = Sizzle,
X			div = document.createElement("div"),
X			id = "__sizzle__";
X
X		div.innerHTML = "<p class='TEST'></p>";
X
X		// Safari can't handle uppercase or unicode characters when
X		// in quirks mode.
X		if ( div.querySelectorAll && div.querySelectorAll(".TEST").length === 0 ) {
X			return;
X		}
X
X		Sizzle = function( query, context, extra, seed ) {
X			context = context || document;
X
X			// Only use querySelectorAll on non-XML documents
X			// (ID selectors don't work in non-HTML documents)
X			if ( !seed && !Sizzle.isXML(context) ) {
X				// See if we find a selector to speed up
X				var match = /^(\w+$)|^\.([\w\-]+$)|^#([\w\-]+$)/.exec( query );
X
X				if ( match && (context.nodeType === 1 || context.nodeType === 9) ) {
X					// Speed-up: Sizzle("TAG")
X					if ( match[1] ) {
X						return makeArray( context.getElementsByTagName( query ), extra );
X
X					// Speed-up: Sizzle(".CLASS")
X					} else if ( match[2] && Expr.find.CLASS && context.getElementsByClassName ) {
X						return makeArray( context.getElementsByClassName( match[2] ), extra );
X					}
X				}
X
X				if ( context.nodeType === 9 ) {
X					// Speed-up: Sizzle("body")
X					// The body element only exists once, optimize finding it
X					if ( query === "body" && context.body ) {
X						return makeArray( [ context.body ], extra );
X
X					// Speed-up: Sizzle("#ID")
X					} else if ( match && match[3] ) {
X						var elem = context.getElementById( match[3] );
X
X						// Check parentNode to catch when Blackberry 4.6 returns
X						// nodes that are no longer in the document #6963
X						if ( elem && elem.parentNode ) {
X							// Handle the case where IE and Opera return items
X							// by name instead of ID
X							if ( elem.id === match[3] ) {
X								return makeArray( [ elem ], extra );
X							}
X
X						} else {
X							return makeArray( [], extra );
X						}
X					}
X
X					try {
X						return makeArray( context.querySelectorAll(query), extra );
X					} catch(qsaError) {}
X
X				// qSA works strangely on Element-rooted queries
X				// We can work around this by specifying an extra ID on the root
X				// and working up from there (Thanks to Andrew Dupont for the technique)
X				// IE 8 doesn't work on object elements
X				} else if ( context.nodeType === 1 && context.nodeName.toLowerCase() !== "object" ) {
X					var oldContext = context,
X						old = context.getAttribute( "id" ),
X						nid = old || id,
X						hasParent = context.parentNode,
X						relativeHierarchySelector = /^\s*[+~]/.test( query );
X
X					if ( !old ) {
X						context.setAttribute( "id", nid );
X					} else {
X						nid = nid.replace( /'/g, "\\$&" );
X					}
X					if ( relativeHierarchySelector && hasParent ) {
X						context = context.parentNode;
X					}
X
X					try {
X						if ( !relativeHierarchySelector || hasParent ) {
X							return makeArray( context.querySelectorAll( "[id='" + nid + "'] " + query ), extra );
X						}
X
X					} catch(pseudoError) {
X					} finally {
X						if ( !old ) {
X							oldContext.removeAttribute( "id" );
X						}
X					}
X				}
X			}
X
X			return oldSizzle(query, context, extra, seed);
X		};
X
X		for ( var prop in oldSizzle ) {
X			Sizzle[ prop ] = oldSizzle[ prop ];
X		}
X
X		// release memory in IE
X		div = null;
X	})();
}
X
(function(){
X	var html = document.documentElement,
X		matches = html.matchesSelector || html.mozMatchesSelector || html.webkitMatchesSelector || html.msMatchesSelector;
X
X	if ( matches ) {
X		// Check to see if it's possible to do matchesSelector
X		// on a disconnected node (IE 9 fails this)
X		var disconnectedMatch = !matches.call( document.createElement( "div" ), "div" ),
X			pseudoWorks = false;
X
X		try {
X			// This should fail with an exception
X			// Gecko does not error, returns false instead
X			matches.call( document.documentElement, "[test!='']:sizzle" );
X
X		} catch( pseudoError ) {
X			pseudoWorks = true;
X		}
X
X		Sizzle.matchesSelector = function( node, expr ) {
X			// Make sure that attribute selectors are quoted
X			expr = expr.replace(/\=\s*([^'"\]]*)\s*\]/g, "='$1']");
X
X			if ( !Sizzle.isXML( node ) ) {
X				try {
X					if ( pseudoWorks || !Expr.match.PSEUDO.test( expr ) && !/!=/.test( expr ) ) {
X						var ret = matches.call( node, expr );
X
X						// IE 9's matchesSelector returns false on disconnected nodes
X						if ( ret || !disconnectedMatch ||
X								// As well, disconnected nodes are said to be in a document
X								// fragment in IE 9, so check for that
X								node.document && node.document.nodeType !== 11 ) {
X							return ret;
X						}
X					}
X				} catch(e) {}
X			}
X
X			return Sizzle(expr, null, null, [node]).length > 0;
X		};
X	}
})();
X
(function(){
X	var div = document.createElement("div");
X
X	div.innerHTML = "<div class='test e'></div><div class='test'></div>";
X
X	// Opera can't find a second classname (in 9.6)
X	// Also, make sure that getElementsByClassName actually exists
X	if ( !div.getElementsByClassName || div.getElementsByClassName("e").length === 0 ) {
X		return;
X	}
X
X	// Safari caches class attributes, doesn't catch changes (in 3.2)
X	div.lastChild.className = "e";
X
X	if ( div.getElementsByClassName("e").length === 1 ) {
X		return;
X	}
X
X	Expr.order.splice(1, 0, "CLASS");
X	Expr.find.CLASS = function( match, context, isXML ) {
X		if ( typeof context.getElementsByClassName !== "undefined" && !isXML ) {
X			return context.getElementsByClassName(match[1]);
X		}
X	};
X
X	// release memory in IE
X	div = null;
})();
X
function dirNodeCheck( dir, cur, doneName, checkSet, nodeCheck, isXML ) {
X	for ( var i = 0, l = checkSet.length; i < l; i++ ) {
X		var elem = checkSet[i];
X
X		if ( elem ) {
X			var match = false;
X
X			elem = elem[dir];
X
X			while ( elem ) {
X				if ( elem[ expando ] === doneName ) {
X					match = checkSet[elem.sizset];
X					break;
X				}
X
X				if ( elem.nodeType === 1 && !isXML ){
X					elem[ expando ] = doneName;
X					elem.sizset = i;
X				}
X
X				if ( elem.nodeName.toLowerCase() === cur ) {
X					match = elem;
X					break;
X				}
X
X				elem = elem[dir];
X			}
X
X			checkSet[i] = match;
X		}
X	}
}
X
function dirCheck( dir, cur, doneName, checkSet, nodeCheck, isXML ) {
X	for ( var i = 0, l = checkSet.length; i < l; i++ ) {
X		var elem = checkSet[i];
X
X		if ( elem ) {
X			var match = false;
X
X			elem = elem[dir];
X
X			while ( elem ) {
X				if ( elem[ expando ] === doneName ) {
X					match = checkSet[elem.sizset];
X					break;
X				}
X
X				if ( elem.nodeType === 1 ) {
X					if ( !isXML ) {
X						elem[ expando ] = doneName;
X						elem.sizset = i;
X					}
X
X					if ( typeof cur !== "string" ) {
X						if ( elem === cur ) {
X							match = true;
X							break;
X						}
X
X					} else if ( Sizzle.filter( cur, [elem] ).length > 0 ) {
X						match = elem;
X						break;
X					}
X				}
X
X				elem = elem[dir];
X			}
X
X			checkSet[i] = match;
X		}
X	}
}
X
if ( document.documentElement.contains ) {
X	Sizzle.contains = function( a, b ) {
X		return a !== b && (a.contains ? a.contains(b) : true);
X	};
X
} else if ( document.documentElement.compareDocumentPosition ) {
X	Sizzle.contains = function( a, b ) {
X		return !!(a.compareDocumentPosition(b) & 16);
X	};
X
} else {
X	Sizzle.contains = function() {
X		return false;
X	};
}
X
Sizzle.isXML = function( elem ) {
X	// documentElement is verified for cases where it doesn't yet exist
X	// (such as loading iframes in IE - #4833)
X	var documentElement = (elem ? elem.ownerDocument || elem : 0).documentElement;
X
X	return documentElement ? documentElement.nodeName !== "HTML" : false;
};
X
var posProcess = function( selector, context, seed ) {
X	var match,
X		tmpSet = [],
X		later = "",
X		root = context.nodeType ? [context] : context;
X
X	// Position selectors must be done after the filter
X	// And so must :not(positional) so we move all PSEUDOs to the end
X	while ( (match = Expr.match.PSEUDO.exec( selector )) ) {
X		later += match[0];
X		selector = selector.replace( Expr.match.PSEUDO, "" );
X	}
X
X	selector = Expr.relative[selector] ? selector + "*" : selector;
X
X	for ( var i = 0, l = root.length; i < l; i++ ) {
X		Sizzle( selector, root[i], tmpSet, seed );
X	}
X
X	return Sizzle.filter( later, tmpSet );
};
X
// EXPOSE
// Override sizzle attribute retrieval
Sizzle.attr = jQuery.attr;
Sizzle.selectors.attrMap = {};
jQuery.find = Sizzle;
jQuery.expr = Sizzle.selectors;
jQuery.expr[":"] = jQuery.expr.filters;
jQuery.unique = Sizzle.uniqueSort;
jQuery.text = Sizzle.getText;
jQuery.isXMLDoc = Sizzle.isXML;
jQuery.contains = Sizzle.contains;
X
X
})();
X
X
var runtil = /Until$/,
X	rparentsprev = /^(?:parents|prevUntil|prevAll)/,
X	// Note: This RegExp should be improved, or likely pulled from Sizzle
X	rmultiselector = /,/,
X	isSimple = /^.[^:#\[\.,]*$/,
X	slice = Array.prototype.slice,
X	POS = jQuery.expr.match.globalPOS,
X	// methods guaranteed to produce a unique set when starting from a unique set
X	guaranteedUnique = {
X		children: true,
X		contents: true,
X		next: true,
X		prev: true
X	};
X
jQuery.fn.extend({
X	find: function( selector ) {
X		var self = this,
X			i, l;
X
X		if ( typeof selector !== "string" ) {
X			return jQuery( selector ).filter(function() {
X				for ( i = 0, l = self.length; i < l; i++ ) {
X					if ( jQuery.contains( self[ i ], this ) ) {
X						return true;
X					}
X				}
X			});
X		}
X
X		var ret = this.pushStack( "", "find", selector ),
X			length, n, r;
X
X		for ( i = 0, l = this.length; i < l; i++ ) {
X			length = ret.length;
X			jQuery.find( selector, this[i], ret );
X
X			if ( i > 0 ) {
X				// Make sure that the results are unique
X				for ( n = length; n < ret.length; n++ ) {
X					for ( r = 0; r < length; r++ ) {
X						if ( ret[r] === ret[n] ) {
X							ret.splice(n--, 1);
X							break;
X						}
X					}
X				}
X			}
X		}
X
X		return ret;
X	},
X
X	has: function( target ) {
X		var targets = jQuery( target );
X		return this.filter(function() {
X			for ( var i = 0, l = targets.length; i < l; i++ ) {
X				if ( jQuery.contains( this, targets[i] ) ) {
X					return true;
X				}
X			}
X		});
X	},
X
X	not: function( selector ) {
X		return this.pushStack( winnow(this, selector, false), "not", selector);
X	},
X
X	filter: function( selector ) {
X		return this.pushStack( winnow(this, selector, true), "filter", selector );
X	},
X
X	is: function( selector ) {
X		return !!selector && (
X			typeof selector === "string" ?
X				// If this is a positional selector, check membership in the returned set
X				// so $("p:first").is("p:last") won't return true for a doc with two "p".
X				POS.test( selector ) ?
X					jQuery( selector, this.context ).index( this[0] ) >= 0 :
X					jQuery.filter( selector, this ).length > 0 :
X				this.filter( selector ).length > 0 );
X	},
X
X	closest: function( selectors, context ) {
X		var ret = [], i, l, cur = this[0];
X
X		// Array (deprecated as of jQuery 1.7)
X		if ( jQuery.isArray( selectors ) ) {
X			var level = 1;
X
X			while ( cur && cur.ownerDocument && cur !== context ) {
X				for ( i = 0; i < selectors.length; i++ ) {
X
X					if ( jQuery( cur ).is( selectors[ i ] ) ) {
X						ret.push({ selector: selectors[ i ], elem: cur, level: level });
X					}
X				}
X
X				cur = cur.parentNode;
X				level++;
X			}
X
X			return ret;
X		}
X
X		// String
X		var pos = POS.test( selectors ) || typeof selectors !== "string" ?
X				jQuery( selectors, context || this.context ) :
X				0;
X
X		for ( i = 0, l = this.length; i < l; i++ ) {
X			cur = this[i];
X
X			while ( cur ) {
X				if ( pos ? pos.index(cur) > -1 : jQuery.find.matchesSelector(cur, selectors) ) {
X					ret.push( cur );
X					break;
X
X				} else {
X					cur = cur.parentNode;
X					if ( !cur || !cur.ownerDocument || cur === context || cur.nodeType === 11 ) {
X						break;
X					}
X				}
X			}
X		}
X
X		ret = ret.length > 1 ? jQuery.unique( ret ) : ret;
X
X		return this.pushStack( ret, "closest", selectors );
X	},
X
X	// Determine the position of an element within
X	// the matched set of elements
X	index: function( elem ) {
X
X		// No argument, return index in parent
X		if ( !elem ) {
X			return ( this[0] && this[0].parentNode ) ? this.prevAll().length : -1;
X		}
X
X		// index in selector
X		if ( typeof elem === "string" ) {
X			return jQuery.inArray( this[0], jQuery( elem ) );
X		}
X
X		// Locate the position of the desired element
X		return jQuery.inArray(
X			// If it receives a jQuery object, the first element is used
X			elem.jquery ? elem[0] : elem, this );
X	},
X
X	add: function( selector, context ) {
X		var set = typeof selector === "string" ?
X				jQuery( selector, context ) :
X				jQuery.makeArray( selector && selector.nodeType ? [ selector ] : selector ),
X			all = jQuery.merge( this.get(), set );
X
X		return this.pushStack( isDisconnected( set[0] ) || isDisconnected( all[0] ) ?
X			all :
X			jQuery.unique( all ) );
X	},
X
X	andSelf: function() {
X		return this.add( this.prevObject );
X	}
});
X
// A painfully simple check to see if an element is disconnected
// from a document (should be improved, where feasible).
function isDisconnected( node ) {
X	return !node || !node.parentNode || node.parentNode.nodeType === 11;
}
X
jQuery.each({
X	parent: function( elem ) {
X		var parent = elem.parentNode;
X		return parent && parent.nodeType !== 11 ? parent : null;
X	},
X	parents: function( elem ) {
X		return jQuery.dir( elem, "parentNode" );
X	},
X	parentsUntil: function( elem, i, until ) {
X		return jQuery.dir( elem, "parentNode", until );
X	},
X	next: function( elem ) {
X		return jQuery.nth( elem, 2, "nextSibling" );
X	},
X	prev: function( elem ) {
X		return jQuery.nth( elem, 2, "previousSibling" );
X	},
X	nextAll: function( elem ) {
X		return jQuery.dir( elem, "nextSibling" );
X	},
X	prevAll: function( elem ) {
X		return jQuery.dir( elem, "previousSibling" );
X	},
X	nextUntil: function( elem, i, until ) {
X		return jQuery.dir( elem, "nextSibling", until );
X	},
X	prevUntil: function( elem, i, until ) {
X		return jQuery.dir( elem, "previousSibling", until );
X	},
X	siblings: function( elem ) {
X		return jQuery.sibling( ( elem.parentNode || {} ).firstChild, elem );
X	},
X	children: function( elem ) {
X		return jQuery.sibling( elem.firstChild );
X	},
X	contents: function( elem ) {
X		return jQuery.nodeName( elem, "iframe" ) ?
X			elem.contentDocument || elem.contentWindow.document :
X			jQuery.makeArray( elem.childNodes );
X	}
}, function( name, fn ) {
X	jQuery.fn[ name ] = function( until, selector ) {
X		var ret = jQuery.map( this, fn, until );
X
X		if ( !runtil.test( name ) ) {
X			selector = until;
X		}
X
X		if ( selector && typeof selector === "string" ) {
X			ret = jQuery.filter( selector, ret );
X		}
X
X		ret = this.length > 1 && !guaranteedUnique[ name ] ? jQuery.unique( ret ) : ret;
X
X		if ( (this.length > 1 || rmultiselector.test( selector )) && rparentsprev.test( name ) ) {
X			ret = ret.reverse();
X		}
X
X		return this.pushStack( ret, name, slice.call( arguments ).join(",") );
X	};
});
X
jQuery.extend({
X	filter: function( expr, elems, not ) {
X		if ( not ) {
X			expr = ":not(" + expr + ")";
X		}
X
X		return elems.length === 1 ?
X			jQuery.find.matchesSelector(elems[0], expr) ? [ elems[0] ] : [] :
X			jQuery.find.matches(expr, elems);
X	},
X
X	dir: function( elem, dir, until ) {
X		var matched = [],
X			cur = elem[ dir ];
X
X		while ( cur && cur.nodeType !== 9 && (until === undefined || cur.nodeType !== 1 || !jQuery( cur ).is( until )) ) {
X			if ( cur.nodeType === 1 ) {
X				matched.push( cur );
X			}
X			cur = cur[dir];
X		}
X		return matched;
X	},
X
X	nth: function( cur, result, dir, elem ) {
X		result = result || 1;
X		var num = 0;
X
X		for ( ; cur; cur = cur[dir] ) {
X			if ( cur.nodeType === 1 && ++num === result ) {
X				break;
X			}
X		}
X
X		return cur;
X	},
X
X	sibling: function( n, elem ) {
X		var r = [];
X
X		for ( ; n; n = n.nextSibling ) {
X			if ( n.nodeType === 1 && n !== elem ) {
X				r.push( n );
X			}
X		}
X
X		return r;
X	}
});
X
// Implement the identical functionality for filter and not
function winnow( elements, qualifier, keep ) {
X
X	// Can't pass null or undefined to indexOf in Firefox 4
X	// Set to 0 to skip string check
X	qualifier = qualifier || 0;
X
X	if ( jQuery.isFunction( qualifier ) ) {
X		return jQuery.grep(elements, function( elem, i ) {
X			var retVal = !!qualifier.call( elem, i, elem );
X			return retVal === keep;
X		});
X
X	} else if ( qualifier.nodeType ) {
X		return jQuery.grep(elements, function( elem, i ) {
X			return ( elem === qualifier ) === keep;
X		});
X
X	} else if ( typeof qualifier === "string" ) {
X		var filtered = jQuery.grep(elements, function( elem ) {
X			return elem.nodeType === 1;
X		});
X
X		if ( isSimple.test( qualifier ) ) {
X			return jQuery.filter(qualifier, filtered, !keep);
X		} else {
X			qualifier = jQuery.filter( qualifier, filtered );
X		}
X	}
X
X	return jQuery.grep(elements, function( elem, i ) {
X		return ( jQuery.inArray( elem, qualifier ) >= 0 ) === keep;
X	});
}
X
X
X
X
function createSafeFragment( document ) {
X	var list = nodeNames.split( "|" ),
X	safeFrag = document.createDocumentFragment();
X
X	if ( safeFrag.createElement ) {
X		while ( list.length ) {
X			safeFrag.createElement(
X				list.pop()
X			);
X		}
X	}
X	return safeFrag;
}
X
var nodeNames = "abbr|article|aside|audio|bdi|canvas|data|datalist|details|figcaption|figure|footer|" +
X		"header|hgroup|mark|meter|nav|output|progress|section|summary|time|video",
X	rinlinejQuery = / jQuery\d+="(?:\d+|null)"/g,
X	rleadingWhitespace = /^\s+/,
X	rxhtmlTag = /<(?!area|br|col|embed|hr|img|input|link|meta|param)(([\w:]+)[^>]*)\/>/ig,
X	rtagName = /<([\w:]+)/,
X	rtbody = /<tbody/i,
X	rhtml = /<|&#?\w+;/,
X	rnoInnerhtml = /<(?:script|style)/i,
X	rnocache = /<(?:script|object|embed|option|style)/i,
X	rnoshimcache = new RegExp("<(?:" + nodeNames + ")[\\s/>]", "i"),
X	// checked="checked" or checked
X	rchecked = /checked\s*(?:[^=]|=\s*.checked.)/i,
X	rscriptType = /\/(java|ecma)script/i,
X	rcleanScript = /^\s*<!(?:\[CDATA\[|\-\-)/,
X	wrapMap = {
X		option: [ 1, "<select multiple='multiple'>", "</select>" ],
X		legend: [ 1, "<fieldset>", "</fieldset>" ],
X		thead: [ 1, "<table>", "</table>" ],
X		tr: [ 2, "<table><tbody>", "</tbody></table>" ],
X		td: [ 3, "<table><tbody><tr>", "</tr></tbody></table>" ],
X		col: [ 2, "<table><tbody></tbody><colgroup>", "</colgroup></table>" ],
X		area: [ 1, "<map>", "</map>" ],
X		_default: [ 0, "", "" ]
X	},
X	safeFragment = createSafeFragment( document );
X
wrapMap.optgroup = wrapMap.option;
wrapMap.tbody = wrapMap.tfoot = wrapMap.colgroup = wrapMap.caption = wrapMap.thead;
wrapMap.th = wrapMap.td;
X
// IE can't serialize <link> and <script> tags normally
if ( !jQuery.support.htmlSerialize ) {
X	wrapMap._default = [ 1, "div<div>", "</div>" ];
}
X
jQuery.fn.extend({
X	text: function( value ) {
X		return jQuery.access( this, function( value ) {
X			return value === undefined ?
X				jQuery.text( this ) :
X				this.empty().append( ( this[0] && this[0].ownerDocument || document ).createTextNode( value ) );
X		}, null, value, arguments.length );
X	},
X
X	wrapAll: function( html ) {
X		if ( jQuery.isFunction( html ) ) {
X			return this.each(function(i) {
X				jQuery(this).wrapAll( html.call(this, i) );
X			});
X		}
X
X		if ( this[0] ) {
X			// The elements to wrap the target around
X			var wrap = jQuery( html, this[0].ownerDocument ).eq(0).clone(true);
X
X			if ( this[0].parentNode ) {
X				wrap.insertBefore( this[0] );
X			}
X
X			wrap.map(function() {
X				var elem = this;
X
X				while ( elem.firstChild && elem.firstChild.nodeType === 1 ) {
X					elem = elem.firstChild;
X				}
X
X				return elem;
X			}).append( this );
X		}
X
X		return this;
X	},
X
X	wrapInner: function( html ) {
X		if ( jQuery.isFunction( html ) ) {
X			return this.each(function(i) {
X				jQuery(this).wrapInner( html.call(this, i) );
X			});
X		}
X
X		return this.each(function() {
X			var self = jQuery( this ),
X				contents = self.contents();
X
X			if ( contents.length ) {
X				contents.wrapAll( html );
X
X			} else {
X				self.append( html );
X			}
X		});
X	},
X
X	wrap: function( html ) {
X		var isFunction = jQuery.isFunction( html );
X
X		return this.each(function(i) {
X			jQuery( this ).wrapAll( isFunction ? html.call(this, i) : html );
X		});
X	},
X
X	unwrap: function() {
X		return this.parent().each(function() {
X			if ( !jQuery.nodeName( this, "body" ) ) {
X				jQuery( this ).replaceWith( this.childNodes );
X			}
X		}).end();
X	},
X
X	append: function() {
X		return this.domManip(arguments, true, function( elem ) {
X			if ( this.nodeType === 1 ) {
X				this.appendChild( elem );
X			}
X		});
X	},
X
X	prepend: function() {
X		return this.domManip(arguments, true, function( elem ) {
X			if ( this.nodeType === 1 ) {
X				this.insertBefore( elem, this.firstChild );
X			}
X		});
X	},
X
X	before: function() {
X		if ( this[0] && this[0].parentNode ) {
X			return this.domManip(arguments, false, function( elem ) {
X				this.parentNode.insertBefore( elem, this );
X			});
X		} else if ( arguments.length ) {
X			var set = jQuery.clean( arguments );
X			set.push.apply( set, this.toArray() );
X			return this.pushStack( set, "before", arguments );
X		}
X	},
X
X	after: function() {
X		if ( this[0] && this[0].parentNode ) {
X			return this.domManip(arguments, false, function( elem ) {
X				this.parentNode.insertBefore( elem, this.nextSibling );
X			});
X		} else if ( arguments.length ) {
X			var set = this.pushStack( this, "after", arguments );
X			set.push.apply( set, jQuery.clean(arguments) );
X			return set;
X		}
X	},
X
X	// keepData is for internal use only--do not document
X	remove: function( selector, keepData ) {
X		for ( var i = 0, elem; (elem = this[i]) != null; i++ ) {
X			if ( !selector || jQuery.filter( selector, [ elem ] ).length ) {
X				if ( !keepData && elem.nodeType === 1 ) {
X					jQuery.cleanData( elem.getElementsByTagName("*") );
X					jQuery.cleanData( [ elem ] );
X				}
X
X				if ( elem.parentNode ) {
X					elem.parentNode.removeChild( elem );
X				}
X			}
X		}
X
X		return this;
X	},
X
X	empty: function() {
X		for ( var i = 0, elem; (elem = this[i]) != null; i++ ) {
X			// Remove element nodes and prevent memory leaks
X			if ( elem.nodeType === 1 ) {
X				jQuery.cleanData( elem.getElementsByTagName("*") );
X			}
X
X			// Remove any remaining nodes
X			while ( elem.firstChild ) {
X				elem.removeChild( elem.firstChild );
X			}
X		}
X
X		return this;
X	},
X
X	clone: function( dataAndEvents, deepDataAndEvents ) {
X		dataAndEvents = dataAndEvents == null ? false : dataAndEvents;
X		deepDataAndEvents = deepDataAndEvents == null ? dataAndEvents : deepDataAndEvents;
X
X		return this.map( function () {
X			return jQuery.clone( this, dataAndEvents, deepDataAndEvents );
X		});
X	},
X
X	html: function( value ) {
X		return jQuery.access( this, function( value ) {
X			var elem = this[0] || {},
X				i = 0,
X				l = this.length;
X
X			if ( value === undefined ) {
X				return elem.nodeType === 1 ?
X					elem.innerHTML.replace( rinlinejQuery, "" ) :
X					null;
X			}
X
X
X			if ( typeof value === "string" && !rnoInnerhtml.test( value ) &&
X				( jQuery.support.leadingWhitespace || !rleadingWhitespace.test( value ) ) &&
X				!wrapMap[ ( rtagName.exec( value ) || ["", ""] )[1].toLowerCase() ] ) {
X
X				value = value.replace( rxhtmlTag, "<$1></$2>" );
X
X				try {
X					for (; i < l; i++ ) {
X						// Remove element nodes and prevent memory leaks
X						elem = this[i] || {};
X						if ( elem.nodeType === 1 ) {
X							jQuery.cleanData( elem.getElementsByTagName( "*" ) );
X							elem.innerHTML = value;
X						}
X					}
X
X					elem = 0;
X
X				// If using innerHTML throws an exception, use the fallback method
X				} catch(e) {}
X			}
X
X			if ( elem ) {
X				this.empty().append( value );
X			}
X		}, null, value, arguments.length );
X	},
X
X	replaceWith: function( value ) {
X		if ( this[0] && this[0].parentNode ) {
X			// Make sure that the elements are removed from the DOM before they are inserted
X			// this can help fix replacing a parent with child elements
X			if ( jQuery.isFunction( value ) ) {
X				return this.each(function(i) {
X					var self = jQuery(this), old = self.html();
X					self.replaceWith( value.call( this, i, old ) );
X				});
X			}
X
X			if ( typeof value !== "string" ) {
X				value = jQuery( value ).detach();
X			}
X
X			return this.each(function() {
X				var next = this.nextSibling,
X					parent = this.parentNode;
X
X				jQuery( this ).remove();
X
X				if ( next ) {
X					jQuery(next).before( value );
X				} else {
X					jQuery(parent).append( value );
X				}
X			});
X		} else {
X			return this.length ?
X				this.pushStack( jQuery(jQuery.isFunction(value) ? value() : value), "replaceWith", value ) :
X				this;
X		}
X	},
X
X	detach: function( selector ) {
X		return this.remove( selector, true );
X	},
X
X	domManip: function( args, table, callback ) {
X		var results, first, fragment, parent,
X			value = args[0],
X			scripts = [];
X
X		// We can't cloneNode fragments that contain checked, in WebKit
X		if ( !jQuery.support.checkClone && arguments.length === 3 && typeof value === "string" && rchecked.test( value ) ) {
X			return this.each(function() {
X				jQuery(this).domManip( args, table, callback, true );
X			});
X		}
X
X		if ( jQuery.isFunction(value) ) {
X			return this.each(function(i) {
X				var self = jQuery(this);
X				args[0] = value.call(this, i, table ? self.html() : undefined);
X				self.domManip( args, table, callback );
X			});
X		}
X
X		if ( this[0] ) {
X			parent = value && value.parentNode;
X
X			// If we're in a fragment, just use that instead of building a new one
X			if ( jQuery.support.parentNode && parent && parent.nodeType === 11 && parent.childNodes.length === this.length ) {
X				results = { fragment: parent };
X
X			} else {
X				results = jQuery.buildFragment( args, this, scripts );
X			}
X
X			fragment = results.fragment;
X
X			if ( fragment.childNodes.length === 1 ) {
X				first = fragment = fragment.firstChild;
X			} else {
X				first = fragment.firstChild;
X			}
X
X			if ( first ) {
X				table = table && jQuery.nodeName( first, "tr" );
X
X				for ( var i = 0, l = this.length, lastIndex = l - 1; i < l; i++ ) {
X					callback.call(
X						table ?
X							root(this[i], first) :
X							this[i],
X						// Make sure that we do not leak memory by inadvertently discarding
X						// the original fragment (which might have attached data) instead of
X						// using it; in addition, use the original fragment object for the last
X						// item instead of first because it can end up being emptied incorrectly
X						// in certain situations (Bug #8070).
X						// Fragments from the fragment cache must always be cloned and never used
X						// in place.
X						results.cacheable || ( l > 1 && i < lastIndex ) ?
X							jQuery.clone( fragment, true, true ) :
X							fragment
X					);
X				}
X			}
X
X			if ( scripts.length ) {
X				jQuery.each( scripts, function( i, elem ) {
X					if ( elem.src ) {
X						jQuery.ajax({
X							type: "GET",
X							global: false,
X							url: elem.src,
X							async: false,
X							dataType: "script"
X						});
X					} else {
X						jQuery.globalEval( ( elem.text || elem.textContent || elem.innerHTML || "" ).replace( rcleanScript, "/*$0*/" ) );
X					}
X
X					if ( elem.parentNode ) {
X						elem.parentNode.removeChild( elem );
X					}
X				});
X			}
X		}
X
X		return this;
X	}
});
X
function root( elem, cur ) {
X	return jQuery.nodeName(elem, "table") ?
X		(elem.getElementsByTagName("tbody")[0] ||
X		elem.appendChild(elem.ownerDocument.createElement("tbody"))) :
X		elem;
}
X
function cloneCopyEvent( src, dest ) {
X
X	if ( dest.nodeType !== 1 || !jQuery.hasData( src ) ) {
X		return;
X	}
X
X	var type, i, l,
X		oldData = jQuery._data( src ),
X		curData = jQuery._data( dest, oldData ),
X		events = oldData.events;
X
X	if ( events ) {
X		delete curData.handle;
X		curData.events = {};
X
X		for ( type in events ) {
X			for ( i = 0, l = events[ type ].length; i < l; i++ ) {
X				jQuery.event.add( dest, type, events[ type ][ i ] );
X			}
X		}
X	}
X
X	// make the cloned public data object a copy from the original
X	if ( curData.data ) {
X		curData.data = jQuery.extend( {}, curData.data );
X	}
}
X
function cloneFixAttributes( src, dest ) {
X	var nodeName;
X
X	// We do not need to do anything for non-Elements
X	if ( dest.nodeType !== 1 ) {
X		return;
X	}
X
X	// clearAttributes removes the attributes, which we don't want,
X	// but also removes the attachEvent events, which we *do* want
X	if ( dest.clearAttributes ) {
X		dest.clearAttributes();
X	}
X
X	// mergeAttributes, in contrast, only merges back on the
X	// original attributes, not the events
X	if ( dest.mergeAttributes ) {
X		dest.mergeAttributes( src );
X	}
X
X	nodeName = dest.nodeName.toLowerCase();
X
X	// IE6-8 fail to clone children inside object elements that use
X	// the proprietary classid attribute value (rather than the type
X	// attribute) to identify the type of content to display
X	if ( nodeName === "object" ) {
X		dest.outerHTML = src.outerHTML;
X
X	} else if ( nodeName === "input" && (src.type === "checkbox" || src.type === "radio") ) {
X		// IE6-8 fails to persist the checked state of a cloned checkbox
X		// or radio button. Worse, IE6-7 fail to give the cloned element
X		// a checked appearance if the defaultChecked value isn't also set
X		if ( src.checked ) {
X			dest.defaultChecked = dest.checked = src.checked;
X		}
X
X		// IE6-7 get confused and end up setting the value of a cloned
X		// checkbox/radio button to an empty string instead of "on"
X		if ( dest.value !== src.value ) {
X			dest.value = src.value;
X		}
X
X	// IE6-8 fails to return the selected option to the default selected
X	// state when cloning options
X	} else if ( nodeName === "option" ) {
X		dest.selected = src.defaultSelected;
X
X	// IE6-8 fails to set the defaultValue to the correct value when
X	// cloning other types of input fields
X	} else if ( nodeName === "input" || nodeName === "textarea" ) {
X		dest.defaultValue = src.defaultValue;
X
X	// IE blanks contents when cloning scripts
X	} else if ( nodeName === "script" && dest.text !== src.text ) {
X		dest.text = src.text;
X	}
X
X	// Event data gets referenced instead of copied if the expando
X	// gets copied too
X	dest.removeAttribute( jQuery.expando );
X
X	// Clear flags for bubbling special change/submit events, they must
X	// be reattached when the newly cloned events are first activated
X	dest.removeAttribute( "_submit_attached" );
X	dest.removeAttribute( "_change_attached" );
}
X
jQuery.buildFragment = function( args, nodes, scripts ) {
X	var fragment, cacheable, cacheresults, doc,
X	first = args[ 0 ];
X
X	// nodes may contain either an explicit document object,
X	// a jQuery collection or context object.
X	// If nodes[0] contains a valid object to assign to doc
X	if ( nodes && nodes[0] ) {
X		doc = nodes[0].ownerDocument || nodes[0];
X	}
X
X	// Ensure that an attr object doesn't incorrectly stand in as a document object
X	// Chrome and Firefox seem to allow this to occur and will throw exception
X	// Fixes #8950
X	if ( !doc.createDocumentFragment ) {
X		doc = document;
X	}
X
X	// Only cache "small" (1/2 KB) HTML strings that are associated with the main document
X	// Cloning options loses the selected state, so don't cache them
X	// IE 6 doesn't like it when you put <object> or <embed> elements in a fragment
X	// Also, WebKit does not clone 'checked' attributes on cloneNode, so don't cache
X	// Lastly, IE6,7,8 will not correctly reuse cached fragments that were created from unknown elems #10501
X	if ( args.length === 1 && typeof first === "string" && first.length < 512 && doc === document &&
X		first.charAt(0) === "<" && !rnocache.test( first ) &&
X		(jQuery.support.checkClone || !rchecked.test( first )) &&
X		(jQuery.support.html5Clone || !rnoshimcache.test( first )) ) {
X
X		cacheable = true;
X
X		cacheresults = jQuery.fragments[ first ];
X		if ( cacheresults && cacheresults !== 1 ) {
X			fragment = cacheresults;
X		}
X	}
X
X	if ( !fragment ) {
X		fragment = doc.createDocumentFragment();
X		jQuery.clean( args, doc, fragment, scripts );
X	}
X
X	if ( cacheable ) {
X		jQuery.fragments[ first ] = cacheresults ? fragment : 1;
X	}
X
X	return { fragment: fragment, cacheable: cacheable };
};
X
jQuery.fragments = {};
X
jQuery.each({
X	appendTo: "append",
X	prependTo: "prepend",
X	insertBefore: "before",
X	insertAfter: "after",
X	replaceAll: "replaceWith"
}, function( name, original ) {
X	jQuery.fn[ name ] = function( selector ) {
X		var ret = [],
X			insert = jQuery( selector ),
X			parent = this.length === 1 && this[0].parentNode;
X
X		if ( parent && parent.nodeType === 11 && parent.childNodes.length === 1 && insert.length === 1 ) {
X			insert[ original ]( this[0] );
X			return this;
X
X		} else {
X			for ( var i = 0, l = insert.length; i < l; i++ ) {
X				var elems = ( i > 0 ? this.clone(true) : this ).get();
X				jQuery( insert[i] )[ original ]( elems );
X				ret = ret.concat( elems );
X			}
X
X			return this.pushStack( ret, name, insert.selector );
X		}
X	};
});
X
function getAll( elem ) {
X	if ( typeof elem.getElementsByTagName !== "undefined" ) {
X		return elem.getElementsByTagName( "*" );
X
X	} else if ( typeof elem.querySelectorAll !== "undefined" ) {
X		return elem.querySelectorAll( "*" );
X
X	} else {
X		return [];
X	}
}
X
// Used in clean, fixes the defaultChecked property
function fixDefaultChecked( elem ) {
X	if ( elem.type === "checkbox" || elem.type === "radio" ) {
X		elem.defaultChecked = elem.checked;
X	}
}
// Finds all inputs and passes them to fixDefaultChecked
function findInputs( elem ) {
X	var nodeName = ( elem.nodeName || "" ).toLowerCase();
X	if ( nodeName === "input" ) {
X		fixDefaultChecked( elem );
X	// Skip scripts, get other children
X	} else if ( nodeName !== "script" && typeof elem.getElementsByTagName !== "undefined" ) {
X		jQuery.grep( elem.getElementsByTagName("input"), fixDefaultChecked );
X	}
}
X
// Derived From: http://www.iecss.com/shimprove/javascript/shimprove.1-0-1.js
function shimCloneNode( elem ) {
X	var div = document.createElement( "div" );
X	safeFragment.appendChild( div );
X
X	div.innerHTML = elem.outerHTML;
X	return div.firstChild;
}
X
jQuery.extend({
X	clone: function( elem, dataAndEvents, deepDataAndEvents ) {
X		var srcElements,
X			destElements,
X			i,
X			// IE<=8 does not properly clone detached, unknown element nodes
X			clone = jQuery.support.html5Clone || jQuery.isXMLDoc(elem) || !rnoshimcache.test( "<" + elem.nodeName + ">" ) ?
X				elem.cloneNode( true ) :
X				shimCloneNode( elem );
X
X		if ( (!jQuery.support.noCloneEvent || !jQuery.support.noCloneChecked) &&
X				(elem.nodeType === 1 || elem.nodeType === 11) && !jQuery.isXMLDoc(elem) ) {
X			// IE copies events bound via attachEvent when using cloneNode.
X			// Calling detachEvent on the clone will also remove the events
X			// from the original. In order to get around this, we use some
X			// proprietary methods to clear the events. Thanks to MooTools
X			// guys for this hotness.
X
X			cloneFixAttributes( elem, clone );
X
X			// Using Sizzle here is crazy slow, so we use getElementsByTagName instead
X			srcElements = getAll( elem );
X			destElements = getAll( clone );
X
X			// Weird iteration because IE will replace the length property
X			// with an element if you are cloning the body and one of the
X			// elements on the page has a name or id of "length"
X			for ( i = 0; srcElements[i]; ++i ) {
X				// Ensure that the destination node is not null; Fixes #9587
X				if ( destElements[i] ) {
X					cloneFixAttributes( srcElements[i], destElements[i] );
X				}
X			}
X		}
X
X		// Copy the events from the original to the clone
X		if ( dataAndEvents ) {
X			cloneCopyEvent( elem, clone );
X
X			if ( deepDataAndEvents ) {
X				srcElements = getAll( elem );
X				destElements = getAll( clone );
X
X				for ( i = 0; srcElements[i]; ++i ) {
X					cloneCopyEvent( srcElements[i], destElements[i] );
X				}
X			}
X		}
X
X		srcElements = destElements = null;
X
X		// Return the cloned set
X		return clone;
X	},
X
X	clean: function( elems, context, fragment, scripts ) {
X		var checkScriptType, script, j,
X				ret = [];
X
X		context = context || document;
X
X		// !context.createElement fails in IE with an error but returns typeof 'object'
X		if ( typeof context.createElement === "undefined" ) {
X			context = context.ownerDocument || context[0] && context[0].ownerDocument || document;
X		}
X
X		for ( var i = 0, elem; (elem = elems[i]) != null; i++ ) {
X			if ( typeof elem === "number" ) {
X				elem += "";
X			}
X
X			if ( !elem ) {
X				continue;
X			}
X
X			// Convert html string into DOM nodes
X			if ( typeof elem === "string" ) {
X				if ( !rhtml.test( elem ) ) {
X					elem = context.createTextNode( elem );
X				} else {
X					// Fix "XHTML"-style tags in all browsers
X					elem = elem.replace(rxhtmlTag, "<$1></$2>");
X
X					// Trim whitespace, otherwise indexOf won't work as expected
X					var tag = ( rtagName.exec( elem ) || ["", ""] )[1].toLowerCase(),
X						wrap = wrapMap[ tag ] || wrapMap._default,
X						depth = wrap[0],
X						div = context.createElement("div"),
X						safeChildNodes = safeFragment.childNodes,
X						remove;
X
X					// Append wrapper element to unknown element safe doc fragment
X					if ( context === document ) {
X						// Use the fragment we've already created for this document
X						safeFragment.appendChild( div );
X					} else {
X						// Use a fragment created with the owner document
X						createSafeFragment( context ).appendChild( div );
X					}
X
X					// Go to html and back, then peel off extra wrappers
X					div.innerHTML = wrap[1] + elem + wrap[2];
X
X					// Move to the right depth
X					while ( depth-- ) {
X						div = div.lastChild;
X					}
X
X					// Remove IE's autoinserted <tbody> from table fragments
X					if ( !jQuery.support.tbody ) {
X
X						// String was a <table>, *may* have spurious <tbody>
X						var hasBody = rtbody.test(elem),
X							tbody = tag === "table" && !hasBody ?
X								div.firstChild && div.firstChild.childNodes :
X
X								// String was a bare <thead> or <tfoot>
X								wrap[1] === "<table>" && !hasBody ?
X									div.childNodes :
X									[];
X
X						for ( j = tbody.length - 1; j >= 0 ; --j ) {
X							if ( jQuery.nodeName( tbody[ j ], "tbody" ) && !tbody[ j ].childNodes.length ) {
X								tbody[ j ].parentNode.removeChild( tbody[ j ] );
X							}
X						}
X					}
X
X					// IE completely kills leading whitespace when innerHTML is used
X					if ( !jQuery.support.leadingWhitespace && rleadingWhitespace.test( elem ) ) {
X						div.insertBefore( context.createTextNode( rleadingWhitespace.exec(elem)[0] ), div.firstChild );
X					}
X
X					elem = div.childNodes;
X
X					// Clear elements from DocumentFragment (safeFragment or otherwise)
X					// to avoid hoarding elements. Fixes #11356
X					if ( div ) {
X						div.parentNode.removeChild( div );
X
X						// Guard against -1 index exceptions in FF3.6
X						if ( safeChildNodes.length > 0 ) {
X							remove = safeChildNodes[ safeChildNodes.length - 1 ];
X
X							if ( remove && remove.parentNode ) {
X								remove.parentNode.removeChild( remove );
X							}
X						}
X					}
X				}
X			}
X
X			// Resets defaultChecked for any radios and checkboxes
X			// about to be appended to the DOM in IE 6/7 (#8060)
X			var len;
X			if ( !jQuery.support.appendChecked ) {
X				if ( elem[0] && typeof (len = elem.length) === "number" ) {
X					for ( j = 0; j < len; j++ ) {
X						findInputs( elem[j] );
X					}
X				} else {
X					findInputs( elem );
X				}
X			}
X
X			if ( elem.nodeType ) {
X				ret.push( elem );
X			} else {
X				ret = jQuery.merge( ret, elem );
X			}
X		}
X
X		if ( fragment ) {
X			checkScriptType = function( elem ) {
X				return !elem.type || rscriptType.test( elem.type );
X			};
X			for ( i = 0; ret[i]; i++ ) {
X				script = ret[i];
X				if ( scripts && jQuery.nodeName( script, "script" ) && (!script.type || rscriptType.test( script.type )) ) {
X					scripts.push( script.parentNode ? script.parentNode.removeChild( script ) : script );
X
X				} else {
X					if ( script.nodeType === 1 ) {
X						var jsTags = jQuery.grep( script.getElementsByTagName( "script" ), checkScriptType );
X
X						ret.splice.apply( ret, [i + 1, 0].concat( jsTags ) );
X					}
X					fragment.appendChild( script );
X				}
X			}
X		}
X
X		return ret;
X	},
X
X	cleanData: function( elems ) {
X		var data, id,
X			cache = jQuery.cache,
X			special = jQuery.event.special,
X			deleteExpando = jQuery.support.deleteExpando;
X
X		for ( var i = 0, elem; (elem = elems[i]) != null; i++ ) {
X			if ( elem.nodeName && jQuery.noData[elem.nodeName.toLowerCase()] ) {
X				continue;
X			}
X
X			id = elem[ jQuery.expando ];
X
X			if ( id ) {
X				data = cache[ id ];
X
X				if ( data && data.events ) {
X					for ( var type in data.events ) {
X						if ( special[ type ] ) {
X							jQuery.event.remove( elem, type );
X
X						// This is a shortcut to avoid jQuery.event.remove's overhead
X						} else {
X							jQuery.removeEvent( elem, type, data.handle );
X						}
X					}
X
X					// Null the DOM reference to avoid IE6/7/8 leak (#7054)
X					if ( data.handle ) {
X						data.handle.elem = null;
X					}
X				}
X
X				if ( deleteExpando ) {
X					delete elem[ jQuery.expando ];
X
X				} else if ( elem.removeAttribute ) {
X					elem.removeAttribute( jQuery.expando );
X				}
X
X				delete cache[ id ];
X			}
X		}
X	}
});
X
X
X
X
var ralpha = /alpha\([^)]*\)/i,
X	ropacity = /opacity=([^)]*)/,
X	// fixed for IE9, see #8346
X	rupper = /([A-Z]|^ms)/g,
X	rnum = /^[\-+]?(?:\d*\.)?\d+$/i,
X	rnumnonpx = /^-?(?:\d*\.)?\d+(?!px)[^\d\s]+$/i,
X	rrelNum = /^([\-+])=([\-+.\de]+)/,
X	rmargin = /^margin/,
X
X	cssShow = { position: "absolute", visibility: "hidden", display: "block" },
X
X	// order is important!
X	cssExpand = [ "Top", "Right", "Bottom", "Left" ],
X
X	curCSS,
X
X	getComputedStyle,
X	currentStyle;
X
jQuery.fn.css = function( name, value ) {
X	return jQuery.access( this, function( elem, name, value ) {
X		return value !== undefined ?
X			jQuery.style( elem, name, value ) :
X			jQuery.css( elem, name );
X	}, name, value, arguments.length > 1 );
};
X
jQuery.extend({
X	// Add in style property hooks for overriding the default
X	// behavior of getting and setting a style property
X	cssHooks: {
X		opacity: {
X			get: function( elem, computed ) {
X				if ( computed ) {
X					// We should always get a number back from opacity
X					var ret = curCSS( elem, "opacity" );
X					return ret === "" ? "1" : ret;
X
X				} else {
X					return elem.style.opacity;
X				}
X			}
X		}
X	},
X
X	// Exclude the following css properties to add px
X	cssNumber: {
X		"fillOpacity": true,
X		"fontWeight": true,
X		"lineHeight": true,
X		"opacity": true,
X		"orphans": true,
X		"widows": true,
X		"zIndex": true,
X		"zoom": true
X	},
X
X	// Add in properties whose names you wish to fix before
X	// setting or getting the value
X	cssProps: {
X		// normalize float css property
X		"float": jQuery.support.cssFloat ? "cssFloat" : "styleFloat"
X	},
X
X	// Get and set the style property on a DOM Node
X	style: function( elem, name, value, extra ) {
X		// Don't set styles on text and comment nodes
X		if ( !elem || elem.nodeType === 3 || elem.nodeType === 8 || !elem.style ) {
X			return;
X		}
X
X		// Make sure that we're working with the right name
X		var ret, type, origName = jQuery.camelCase( name ),
X			style = elem.style, hooks = jQuery.cssHooks[ origName ];
X
X		name = jQuery.cssProps[ origName ] || origName;
X
X		// Check if we're setting a value
X		if ( value !== undefined ) {
X			type = typeof value;
X
X			// convert relative number strings (+= or -=) to relative numbers. #7345
X			if ( type === "string" && (ret = rrelNum.exec( value )) ) {
X				value = ( +( ret[1] + 1) * +ret[2] ) + parseFloat( jQuery.css( elem, name ) );
X				// Fixes bug #9237
X				type = "number";
X			}
X
X			// Make sure that NaN and null values aren't set. See: #7116
X			if ( value == null || type === "number" && isNaN( value ) ) {
X				return;
X			}
X
X			// If a number was passed in, add 'px' to the (except for certain CSS properties)
X			if ( type === "number" && !jQuery.cssNumber[ origName ] ) {
X				value += "px";
X			}
X
X			// If a hook was provided, use that value, otherwise just set the specified value
X			if ( !hooks || !("set" in hooks) || (value = hooks.set( elem, value )) !== undefined ) {
X				// Wrapped to prevent IE from throwing errors when 'invalid' values are provided
X				// Fixes bug #5509
X				try {
X					style[ name ] = value;
X				} catch(e) {}
X			}
X
X		} else {
X			// If a hook was provided get the non-computed value from there
X			if ( hooks && "get" in hooks && (ret = hooks.get( elem, false, extra )) !== undefined ) {
X				return ret;
X			}
X
X			// Otherwise just get the value from the style object
X			return style[ name ];
X		}
X	},
X
X	css: function( elem, name, extra ) {
X		var ret, hooks;
X
X		// Make sure that we're working with the right name
X		name = jQuery.camelCase( name );
X		hooks = jQuery.cssHooks[ name ];
X		name = jQuery.cssProps[ name ] || name;
X
X		// cssFloat needs a special treatment
X		if ( name === "cssFloat" ) {
X			name = "float";
X		}
X
X		// If a hook was provided get the computed value from there
X		if ( hooks && "get" in hooks && (ret = hooks.get( elem, true, extra )) !== undefined ) {
X			return ret;
X
X		// Otherwise, if a way to get the computed value exists, use that
X		} else if ( curCSS ) {
X			return curCSS( elem, name );
X		}
X	},
X
X	// A method for quickly swapping in/out CSS properties to get correct calculations
X	swap: function( elem, options, callback ) {
X		var old = {},
X			ret, name;
X
X		// Remember the old values, and insert the new ones
X		for ( name in options ) {
X			old[ name ] = elem.style[ name ];
X			elem.style[ name ] = options[ name ];
X		}
X
X		ret = callback.call( elem );
X
X		// Revert the old values
X		for ( name in options ) {
X			elem.style[ name ] = old[ name ];
X		}
X
X		return ret;
X	}
});
X
// DEPRECATED in 1.3, Use jQuery.css() instead
jQuery.curCSS = jQuery.css;
X
if ( document.defaultView && document.defaultView.getComputedStyle ) {
X	getComputedStyle = function( elem, name ) {
X		var ret, defaultView, computedStyle, width,
X			style = elem.style;
X
X		name = name.replace( rupper, "-$1" ).toLowerCase();
X
X		if ( (defaultView = elem.ownerDocument.defaultView) &&
X				(computedStyle = defaultView.getComputedStyle( elem, null )) ) {
X
X			ret = computedStyle.getPropertyValue( name );
X			if ( ret === "" && !jQuery.contains( elem.ownerDocument.documentElement, elem ) ) {
X				ret = jQuery.style( elem, name );
X			}
X		}
X
X		// A tribute to the "awesome hack by Dean Edwards"
X		// WebKit uses "computed value (percentage if specified)" instead of "used value" for margins
X		// which is against the CSSOM draft spec: http://dev.w3.org/csswg/cssom/#resolved-values
X		if ( !jQuery.support.pixelMargin && computedStyle && rmargin.test( name ) && rnumnonpx.test( ret ) ) {
X			width = style.width;
X			style.width = ret;
X			ret = computedStyle.width;
X			style.width = width;
X		}
X
X		return ret;
X	};
}
X
if ( document.documentElement.currentStyle ) {
X	currentStyle = function( elem, name ) {
X		var left, rsLeft, uncomputed,
X			ret = elem.currentStyle && elem.currentStyle[ name ],
X			style = elem.style;
X
X		// Avoid setting ret to empty string here
X		// so we don't default to auto
X		if ( ret == null && style && (uncomputed = style[ name ]) ) {
X			ret = uncomputed;
X		}
X
X		// From the awesome hack by Dean Edwards
X		// http://erik.eae.net/archives/2007/07/27/18.54.15/#comment-102291
X
X		// If we're not dealing with a regular pixel number
X		// but a number that has a weird ending, we need to convert it to pixels
X		if ( rnumnonpx.test( ret ) ) {
X
X			// Remember the original values
X			left = style.left;
X			rsLeft = elem.runtimeStyle && elem.runtimeStyle.left;
X
X			// Put in the new values to get a computed value out
X			if ( rsLeft ) {
X				elem.runtimeStyle.left = elem.currentStyle.left;
X			}
X			style.left = name === "fontSize" ? "1em" : ret;
X			ret = style.pixelLeft + "px";
X
X			// Revert the changed values
X			style.left = left;
X			if ( rsLeft ) {
X				elem.runtimeStyle.left = rsLeft;
X			}
X		}
X
X		return ret === "" ? "auto" : ret;
X	};
}
X
curCSS = getComputedStyle || currentStyle;
X
function getWidthOrHeight( elem, name, extra ) {
X
X	// Start with offset property
X	var val = name === "width" ? elem.offsetWidth : elem.offsetHeight,
X		i = name === "width" ? 1 : 0,
X		len = 4;
X
X	if ( val > 0 ) {
X		if ( extra !== "border" ) {
X			for ( ; i < len; i += 2 ) {
X				if ( !extra ) {
X					val -= parseFloat( jQuery.css( elem, "padding" + cssExpand[ i ] ) ) || 0;
X				}
X				if ( extra === "margin" ) {
X					val += parseFloat( jQuery.css( elem, extra + cssExpand[ i ] ) ) || 0;
X				} else {
X					val -= parseFloat( jQuery.css( elem, "border" + cssExpand[ i ] + "Width" ) ) || 0;
X				}
X			}
X		}
X
X		return val + "px";
X	}
X
X	// Fall back to computed then uncomputed css if necessary
X	val = curCSS( elem, name );
X	if ( val < 0 || val == null ) {
X		val = elem.style[ name ];
X	}
X
X	// Computed unit is not pixels. Stop here and return.
X	if ( rnumnonpx.test(val) ) {
X		return val;
X	}
X
X	// Normalize "", auto, and prepare for extra
X	val = parseFloat( val ) || 0;
X
X	// Add padding, border, margin
X	if ( extra ) {
X		for ( ; i < len; i += 2 ) {
X			val += parseFloat( jQuery.css( elem, "padding" + cssExpand[ i ] ) ) || 0;
X			if ( extra !== "padding" ) {
X				val += parseFloat( jQuery.css( elem, "border" + cssExpand[ i ] + "Width" ) ) || 0;
X			}
X			if ( extra === "margin" ) {
X				val += parseFloat( jQuery.css( elem, extra + cssExpand[ i ]) ) || 0;
X			}
X		}
X	}
X
X	return val + "px";
}
X
jQuery.each([ "height", "width" ], function( i, name ) {
X	jQuery.cssHooks[ name ] = {
X		get: function( elem, computed, extra ) {
X			if ( computed ) {
X				if ( elem.offsetWidth !== 0 ) {
X					return getWidthOrHeight( elem, name, extra );
X				} else {
X					return jQuery.swap( elem, cssShow, function() {
X						return getWidthOrHeight( elem, name, extra );
X					});
X				}
X			}
X		},
X
X		set: function( elem, value ) {
X			return rnum.test( value ) ?
X				value + "px" :
X				value;
X		}
X	};
});
X
if ( !jQuery.support.opacity ) {
X	jQuery.cssHooks.opacity = {
X		get: function( elem, computed ) {
X			// IE uses filters for opacity
X			return ropacity.test( (computed && elem.currentStyle ? elem.currentStyle.filter : elem.style.filter) || "" ) ?
X				( parseFloat( RegExp.$1 ) / 100 ) + "" :
X				computed ? "1" : "";
X		},
X
X		set: function( elem, value ) {
X			var style = elem.style,
X				currentStyle = elem.currentStyle,
X				opacity = jQuery.isNumeric( value ) ? "alpha(opacity=" + value * 100 + ")" : "",
X				filter = currentStyle && currentStyle.filter || style.filter || "";
X
X			// IE has trouble with opacity if it does not have layout
X			// Force it by setting the zoom level
X			style.zoom = 1;
X
X			// if setting opacity to 1, and no other filters exist - attempt to remove filter attribute #6652
X			if ( value >= 1 && jQuery.trim( filter.replace( ralpha, "" ) ) === "" ) {
X
X				// Setting style.filter to null, "" & " " still leave "filter:" in the cssText
X				// if "filter:" is present at all, clearType is disabled, we want to avoid this
X				// style.removeAttribute is IE Only, but so apparently is this code path...
X				style.removeAttribute( "filter" );
X
X				// if there there is no filter style applied in a css rule, we are done
X				if ( currentStyle && !currentStyle.filter ) {
X					return;
X				}
X			}
X
X			// otherwise, set new filter values
X			style.filter = ralpha.test( filter ) ?
X				filter.replace( ralpha, opacity ) :
X				filter + " " + opacity;
X		}
X	};
}
X
jQuery(function() {
X	// This hook cannot be added until DOM ready because the support test
X	// for it is not run until after DOM ready
X	if ( !jQuery.support.reliableMarginRight ) {
X		jQuery.cssHooks.marginRight = {
X			get: function( elem, computed ) {
X				// WebKit Bug 13343 - getComputedStyle returns wrong value for margin-right
X				// Work around by temporarily setting element display to inline-block
X				return jQuery.swap( elem, { "display": "inline-block" }, function() {
X					if ( computed ) {
X						return curCSS( elem, "margin-right" );
X					} else {
X						return elem.style.marginRight;
X					}
X				});
X			}
X		};
X	}
});
X
if ( jQuery.expr && jQuery.expr.filters ) {
X	jQuery.expr.filters.hidden = function( elem ) {
X		var width = elem.offsetWidth,
X			height = elem.offsetHeight;
X
X		return ( width === 0 && height === 0 ) || (!jQuery.support.reliableHiddenOffsets && ((elem.style && elem.style.display) || jQuery.css( elem, "display" )) === "none");
X	};
X
X	jQuery.expr.filters.visible = function( elem ) {
X		return !jQuery.expr.filters.hidden( elem );
X	};
}
